
(:TYPE :SYSTEM :NAME "weblocks-scripts" :AUTHOR "Slava Akhmechet" :MAINTAINER
 "Olexiy Zamkoviy, Scott L. Burson" :VERSION "0.0.1" :LICENCE "LLGPL"
 :DESCRIPTION "A set of scripts for weblocks framework." :LONG-DESCRIPTION NIL
 :DEPENDS-ON ("cl-fad" "cl-ppcre") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "WEBLOCKS-SCRIPTS" :FULL-NAME "WEBLOCKS-SCRIPTS"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "COMPUTE-DOCUMENTATION-PATH" :PACKAGE-NAME "WEBLOCKS-SCRIPTS"
      :EXTERNALP NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION "Computes the directory where generated documentation should
reside.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "DOCUMENT-PACKAGE" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "SYSTEM" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL)
       (:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      ((:NAME "IGNORE-ERRORS-P" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP
        NIL)
       (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "DOCUMENT-PACKAGE/TINAA" :PACKAGE-NAME "WEBLOCKS-SCRIPTS"
      :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "DOCUMENT-WEBLOCKS" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*APP-NAME-PLACEHOLDER*" :PACKAGE-NAME "WEBLOCKS-SCRIPTS"
      :EXTERNALP NIL)
     :DOCUMENTATION "A placeholder located in template files." :INITIAL-VALUE
     "(CL-PPCRE:QUOTE-META-CHARS \"{APPNAME}\")")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ATTRIBUTIZE-NAME" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL))
     :DOCUMENTATION "Convert a string or a symbol to a format suitable for
serialization (in particular for markup languages like HTML).

Ex:
(attributize-name 'hello-world) => \"hello-world\"
(attributize-name \"Hello world-ref\") => \"hello-world-ref\"")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-DIR" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "PATHNAME" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Create directory 'pathname'.

Borrowed from cl-darcs with permission of copyright owner.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "COPY-DIRECTORY" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "SOURCE" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL)
      (:NAME "TARGET" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "EXCLUDING" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL))
     :DOCUMENTATION "Copy all files and directories in 'source' to 'target'.
'source' and 'target' are pathnames designating directories, both of
which must exist.  'Excluding' is a list of files and directories
to exclude.

Symlinks will confuse the function.

Borrowed from cl-darcs with permission of copyright owner.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ASDF-SYSTEM-DIRECTORY" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     ((:NAME "SYSTEM-NAME" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL))
     :DOCUMENTATION "Returns a directory of the asdf system file of system
'system-name'.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "COPY-FILE-REPLACE" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     ((:NAME "SOURCE" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL)
      (:NAME "TARGET" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "MATCH" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL)
      (:NAME "REPLACEMENT" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL))
     :DOCUMENTATION
     "Copies 'source' to 'target' replacing all instances of 'match' with
'replacement' in the target.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-APPLICATION" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "TARGET" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP NIL))
     :DOCUMENTATION
     "Creates a directory 'name' under directory 'target' and fills it
with files that allow to easily get started with a new Weblocks
application.

If 'target' isn't specified, 'make-applicaiton' uses
*default-pathname-defaults*.

'name' cannot be NIL, as it's being used in numerous places in the
generated files to specify the name of the newly created
application. Note, 'name' is expected to be a symbol naming the
application."))
   :EXTERNAL-SYMBOLS
   ((:NAME "DOCUMENT-WEBLOCKS" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP T)
    (:NAME "MAKE-APPLICATION" :PACKAGE-NAME "WEBLOCKS-SCRIPTS" :EXTERNALP T)))))
