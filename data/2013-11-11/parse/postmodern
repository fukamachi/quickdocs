    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*RESULT-STYLES*" :PACKAGE-NAME "POSTMODERN" :EXTERNALP NIL)
     :DOCUMENTATION
     "Mapping from keywords identifying result styles to the row-reader
that should be used and whether all values or only one value should be
returned."
     :INITIAL-VALUE
     "'((:NONE CL-POSTGRES:IGNORE-ROW-READER POSTMODERN::ALL-ROWS)
  (:LISTS CL-POSTGRES:LIST-ROW-READER POSTMODERN::ALL-ROWS)
  (:LIST CL-POSTGRES:LIST-ROW-READER POSTMODERN::SINGLE-ROW)
  (:ROWS CL-POSTGRES:LIST-ROW-READER POSTMODERN::ALL-ROWS)
  (:ROW CL-POSTGRES:LIST-ROW-READER POSTMODERN::SINGLE-ROW)
  (:ALISTS POSTMODERN::SYMBOL-ALIST-ROW-READER POSTMODERN::ALL-ROWS)
  (:ALIST POSTMODERN::SYMBOL-ALIST-ROW-READER POSTMODERN::SINGLE-ROW)
  (:STR-ALISTS CL-POSTGRES:ALIST-ROW-READER POSTMODERN::ALL-ROWS)
  (:STR-ALIST CL-POSTGRES:ALIST-ROW-READER POSTMODERN::SINGLE-ROW)
  (:PLISTS POSTMODERN::SYMBOL-PLIST-ROW-READER POSTMODERN::ALL-ROWS)
  (:PLIST POSTMODERN::SYMBOL-PLIST-ROW-READER POSTMODERN::SINGLE-ROW)
  (:COLUMN POSTMODERN::COLUMN-ROW-READER POSTMODERN::ALL-ROWS)
  (:SINGLE POSTMODERN::COLUMN-ROW-READER POSTMODERN::SINGLE-ROW)
  (:SINGLE! POSTMODERN::COLUMN-ROW-READER POSTMODERN::SINGLE-ROW!))")
