
(:TYPE :SYSTEM :NAME "uuid" :AUTHOR "Boian Tzonev" :MAINTAINER "Boian Tzonev"
 :VERSION "2012.12.26" :LICENCE "LLGPL" :DESCRIPTION "UUID Generation"
 :LONG-DESCRIPTION "Lisp implementation of rfc 4122" :DEPENDS-ON
 ("ironclad" "trivial-utf-8") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "UUID" :FULL-NAME "UUID" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*CLOCK-SEQ*" :PACKAGE-NAME "UUID" :EXTERNALP NIL) :DOCUMENTATION
     "Holds the clock sequence. It is set when a version 1 uuid is
generated for the first time and remains unchanged during a whole
session."
     :INITIAL-VALUE "0")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*NODE*" :PACKAGE-NAME "UUID" :EXTERNALP NIL) :DOCUMENTATION
     "Holds the IEEE 802 MAC address or a random number when such is not
available"
     :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*TICKS-PER-COUNT*" :PACKAGE-NAME "UUID" :EXTERNALP T)
     :DOCUMENTATION
     "Holds the amount of ticks per count. The ticks per count determine
the number of possible version 1 uuids created for one time
interval. Common Lisp provides INTERNAL-TIME-UNITS-PER-SECOND which
gives the ticks per count for the current system so *ticks-per-count*
can be set to INTERNAL-TIME-UNITS-PER-SECOND"
     :INITIAL-VALUE "1024")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*UUID-RANDOM-STATE*" :PACKAGE-NAME "UUID" :EXTERNALP NIL)
     :DOCUMENTATION
     "Holds the random state used for generation of random numbers"
     :INITIAL-VALUE "NIL")
    (:TYPE :CLASS :SYMBOL (:NAME "UUID" :PACKAGE-NAME "UUID" :EXTERNALP T)
     :SUPER-CLASS-LIST NIL :DOCUMENTATION "Represents an uuid" :SLOT-LIST
     ((:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "TIME-LOW" :PACKAGE-NAME "UUID" :EXTERNALP NIL) :ACCESSORS
       ((:NAME "TIME-LOW" :PACKAGE-NAME "UUID" :EXTERNALP NIL)) :READERS NIL
       :WRITERS NIL :DOCUMENTATION NIL)
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "TIME-MID" :PACKAGE-NAME "UUID" :EXTERNALP NIL) :ACCESSORS
       ((:NAME "TIME-MID" :PACKAGE-NAME "UUID" :EXTERNALP NIL)) :READERS NIL
       :WRITERS NIL :DOCUMENTATION NIL)
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "TIME-HIGH-AND-VERSION" :PACKAGE-NAME "UUID" :EXTERNALP NIL)
       :ACCESSORS ((:NAME "TIME-HIGH" :PACKAGE-NAME "UUID" :EXTERNALP NIL))
       :READERS NIL :WRITERS NIL :DOCUMENTATION NIL)
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "CLOCK-SEQ-AND-RESERVED" :PACKAGE-NAME "UUID" :EXTERNALP NIL)
       :ACCESSORS ((:NAME "CLOCK-SEQ-VAR" :PACKAGE-NAME "UUID" :EXTERNALP NIL))
       :READERS NIL :WRITERS NIL :DOCUMENTATION NIL)
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "CLOCK-SEQ-LOW" :PACKAGE-NAME "UUID" :EXTERNALP NIL) :ACCESSORS
       ((:NAME "CLOCK-SEQ-LOW" :PACKAGE-NAME "UUID" :EXTERNALP NIL)) :READERS
       NIL :WRITERS NIL :DOCUMENTATION NIL)
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "NODE" :PACKAGE-NAME "UUID" :EXTERNALP NIL) :ACCESSORS
       ((:NAME "NODE" :PACKAGE-NAME "UUID" :EXTERNALP NIL)) :READERS NIL
       :WRITERS NIL :DOCUMENTATION NIL)))
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-UUID-FROM-STRING" :PACKAGE-NAME "UUID" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION
     "Creates an uuid from the string represenation of an uuid. (example input string 6ba7b810-9dad-11d1-80b4-00c04fd430c8)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+NAMESPACE-DNS+" :PACKAGE-NAME "UUID" :EXTERNALP T) :DOCUMENTATION
     "The DNS Namespace. Can be used for the generation of uuids version 3 and 5"
     :INITIAL-VALUE
     "(UUID:MAKE-UUID-FROM-STRING \"6ba7b810-9dad-11d1-80b4-00c04fd430c8\")")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+NAMESPACE-URL+" :PACKAGE-NAME "UUID" :EXTERNALP T) :DOCUMENTATION
     "The URL Namespace. Can be used for the generation of uuids version 3 and 5"
     :INITIAL-VALUE
     "(UUID:MAKE-UUID-FROM-STRING \"6ba7b811-9dad-11d1-80b4-00c04fd430c8\")")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+NAMESPACE-OID+" :PACKAGE-NAME "UUID" :EXTERNALP T) :DOCUMENTATION
     "The OID Namespace. Can be used for the generation of uuids version 3 and 5"
     :INITIAL-VALUE
     "(UUID:MAKE-UUID-FROM-STRING \"6ba7b812-9dad-11d1-80b4-00c04fd430c8\")")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+NAMESPACE-X500+" :PACKAGE-NAME "UUID" :EXTERNALP T)
     :DOCUMENTATION
     "The x500+ Namespace. Can be used for the generation of uuids version 3 and 5"
     :INITIAL-VALUE
     "(UUID:MAKE-UUID-FROM-STRING \"6ba7b814-9dad-11d1-80b4-00c04fd430c8\")")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "GET-NODE-ID" :PACKAGE-NAME "UUID" :EXTERNALP NIL) :LAMBDA-LIST
     (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :DOCUMENTATION
     "Get MAC address of first ethernet device")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "GET-TIMESTAMP" :PACKAGE-NAME "UUID" :EXTERNALP NIL) :LAMBDA-LIST
     (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :DOCUMENTATION
     "Get timestamp, compensate nanoseconds intervals")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "GET-TIMESTAMP" :PACKAGE-NAME "UUID" :EXTERNALP NIL) :LAMBDA-LIST
     (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :DOCUMENTATION
     "Get timestamp, compensate nanoseconds intervals")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FORMAT-V3OR5-UUID" :PACKAGE-NAME "UUID" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "HASH" :PACKAGE-NAME "UUID" :EXTERNALP NIL)
      (:NAME "VER" :PACKAGE-NAME "UUID" :EXTERNALP NIL))
     :DOCUMENTATION
     "Helper function to format a version 3 or 5 uuid. Formatting means setting the appropriate version bytes.")
    (:TYPE :METHOD :SYMBOL
     (:NAME "PRINT-OBJECT" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "ID" :PACKAGE-NAME "UUID" :EXTERNALP NIL)
       (:NAME "UUID" :PACKAGE-NAME "UUID" :EXTERNALP T))
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION
     "Prints an uuid in the string represenation of an uuid. (example string 6ba7b810-9dad-11d1-80b4-00c04fd430c8)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PRINT-BYTES" :PACKAGE-NAME "UUID" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "UUID" :PACKAGE-NAME "UUID" :EXTERNALP T))
     :DOCUMENTATION
     "Prints the raw bytes in hex form. (example output 6ba7b8109dad11d180b400c04fd430c8)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FORMAT-AS-URN" :PACKAGE-NAME "UUID" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "UUID" :PACKAGE-NAME "UUID" :EXTERNALP T))
     :DOCUMENTATION "Prints the uuid as a urn")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-NULL-UUID" :PACKAGE-NAME "UUID" :EXTERNALP T) :LAMBDA-LIST
     (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :DOCUMENTATION
     "Generates a NULL uuid (i.e 00000000-0000-0000-0000-000000000000)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-V1-UUID" :PACKAGE-NAME "UUID" :EXTERNALP T) :LAMBDA-LIST
     (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :DOCUMENTATION
     "Generates a version 1 (time-based) uuid.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-V3-UUID" :PACKAGE-NAME "UUID" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "NAMESPACE" :PACKAGE-NAME "UUID" :EXTERNALP NIL)
      (:NAME "NAME" :PACKAGE-NAME "UUID" :EXTERNALP NIL))
     :DOCUMENTATION "Generates a version 3 (named based MD5) uuid.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-V4-UUID" :PACKAGE-NAME "UUID" :EXTERNALP T) :LAMBDA-LIST
     (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :DOCUMENTATION
     "Generates a version 4 (random) uuid")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-V5-UUID" :PACKAGE-NAME "UUID" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "NAMESPACE" :PACKAGE-NAME "UUID" :EXTERNALP NIL)
      (:NAME "NAME" :PACKAGE-NAME "UUID" :EXTERNALP NIL))
     :DOCUMENTATION "Generates a version 5 (name based SHA1) uuid.")
    (:TYPE :FUNCTION :SYMBOL (:NAME "UUID=" :PACKAGE-NAME "UUID" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "UUID1" :PACKAGE-NAME "UUID" :EXTERNALP NIL)
      (:NAME "UUID2" :PACKAGE-NAME "UUID" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "UUID-TO-BYTE-ARRAY" :PACKAGE-NAME "UUID" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "UUID" :PACKAGE-NAME "UUID" :EXTERNALP T))
     :DOCUMENTATION "Converts an uuid to byte-array")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ARR-TO-BYTES" :PACKAGE-NAME "UUID" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "FROM" :PACKAGE-NAME "UUID" :EXTERNALP NIL)
      (:NAME "TO" :PACKAGE-NAME "UUID" :EXTERNALP NIL)
      (:NAME "ARRAY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Helper macro used in byte-array-to-uuid.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "BYTE-ARRAY-TO-UUID" :PACKAGE-NAME "UUID" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "ARRAY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION
     "Converts a byte-array generated with uuid-to-byte-array to an uuid.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "DIGEST-UUID" :PACKAGE-NAME "UUID" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "DIGEST" :PACKAGE-NAME "UUID" :EXTERNALP NIL)
