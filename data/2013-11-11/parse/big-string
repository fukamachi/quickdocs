
(:TYPE :SYSTEM :NAME "big-string" :AUTHOR "Robert Smith <quad@symbo1ics.com>"
 :MAINTAINER NIL :VERSION NIL :LICENCE "BSD 3-clause (see LICENSE)"
 :DESCRIPTION "Big strings, similar to Java's StringBuilder." :LONG-DESCRIPTION
 NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "BIG-STRING" :FULL-NAME "BIG-STRING" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :TYPE :SYMBOL
     (:NAME "ARRAY-SIZE" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :TYPE :SYMBOL
     (:NAME "BIG-STRING-SIZE" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :STRUCT :SYMBOL
     (:NAME "BIG-STRING" :PACKAGE-NAME "BIG-STRING" :EXTERNALP T)
     :SUPER-CLASS-LIST
     ((:NAME "STRUCTURE-OBJECT" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL :SLOT-LIST
     ((:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "TRACKED-LENGTH" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL)
       :ACCESSORS NIL :READERS NIL :WRITERS NIL :DOCUMENTATION NIL)
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "CUMULATIVE-LENGTHS" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL)
       :ACCESSORS NIL :READERS NIL :WRITERS NIL :DOCUMENTATION NIL)
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "STRINGS" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL) :ACCESSORS
       NIL :READERS NIL :WRITERS NIL :DOCUMENTATION NIL)))
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-BIG-STRING" :PACKAGE-NAME "BIG-STRING" :EXTERNALP T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "PRINT-OBJECT" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "OBJECT30" :PACKAGE-NAME NIL :EXTERNALP NIL)
       (:NAME "BIG-STRING" :PACKAGE-NAME "BIG-STRING" :EXTERNALP T))
      (:NAME "STREAM31" :PACKAGE-NAME NIL :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "BIG-STRING-LENGTH" :PACKAGE-NAME "BIG-STRING" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "BS" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL))
     :DOCUMENTATION "Return the total length of the big string BS.

Time complexity: O(1)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "BIG-STRING-APPEND" :PACKAGE-NAME "BIG-STRING" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "BS" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL)
      (:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION
     "Destructively append the string STRING to the big string BS.

Time complexity: O(1)   [amortized]")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "BINARY-SEARCH" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "N" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL)
      (:NAME "ARRAY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION
     "Find the least value in the array ARRAY greater than N. Return NIL
if N is negative or no such number is found.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "BIG-STRING-CHAR" :PACKAGE-NAME "BIG-STRING" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "BS" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL)
      (:NAME "N" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL))
     :DOCUMENTATION "Find the Nth char in the big string BS.

Time complexity: O(log n)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "BIG-STRING-SUBSTRING" :PACKAGE-NAME "BIG-STRING" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "BS" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL)
      (:NAME "START" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "END" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL)
       ((:NAME "BIG-STRING-LENGTH" :PACKAGE-NAME "BIG-STRING" :EXTERNALP T)
        (:NAME "BS" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL))))
     :DOCUMENTATION
     "Compute the substring from the big string BS from the index START
 to before the index END.

Time complexity: with n = end - start, O(n log n)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "STRING-OF-BIG-STRING" :PACKAGE-NAME "BIG-STRING" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "BS" :PACKAGE-NAME "BIG-STRING" :EXTERNALP NIL))
     :DOCUMENTATION "Build a string from the big string BS.

Time complexity: O(size(bs))"))
   :EXTERNAL-SYMBOLS
   ((:NAME "BIG-STRING-CHAR" :PACKAGE-NAME "BIG-STRING" :EXTERNALP T)
