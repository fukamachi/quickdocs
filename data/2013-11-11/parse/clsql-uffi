
(:TYPE :SYSTEM :NAME "clsql-uffi" :AUTHOR "Kevin M. Rosenberg <kmr@debian.org>"
 :MAINTAINER "Kevin M. Rosenberg <kmr@debian.org>" :VERSION NIL :LICENCE
 "Lessor Lisp General Public License" :DESCRIPTION
 "Common UFFI Helper functions for Common Lisp SQL Interface Library"
 :LONG-DESCRIPTION
 "cl-sql-uffi package provides common helper functions using the UFFI for the CLSQL package."
 :DEPENDS-ON ("CLSQL" "(VERSION UFFI 2.0)") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CLSQL-UFFI" :FULL-NAME "CLSQL-UFFI" :DOCUMENTATION
   "Common functions for interfaces using UFFI" :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "FIND-AND-LOAD-FOREIGN-LIBRARY" :PACKAGE-NAME "CLSQL-UFFI"
      :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "FILENAMES" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "MODULE" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL)
      (:NAME "SUPPORTING-LIBRARIES" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL)
      ((:NAME "ERRORP" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL)
       (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION
     "Attempt to load a foreign library. This will search for any of the filenames, as
well as any of the filenames in any of the clsql:*foreign-library-search-paths*")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*CLSQL-UFFI-LIBRARY-FILENAMES*" :PACKAGE-NAME "CLSQL-UFFI"
      :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "`(,@(WHEN (> MOST-POSITIVE-FIXNUM (EXPT 2 32)) (LIST \"clsql_uffi64\"))
  \"clsql_uffi\")")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*CLSQL-UFFI-SUPPORTING-LIBRARIES*" :PACKAGE-NAME "CLSQL-UFFI"
      :EXTERNALP NIL)
     :DOCUMENTATION
     "Used only by CMU. List of library flags needed to be passed to ld to
load the MySQL client library succesfully.  If this differs at your site,
set to the right path before compiling or loading the system."
     :INITIAL-VALUE "'(\"c\")")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*UFFI-LIBRARY-LOADED*" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL)
     :DOCUMENTATION "T if foreign library was able to be loaded successfully"
     :INITIAL-VALUE "NIL")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "LOAD-UFFI-FOREIGN-LIBRARY" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP
      NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CANONICALIZE-TYPE-LIST" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "TYPES" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL)
      (:NAME "AUTO-LIST" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL))
     :DOCUMENTATION "Ensure a field type list meets expectations")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ATOI" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "STR" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL)) :DOCUMENTATION
     NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "C-STRTOUL" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "STR" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL)
      (:NAME "ENDPTR" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL)
