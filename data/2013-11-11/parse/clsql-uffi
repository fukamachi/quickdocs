     NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ATOL64" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "STR" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL)
      (:NAME "HIGH32" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+2^32+" :PACKAGE-NAME "CLSQL-UFFI" :EXTERNALP NIL) :DOCUMENTATION
