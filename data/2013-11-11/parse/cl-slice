     (:NAME "TRAVERSE-REPRESENTATIONS" :PACKAGE-NAME "CL-SLICE-DEV" :EXTERNALP
      T)
     :LAMBDA-LIST
     (((:NAME "SUBSCRIPTS" :PACKAGE-NAME "CL-SLICE-DEV" :EXTERNALP NIL)
       (:NAME "REPRESENTATIONS" :PACKAGE-NAME "CL-SLICE-DEV" :EXTERNALP NIL)
       (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
