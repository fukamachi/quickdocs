
(:TYPE :SYSTEM :NAME "spellcheck" :AUTHOR "Mikael Jansson <mikael@lisp.se>"
 :MAINTAINER NIL :VERSION "0.0.1" :LICENCE NIL :DESCRIPTION
 "Peter Norvig's spell corrector." :LONG-DESCRIPTION NIL :DEPENDS-ON
 ("ALEXANDRIA" "CL-PPCRE") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "SPELLCHECK" :FULL-NAME "SPELLCHECK" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*ALPHABET*" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "\"abcdefghijklmnopqrstuvwxyz\"")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*FREQ*" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T) :DOCUMENTATION
     NIL :INITIAL-VALUE "(MAKE-HASH-TABLE :TEST #'EQUAL)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "RESET" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T) :LAMBDA-LIST
     (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :DOCUMENTATION
     "Resets the default frequency distribution to empty.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WORDS" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)) :DOCUMENTATION
     "Tokenizes string into a list of words.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TRAIN" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "WORDS" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FREQUENCY" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
       (:NAME "*FREQ*" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)))
     :DOCUMENTATION
     "Adds the given list of words to the frequency distribution.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TRAIN-WITH-FILE" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FILENAME" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
       ((:NAME "MERGE-PATHNAMES" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        ((:NAME "SYSTEM-SOURCE-DIRECTORY" :PACKAGE-NAME "ASDF/SYSTEM"
          :EXTERNALP T)
         (:NAME "SPELLCHECK" :PACKAGE-NAME "KEYWORD" :EXTERNALP T))
        "big.txt"))
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FREQUENCY" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
       (:NAME "*FREQ*" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)))
     :DOCUMENTATION
     "Adds the list of words from the given file to the frequency distribution.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "INITIALIZE" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T) :LAMBDA-LIST
     (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :DOCUMENTATION
     "Initialize with default settings.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "EDITS-1" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "WORD" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)) :DOCUMENTATION
     "Returns a list of one character edits of word.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "KNOWN-EDITS-2" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "WORD" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FREQUENCY" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
       (:NAME "*FREQ*" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)))
     :DOCUMENTATION "Returns the list of two character edits of word.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "KNOWN" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "WORDS" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FREQUENCY" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
       (:NAME "*FREQ*" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)))
     :DOCUMENTATION "Looks up the list of words to see if they are known.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "KNOWN-WORD-P" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "WORD" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FREQUENCY" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
       (:NAME "*FREQ*" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)))
     :DOCUMENTATION "Returns true if word is known.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CORRECT" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "WORD" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FREQUENCY" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP NIL)
       (:NAME "*FREQ*" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)))
     :DOCUMENTATION
     "Returns the most likely spelling correction of word, or word itself."))
   :EXTERNAL-SYMBOLS
   ((:NAME "CORRECT" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)
    (:NAME "TRAIN-WITH-FILE" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)
    (:NAME "RESET" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)
    (:NAME "TRAIN" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)
    (:NAME "WORDS" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)
    (:NAME "*FREQ*" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)
    (:NAME "INITIALIZE" :PACKAGE-NAME "SPELLCHECK" :EXTERNALP T)))))
