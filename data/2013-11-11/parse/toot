    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*MIME-TYPE-HASH*" :PACKAGE-NAME "TOOT" :EXTERNALP NIL)
     :DOCUMENTATION "A hash table which maps file suffixes to MIME types."
     :INITIAL-VALUE "(LET ((TOOT::HASH (MAKE-HASH-TABLE :TEST #'EQUALP)))
  (LOOP TOOT::FOR (TYPE . TOOT::SUFFIXES) TOOT::IN TOOT::*MIME-TYPE-LIST*
        DO (LOOP TOOT::FOR TOOT::SUFFIX TOOT::IN TOOT::SUFFIXES
                 DO (SETF (GETHASH TOOT::SUFFIX TOOT::HASH) TYPE)))
  TOOT::HASH)")
