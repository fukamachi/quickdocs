
(:TYPE :SYSTEM :NAME "toot" :AUTHOR NIL :MAINTAINER NIL :VERSION "0.0.1"
 :LICENCE NIL :DESCRIPTION
 "A minimal web server originally built by stripping down Edi Weitz's Hunchentoot"
 :LONG-DESCRIPTION NIL :DEPENDS-ON
 ("ALEXANDRIA" "CHUNGA" "CL-BASE64" "CL-FAD" "CL-PPCRE" "FLEXI-STREAMS"
  "CL+SSL" "MD5" "TRIVIAL-BACKTRACE" "USOCKET" "BORDEAUX-THREADS" "PURI")
 :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "TOOT" :FULL-NAME "TOOT" :DOCUMENTATION
   "A very simple web server." :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "LWSP-CHAR-P" :PACKAGE-NAME "TOOT" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "CHAR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)) :DOCUMENTATION
     "Returns true if CHAR is a linear-whitespace-char (LWSP-char).  Either
   space or tab, in short.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-UNTIL-NEXT-BOUNDARY" :PACKAGE-NAME "TOOT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BOUNDARY" :PACKAGE-NAME "TOOT" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "DISCARD" :PACKAGE-NAME "TOOT" :EXTERNALP NIL)
      (:NAME "OUT-STREAM" :PACKAGE-NAME "TOOT" :EXTERNALP NIL))
     :DOCUMENTATION
     "Reads from STREAM up to the next boundary.  Returns two values: read
   data (nil if DISCARD is true), and true if the boundary is not last
   (i.e., there's more data).")
    (:TYPE :STRUCT :SYMBOL (:NAME "HEADER" :PACKAGE-NAME "TOOT" :EXTERNALP NIL)
     :SUPER-CLASS-LIST NIL :DOCUMENTATION NIL :SLOT-LIST NIL)
