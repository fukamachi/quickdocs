    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*HTTP-REASON-PHRASE-MAP*" :PACKAGE-NAME "TOOT" :EXTERNALP NIL)
     :DOCUMENTATION "Used to map numerical return codes to reason phrases."
     :INITIAL-VALUE "(MAKE-HASH-TABLE)")
    (:TYPE :MACRO :SYMBOL
     (:NAME "DEF-HTTP-STATUS-CODE" :PACKAGE-NAME "TOOT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "TOOT" :EXTERNALP T)
      (:NAME "VALUE" :PACKAGE-NAME "TOOT" :EXTERNALP NIL)
      (:NAME "REASON-PHRASE" :PACKAGE-NAME "TOOT" :EXTERNALP T))
     :DOCUMENTATION "Shortcut to define constants for return codes.  NAME is a
Lisp symbol, VALUE is the numerical value of the return code, and
REASON-PHRASE is the phrase (a string) to be shown in the
server's status line.")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+CRLF+" :PACKAGE-NAME "TOOT" :EXTERNALP NIL) :DOCUMENTATION
     "A 2-element array consisting of the character codes for a CRLF
sequence."
     :INITIAL-VALUE "(IF (BOUNDP 'TOOT::+CRLF+)
    (SYMBOL-VALUE 'TOOT::+CRLF+)
    (MAKE-ARRAY 2 :ELEMENT-TYPE '(UNSIGNED-BYTE 8) :INITIAL-CONTENTS
                (MAPCAR 'CHAR-CODE '(#\\Return #\\Newline))))")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+HTTP-CONTINUE+" :PACKAGE-NAME "TOOT" :EXTERNALP T) :DOCUMENTATION
     "HTTP return code (100) for 'Continue'." :INITIAL-VALUE
     "(IF (BOUNDP 'TOOT:+HTTP-CONTINUE+)
    (SYMBOL-VALUE 'TOOT:+HTTP-CONTINUE+)
    100)")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+HTTP-SWITCHING-PROTOCOLS+" :PACKAGE-NAME "TOOT" :EXTERNALP T)
     :DOCUMENTATION "HTTP return code (101) for 'Switching Protocols'."
