    (:TYPE :MACRO :SYMBOL
     (:NAME "WITH-TRACK-BEST" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       ((:NAME "NAME" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
        (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
       ((:NAME "KEEP" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL) 1)
       ((:NAME "ORDER-BY-FN" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
        ((:NAME "FUNCTION" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
         (:NAME ">" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
       ((:NAME "ALWAYS-RETURN-LIST" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
        (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
       ((:NAME "RETURN-BEST" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
        (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL))
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
