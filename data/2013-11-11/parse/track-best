       (:NAME "BEST" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL) :ACCESSORS NIL
       :READERS NIL :WRITERS NIL :DOCUMENTATION NIL)
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "KEEP" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL) :ACCESSORS NIL
       :READERS NIL :WRITERS NIL :DOCUMENTATION NIL)
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "ORDER-BY-FN" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
       :ACCESSORS NIL :READERS NIL :WRITERS NIL :DOCUMENTATION NIL)))
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-BEST-TRACKER" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (((:NAME "BEST" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "DUM48" :PACKAGE-NAME NIL :EXTERNALP NIL))
       ((:NAME "MAKE-ARRAY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) 0
        (:NAME "ELEMENT-TYPE" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        ((:NAME "QUOTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
         (:NAME "BEST-ITEM" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL))
        (:NAME "ADJUSTABLE" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
      (((:NAME "KEEP" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "DUM49" :PACKAGE-NAME NIL :EXTERNALP NIL))
       1)
      (((:NAME "ORDER-BY-FN" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "DUM50" :PACKAGE-NAME NIL :EXTERNALP NIL))
       ((:NAME "FUNCTION" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        (:NAME ">" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))))
     :DOCUMENTATION NIL)
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*CURRENT-BEST-TRACKER*" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
     :DOCUMENTATION "The current best-tracker instance." :INITIAL-VALUE "NIL")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "INSERT-TRACKED-ITEM" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "TRACKER" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
      (:NAME "ITEM" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "ALWAYS" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL))
     :DOCUMENTATION
     "Insert the ITEM into the TRACKER.  If ALWAYS is true, then do it even though it is not bigger than the last thing in the array.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TRACK" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "ITEM" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
      (:NAME "SCORE" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "TRACKER" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
       (:NAME "*CURRENT-BEST-TRACKER*" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP
        NIL)))
     :DOCUMENTATION "Add the ITEM with SCORE to the TRACKER.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAP-BEST" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "FN" :PACKAGE-NAME "TRACK-BEST" :EXTERNALP NIL)
