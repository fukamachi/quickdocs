        (:NAME "CONSTANT" :PACKAGE-NAME "KEYWORD" :EXTERNALP T))))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "DOCUMENT" :PACKAGE-NAME "CLOD" :EXTERNALP NIL) :LAMBDA-LIST
     (((:NAME "SYM" :PACKAGE-NAME "CLOD" :EXTERNALP NIL)
