
(:TYPE :SYSTEM :NAME "cl-emacs-if" :AUTHOR NIL :MAINTAINER NIL :VERSION "1.0"
 :LICENCE NIL :DESCRIPTION "Provides Emacs-like IF and WHILE operators"
 :LONG-DESCRIPTION NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "EMACS-IF" :FULL-NAME "EMACS-IF" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :MACRO :SYMBOL (:NAME "IF" :PACKAGE-NAME "EMACS-IF" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "TEST" :PACKAGE-NAME "EMACS-IF" :EXTERNALP NIL)
      (:NAME "THEN" :PACKAGE-NAME "EMACS-IF" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "ELSE" :PACKAGE-NAME "EMACS-IF" :EXTERNALP NIL))
     :DOCUMENTATION
     "Emacs like IF statement that allows multiple ELSE clauses")
    (:TYPE :MACRO :SYMBOL (:NAME "WHILE" :PACKAGE-NAME "EMACS-IF" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "TEST" :PACKAGE-NAME "EMACS-IF" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "EMACS-IF" :EXTERNALP NIL))
     :DOCUMENTATION
     "Emacs like WHILE loop statement. Just like other CL loops the body
is wrapped into the BLOCK named NIL and a TAGBODY form"))
   :EXTERNAL-SYMBOLS
   ((:NAME "IF" :PACKAGE-NAME "EMACS-IF" :EXTERNALP T)
    (:NAME "WHILE" :PACKAGE-NAME "EMACS-IF" :EXTERNALP T)))))
