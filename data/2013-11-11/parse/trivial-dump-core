
(:TYPE :SYSTEM :NAME "trivial-dump-core" :AUTHOR
 "Rolando Pereira <rolando_pereira_2@hotmail.com>" :MAINTAINER NIL :VERSION NIL
 :LICENCE "MIT License" :DESCRIPTION
 "trivial-dump-core: Wrapper that allows the saving of the
current lisp image as a \"core\" or as an \"executable\" across
implementations."
 :LONG-DESCRIPTION NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "TRIVIAL-DUMP-CORE" :FULL-NAME "TRIVIAL-DUMP-CORE"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "DUMP-IMAGE" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "FILENAME" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP NIL))
     :DOCUMENTATION
     "Dump the current state of the Lisp process to an image file.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "SAVE-EXECUTABLE" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "FILENAME" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP NIL)
      (:NAME "INIT-FUNCTION" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP NIL))
     :DOCUMENTATION
     "Make a stand-alone executable file from the current Lisp process.

FILENAME: Path of the executable file

INIT-FUNCTION: Zero-argument function object that acts as the entry
point to your executable (the equivalent of main() in C).

Some implementations (Clozure, SBCL) will quit after this function is
called.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PRINT-DUMP-IMAGE-SLIME-HELP" :PACKAGE-NAME "TRIVIAL-DUMP-CORE"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "FILENAME" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP NIL))
     :DOCUMENTATION
     "Print on the screen the command the user needs to run on the
*inferior-lisp* buffer to dump this image. This macro is only called
when trying to dump a sbcl lisp image inside slime.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "PRINT-SAVE-SLIME-AND-DIE-HELP" :PACKAGE-NAME "TRIVIAL-DUMP-CORE"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "FILENAME" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP NIL)
      (:NAME "INIT-FUNCTION" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP NIL))
     :DOCUMENTATION
     "Print on the screen the command the user needs to run on the
*inferior-lisp* buffer to save this image. This macro is only called
when trying to save a sbcl lisp image inside slime.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "%SAVE-EXECUTABLE-SBCL" :PACKAGE-NAME "TRIVIAL-DUMP-CORE"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "FILENAME" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP NIL)
      (:NAME "INIT-FUNCTION" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "IS-SLIME-RUNNING" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP
      NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION
     "Return T if slime is running in the lisp image, otherwise return NIL.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "STRING-TO-SYMBOL" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP
      NIL)
     :LAMBDA-LIST ((:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Convert a string to a symbol")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SBCL-SAVE-SLIME-AND-DIE" :PACKAGE-NAME "TRIVIAL-DUMP-CORE"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "FILENAME" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP NIL)
      (:NAME "INIT-FUNCTION" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP NIL))
     :DOCUMENTATION "Save a sbcl image, even when running from inside Slime.

This function should only be used in the *inferior-buffer* buffer,
inside emacs.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SBCL-DUMP-IMAGE-SLIME" :PACKAGE-NAME "TRIVIAL-DUMP-CORE"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "FILENAME" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP NIL))
     :DOCUMENTATION "Save a sbcl image, even when running from inside Slime.

This function should only be used in the *inferior-buffer* buffer,
inside emacs."))
   :EXTERNAL-SYMBOLS
   ((:NAME "DUMP-IMAGE" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP T)
    (:NAME "SAVE-EXECUTABLE" :PACKAGE-NAME "TRIVIAL-DUMP-CORE" :EXTERNALP T)))))
