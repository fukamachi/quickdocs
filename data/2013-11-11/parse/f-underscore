
(:TYPE :SYSTEM :NAME "f-underscore" :AUTHOR "Nick Allen <nallen05@gmail.com>"
 :MAINTAINER NIL :VERSION "0.1" :LICENCE NIL :DESCRIPTION
 "a tiny library of functional programming utils placed into the public domain.

the idea is to make functional programs shorter and easier to read without resorting
to syntax [like arc's square bracket unary function syntax]"
 :LONG-DESCRIPTION NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "F-UNDERSCORE" :FULL-NAME "F-UNDERSCORE" :DOCUMENTATION
   NIL :SYMBOL-LIST
   ((:TYPE :MACRO :SYMBOL (:NAME "F" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "ARGS" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP NIL))
     :DOCUMENTATION "a synonym for LAMBDA")
    (:TYPE :MACRO :SYMBOL
     (:NAME "F0" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP NIL))
     :DOCUMENTATION "a LAMBDA that takes 0 arguments (aka a 'thunk')")
    (:TYPE :MACRO :SYMBOL
     (:NAME "F_" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP NIL))
     :DOCUMENTATION "a LAMBDA that takes 1 argument: `_'")
    (:TYPE :MACRO :SYMBOL
     (:NAME "F_N" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP NIL))
     :DOCUMENTATION "a LAMBDA that takes 1 &REST argument: `_'")
    (:TYPE :MACRO :SYMBOL
     (:NAME "F_%" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP NIL))
     :DOCUMENTATION "a LAMBDA that takes 1 &REST that it ignores")
    (:TYPE :MACRO :SYMBOL (:NAME "M" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "MACRO-LAMBDA-LIST" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP NIL))
     :DOCUMENTATION
     "a LAMBDA that has a macro-lambda-list instead of an ordinary lambda-list"))
   :EXTERNAL-SYMBOLS
   ((:NAME "F_N" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T)
    (:NAME "F_%" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T)
    (:NAME "F0" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T)
    (:NAME "F" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T)
    (:NAME "_" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T)
    (:NAME "M" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T)
    (:NAME "F_" :PACKAGE-NAME "F-UNDERSCORE" :EXTERNALP T)))))
