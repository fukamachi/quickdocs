    (:TYPE :CLASS :SYMBOL
     (:NAME "UNIT-TEST" :PACKAGE-NAME "UNIT-TEST" :EXTERNALP NIL)
     :SUPER-CLASS-LIST NIL :DOCUMENTATION
     "The base class for all unit tests.  When tests are defined,
they will be stored in a global list of tests which can be run at any time with
RUN-ALL-TESTS."
     :SLOT-LIST
     ((:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "UNIT" :PACKAGE-NAME "UNIT-TEST" :EXTERNALP NIL) :ACCESSORS NIL
       :READERS
       ((:NAME "UNIT-TEST-UNIT" :PACKAGE-NAME "UNIT-TEST" :EXTERNALP NIL))
       :WRITERS NIL :DOCUMENTATION "Name of unit being tested.
Used to group related tests.")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "NAME" :PACKAGE-NAME "UNIT-TEST" :EXTERNALP NIL) :ACCESSORS NIL
