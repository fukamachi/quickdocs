   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*UNIT-TESTS*" :PACKAGE-NAME "UNIT-TEST" :EXTERNALP NIL)
     :DOCUMENTATION "This contains all unit tests.  Tests are added
simply by creating instances of test objects."
     :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*UNIT-TEST-DEBUG*" :PACKAGE-NAME "UNIT-TEST" :EXTERNALP T)
