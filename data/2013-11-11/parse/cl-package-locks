
(:TYPE :SYSTEM :NAME "cl-package-locks" :AUTHOR
 "Elliott Johnson <elliott@elliottjohnson.net>" :MAINTAINER NIL :VERSION
 "0.0.2" :LICENCE "Copyright 2011 Elliott Johnson" :DESCRIPTION
 "A library to provide a unified way to work with package locks across supported common lisp implementations."
 :LONG-DESCRIPTION NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-PACKAGE-LOCKS" :FULL-NAME "CL-PACKAGE-LOCKS"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "RESOLVE-PACKAGE" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Resolves a descriptor to a package object.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "RESOLVE-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP NIL))
     :DOCUMENTATION "Resolves a list of packages.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PACKAGE-LOCKED-P" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Returns true if a given resolveable PACKAGE is locked.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "LOCKED-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP NIL))
     :DOCUMENTATION
     "Accepts a list of packages and returns those that are locked.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ALL-LOCKED-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP
      T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION "Returns a list of all locked packages.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "UNLOCKED-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP NIL))
     :DOCUMENTATION
     "Accepts a list of packages and returns those that are unlocked.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ALL-UNLOCKED-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP
      T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION "Returns a list of all unlocked packages.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "LOCK-PACKAGE" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Locks a provided package.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "LOCK-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP NIL))
     :DOCUMENTATION "Locks the provided packages.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "UNLOCK-PACKAGE" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Unlocks a provided package.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "UNLOCK-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP NIL))
     :DOCUMENTATION "Unlocks the provided packages.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "WITH-PACKAGES-UNLOCKED" :PACKAGE-NAME "CL-PACKAGE-LOCKS"
      :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP NIL))
     :DOCUMENTATION
     "Accepts a list of packages that be unlocked for the duration 
of BODY and locked upon return.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "WITHOUT-PACKAGE-LOCKS" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP
      T)
     :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP NIL))
     :DOCUMENTATION "Unlocks all packages for the duration of body."))
   :EXTERNAL-SYMBOLS
   ((:NAME "PACKAGE-LOCKED-P" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
    (:NAME "LOCK-PACKAGE" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
    (:NAME "LOCK-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
    (:NAME "ALL-UNLOCKED-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP
     T)
    (:NAME "RESOLVE-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
    (:NAME "WITH-PACKAGES-UNLOCKED" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP
     T)
    (:NAME "LOCKED-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
    (:NAME "ALL-LOCKED-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
    (:NAME "UNLOCK-PACKAGE" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
    (:NAME "WITHOUT-PACKAGE-LOCKS" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP
     T)
    (:NAME "UNLOCKED-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
    (:NAME "UNLOCK-PACKAGES" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)
    (:NAME "RESOLVE-PACKAGE" :PACKAGE-NAME "CL-PACKAGE-LOCKS" :EXTERNALP T)))))
