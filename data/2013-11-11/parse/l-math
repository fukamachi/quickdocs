    (:TYPE :GENERIC :SYMBOL
     (:NAME "BILINEAR-INTERPOLATION" :PACKAGE-NAME "L-MATH" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "TOP-LEFT" :PACKAGE-NAME "L-MATH" :EXTERNALP NIL)
      (:NAME "BOTTOM-LEFT" :PACKAGE-NAME "L-MATH" :EXTERNALP NIL)
      (:NAME "TOP-RIGHT" :PACKAGE-NAME "L-MATH" :EXTERNALP NIL)
