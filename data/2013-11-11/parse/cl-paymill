     (:NAME "CLEAR-COOKIE-JAR" :PACKAGE-NAME "PAYMILL" :EXTERNALP NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "RESOURCE-FROM-KEYWORD" :PACKAGE-NAME "PAYMILL" :EXTERNALP NIL)
