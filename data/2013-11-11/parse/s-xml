
(:TYPE :SYSTEM :NAME "s-xml" :AUTHOR "Sven Van Caekenberghe <svc@mac.com>"
 :MAINTAINER
 "Sven Van Caekenberghe <svc@mac.com>, Brian Mastenbrook <>, Rudi Schlatte <>"
 :VERSION "3" :LICENCE "Lisp Lesser General Public License (LLGPL)"
 :DESCRIPTION "Simple Common Lisp XML Parser" :LONG-DESCRIPTION
 "S-XML is a Common Lisp implementation of a simple XML parser, with a SAX-like and DOM interface"
 :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "S-XML" :FULL-NAME "S-XML" :DOCUMENTATION
   "A simple XML parser with an efficient, purely functional, event-based interface as well as a DOM interface"
   :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "PARSER-ERROR" :PACKAGE-NAME "S-XML" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "MESSAGE" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "ARGS" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PARSE-ATTRIBUTE-NAME" :PACKAGE-NAME "S-XML" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Default parser for the attribute name")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PARSE-ATTRIBUTE-VALUE" :PACKAGE-NAME "S-XML" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
      (:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Default parser for the attribute value")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*ATTRIBUTE-NAME-PARSER*" :PACKAGE-NAME "S-XML" :EXTERNALP T)
     :DOCUMENTATION
     "Called to compute interned attribute name from a buffer that will be reused"
     :INITIAL-VALUE "#'S-XML:PARSE-ATTRIBUTE-NAME")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*ATTRIBUTE-VALUE-PARSER*" :PACKAGE-NAME "S-XML" :EXTERNALP T)
     :DOCUMENTATION
     "Called to compute an element of an attribute list from a buffer that will be reused"
     :INITIAL-VALUE "#'S-XML:PARSE-ATTRIBUTE-VALUE")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WHITESPACE-CHAR-P" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "CHAR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Is char an XML whitespace character ?")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "IDENTIFIER-CHAR-P" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "CHAR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Is char an XML identifier character ?")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SKIP-WHITESPACE" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION
     "Skip over XML whitespace in stream, return first non-whitespace
  character which was peeked but not read, return nil on eof")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-EXTENDABLE-STRING" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "SIZE" :PACKAGE-NAME "S-XML" :EXTERNALP NIL) 10))
     :DOCUMENTATION
     "Make an extendable string which is a one-dimensional character
  array which is adjustable and has a fill pointer")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PRINT-STRING-XML" :PACKAGE-NAME "S-XML" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "START" :PACKAGE-NAME "S-XML" :EXTERNALP NIL) 0)
      (:NAME "END" :PACKAGE-NAME "S-XML" :EXTERNALP NIL))
     :DOCUMENTATION
     "Write the characters of string to stream using basic XML conventions")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-STANDARD-ENTITIES" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION
     "A hashtable mapping XML entity names to their replacement strings,
  filled with the standard set")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "RESOLVE-ENTITY" :PACKAGE-NAME "S-XML" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "EXTENDABLE-STRING" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
      (:NAME "ENTITIES" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
      (:NAME "ENTITY" :PACKAGE-NAME "S-XML" :EXTERNALP NIL))
     :DOCUMENTATION
     "Read and resolve an XML entity from stream, positioned after the '&' entity marker, 
  accepting &name; &#DEC; and &#xHEX; formats,
  destructively modifying string, which is also returned,
  destructively modifying entity, incorrect entity formats result in errors")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*IGNORE-NAMESPACES*" :PACKAGE-NAME "S-XML" :EXTERNALP T)
     :DOCUMENTATION
     "When t, namespaces are ignored like in the old version of S-XML"
     :INITIAL-VALUE "NIL")
    (:TYPE :CLASS :SYMBOL
     (:NAME "XML-NAMESPACE" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
     :SUPER-CLASS-LIST NIL :DOCUMENTATION
     "Describes an XML namespace and how it is handled" :SLOT-LIST
     ((:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "URI" :PACKAGE-NAME "S-XML" :EXTERNALP NIL) :ACCESSORS
       ((:NAME "GET-URI" :PACKAGE-NAME "S-XML" :EXTERNALP T)) :READERS NIL
       :WRITERS NIL :DOCUMENTATION "The URI used to identify this namespace")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "PREFIX" :PACKAGE-NAME "S-XML" :EXTERNALP NIL) :ACCESSORS
       ((:NAME "GET-PREFIX" :PACKAGE-NAME "S-XML" :EXTERNALP T)) :READERS NIL
       :WRITERS NIL :DOCUMENTATION
       "The preferred prefix assigned to this namespace")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :ACCESSORS
       ((:NAME "GET-PACKAGE" :PACKAGE-NAME "S-XML" :EXTERNALP T)) :READERS NIL
       :WRITERS NIL :DOCUMENTATION
       "The Common Lisp package where this namespace's symbols are interned")))
    (:TYPE :METHOD :SYMBOL
     (:NAME "PRINT-OBJECT" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "OBJECT" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
       (:NAME "XML-NAMESPACE" :PACKAGE-NAME "S-XML" :EXTERNALP NIL))
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*LOCAL-NAMESPACE*" :PACKAGE-NAME "S-XML" :EXTERNALP T)
     :DOCUMENTATION "The local (global default) XML namespace" :INITIAL-VALUE
     "(MAKE-INSTANCE 'S-XML::XML-NAMESPACE :URI \"local\" :PREFIX \"\" :PACKAGE
               (FIND-PACKAGE :KEYWORD))")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*XML-NAMESPACE*" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
     :DOCUMENTATION
     "REC-xml-names-19990114 says the prefix xml is bound to the namespace http://www.w3.org/XML/1998/namespace."
     :INITIAL-VALUE "(MAKE-INSTANCE 'S-XML::XML-NAMESPACE :URI
               \"http://www.w3.org/XML/1998/namespace\" :PREFIX \"xml\" :PACKAGE
               (OR (FIND-PACKAGE :XML) (MAKE-PACKAGE :XML :NICKNAMES '(\"XML\"))))")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*KNOWN-NAMESPACES*" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)
     :DOCUMENTATION "The list of known/defined namespaces" :INITIAL-VALUE
     "(LIST S-XML:*LOCAL-NAMESPACE* S-XML::*XML-NAMESPACE*)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*NAMESPACES*" :PACKAGE-NAME "S-XML" :EXTERNALP T) :DOCUMENTATION
     "Ordered list of (prefix . XML-namespace) bindings currently in effect - special variable"
     :INITIAL-VALUE
     "`((,\"xml\" ,@S-XML::*XML-NAMESPACE*) (,\"\" ,@S-XML:*LOCAL-NAMESPACE*))")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FIND-NAMESPACE" :PACKAGE-NAME "S-XML" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "URI" :PACKAGE-NAME "S-XML" :EXTERNALP NIL)) :DOCUMENTATION
