    (:TYPE :GENERIC :SYMBOL
     (:NAME "EDGES-NODES-EQUAL" :PACKAGE-NAME "EPIGRAPH" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "EDGE1" :PACKAGE-NAME "EPIGRAPH" :EXTERNALP NIL)
      (:NAME "EDGE2" :PACKAGE-NAME "EPIGRAPH" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "TEST" :PACKAGE-NAME "EPIGRAPH" :EXTERNALP NIL))
     :DOCUMENTATION "Returns T if the two edges connect the same
  nodes. Note that currently there is no notion of direction between
  edges, so edges from A to B and from B to A are equivalent and
  therefore edges-nodes-equal returns T for those two edges.")
    (:TYPE :METHOD :SYMBOL
     (:NAME "EDGES-NODES-EQUAL" :PACKAGE-NAME "EPIGRAPH" :EXTERNALP T)
