
(:TYPE :SYSTEM :NAME "epigraph" :AUTHOR "Cyrus Harmon <ch-lisp@bobobeach.com>"
 :MAINTAINER NIL :VERSION "0.0.2" :LICENCE "BSD" :DESCRIPTION
 "A library for representing and processing graphs (nodes and edges)"
 :LONG-DESCRIPTION NIL :DEPENDS-ON ("ALEXANDRIA") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "EPIGRAPH" :FULL-NAME "EPIGRAPH" :DOCUMENTATION NIL
