
(:TYPE :SYSTEM :NAME "3bmd-ext-code-blocks" :AUTHOR NIL :MAINTAINER NIL
 :VERSION NIL :LICENCE NIL :DESCRIPTION
 "extension to 3bmd implementing github style ``` delimited code blocks, with support for syntax highlighting using colorize"
 :LONG-DESCRIPTION NIL :DEPENDS-ON ("3BMD" "COLORIZE" "ALEXANDRIA")
 :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "3BMD-CODE-BLOCKS" :FULL-NAME "3BMD-CODE-BLOCKS"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*CODE-BLOCKS-DEFAULT-COLORIZE*" :PACKAGE-NAME "3BMD-CODE-BLOCKS"
      :EXTERNALP T)
     :DOCUMENTATION
     "a colorize coloring type name, like :common-lisp or :elisp "
     :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*COLORIZE-CODE-SPANS-AS*" :PACKAGE-NAME "3BMD-CODE-BLOCKS"
      :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*COLORIZE-VERBATIM-BLOCK-AS*" :PACKAGE-NAME "3BMD-CODE-BLOCKS"
      :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*COLORIZE-NAME-MAP*" :PACKAGE-NAME "3BMD-CODE-BLOCKS" :EXTERNALP
      NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(ALEXANDRIA.0.DEV:PLIST-HASH-TABLE
 '(\"lisp\" :COMMON-LISP \"basiclisp\" :LISP \"scheme\" :SCHEME \"elisp\" :ELISP
   \"emacslisp\" :ELISP \"cl\" :COMMON-LISP \"commonlisp\" :COMMON-LISP \"clisp\"
   :COMMON-LISP \"c\" :C \"c++\" :C++ \"java\" :JAVA \"objc\" :OBJECTIVE-C \"objectivec\"
   :OBJECTIVE-C \"erlang\" :ERLANG \"python\" :PYTHON \"haskell\" :HASKELL \"diff\"
   :DIFF \"webkit\" :WEBKIT)
 :TEST #'EQUAL)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FIND-COLORING-TYPE" :PACKAGE-NAME "3BMD-CODE-BLOCKS" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "3BMD-CODE-BLOCKS" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*CODE-BLOCKS*" :PACKAGE-NAME "3BMD-CODE-BLOCKS" :EXTERNALP T)
     :DOCUMENTATION NIL :INITIAL-VALUE "NIL")
    (:TYPE :METHOD :SYMBOL
     (:NAME "PRINT-TAGGED-ELEMENT" :PACKAGE-NAME "3BMD-EXT" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "TAG" :PACKAGE-NAME "3BMD-CODE-BLOCKS" :EXTERNALP NIL)
       ((:NAME "EQL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        ((:NAME "QUOTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
         (:NAME "CODE-BLOCK" :PACKAGE-NAME "3BMD-CODE-BLOCKS" :EXTERNALP
          NIL))))
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "PRINT-TAGGED-ELEMENT" :PACKAGE-NAME "3BMD-EXT" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "TAG" :PACKAGE-NAME "3BMD-CODE-BLOCKS" :EXTERNALP NIL)
       ((:NAME "EQL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        (:NAME "CODE" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)))
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "*CODE-BLOCKS*" :PACKAGE-NAME "3BMD-CODE-BLOCKS" :EXTERNALP T)
