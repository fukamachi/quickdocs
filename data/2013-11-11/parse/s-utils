
(:TYPE :SYSTEM :NAME "s-utils" :AUTHOR "Sven Van Caekenberghe <svc@mac.com>"
 :MAINTAINER "Sven Van Caekenberghe <svc@mac.com>" :VERSION "1" :LICENCE
 "Lesser Lisp General Public License (LLGPL)" :DESCRIPTION
 "S-UTILS is collection of Common Lisp utilities" :LONG-DESCRIPTION
 "S-UTILS is collection of Common Lisp utilities" :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "S-UTILS" :FULL-NAME "S-UTILS" :DOCUMENTATION
   "S-UTILS is collection of Common Lisp utilities" :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-SUBDIRECTORY" :PACKAGE-NAME "S-UTILS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "BASEDIR" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
      (:NAME "SUBDIR" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL))
     :DOCUMENTATION
     "Give a pathname, basedir, of a directory, create a subdirectory with name subdir")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PATHNAME-PARENT" :PACKAGE-NAME "S-UTILS" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "PATHNAME" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Given a pathname, return the parent pathname")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "COPY-STREAM" :PACKAGE-NAME "S-UTILS" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "IN" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
      (:NAME "OUT" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "BUFFER" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
       ((:NAME "MAKE-STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) 4096))
      (:NAME "SECOND-BUFFER" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
      ((:NAME "CONVERTOR" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
       ((:NAME "FUNCTION" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        (:NAME "IDENTITY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))))
     :DOCUMENTATION
     "Copy all data from input stream in to output stream out using buffer (and read/write-sequence)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TOKENS" :PACKAGE-NAME "S-UTILS" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "START" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL) 0)
      (:NAME "END" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
      ((:NAME "SEPARATORS" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
       ((:NAME "LIST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) #\  #\Return
        #\Newline #\Tab)))
     :DOCUMENTATION
     "Split string in a list of tokens using separators, a list of characters")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+US-DAY-NAMES+" :PACKAGE-NAME "S-UTILS" :EXTERNALP T)
     :DOCUMENTATION "US English short day name constant strings" :INITIAL-VALUE
     "'(\"Mon\" \"Tue\" \"Wed\" \"Thu\" \"Fri\" \"Sat\" \"Sun\")")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+US-MONTH-NAMES+" :PACKAGE-NAME "S-UTILS" :EXTERNALP T)
     :DOCUMENTATION "US English short month name constant strings"
     :INITIAL-VALUE
     "'(\"Jan\" \"Feb\" \"Mar\" \"Apr\" \"May\" \"Jun\" \"Jul\" \"Aug\" \"Sep\" \"Oct\" \"Nov\" \"Dec\")")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+US-TIME-FORMAT+" :PACKAGE-NAME "S-UTILS" :EXTERNALP T)
     :DOCUMENTATION "US English style date-time format" :INITIAL-VALUE
     "'(:DAY-NAME #\\  :MONTH-NAME #\\  :DATE #\\  :YEAR #\\  :HOUR #\\: :MINUTE #\\:
  :SECOND)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+US-DATE-FORMAT+" :PACKAGE-NAME "S-UTILS" :EXTERNALP T)
     :DOCUMENTATION "US English style date-only format" :INITIAL-VALUE
     "'(:DAY-NAME #\\  :MONTH-NAME #\\  :DATE #\\  :YEAR)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FORMAT-UNIVERSAL-TIME" :PACKAGE-NAME "S-UTILS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "UNIVERSAL-TIME" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FORMAT" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "+US-TIME-FORMAT+" :PACKAGE-NAME "S-UTILS" :EXTERNALP T))
      ((:NAME "DAY-NAMES" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
       (:NAME "+US-DAY-NAMES+" :PACKAGE-NAME "S-UTILS" :EXTERNALP T))
      ((:NAME "MONTH-NAMES" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
       (:NAME "+US-MONTH-NAMES+" :PACKAGE-NAME "S-UTILS" :EXTERNALP T))
      (:NAME "DECODE-IN-TIMEZONE" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION
     "Format universal time using format, day-names and month-names, if stream is not nil use it, else return a string")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+EN-DURATION-UNIT-NAMES+" :PACKAGE-NAME "S-UTILS" :EXTERNALP T)
     :DOCUMENTATION "English time duration unit name constant strings"
     :INITIAL-VALUE "#(\"year\" \"day\" \"hour\" \"minute\" \"second\")")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FORMAT-DURATION" :PACKAGE-NAME "S-UTILS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "SECONDS" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "UNIT-NAMES" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
       (:NAME "+EN-DURATION-UNIT-NAMES+" :PACKAGE-NAME "S-UTILS" :EXTERNALP T))
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION
     "Format seconds as duration using unit-names, if stream is not nil use it, else return a string")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FORMAT-ISO-GMT-TIME" :PACKAGE-NAME "S-UTILS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "UNIVERSAL-TIME" :PACKAGE-NAME "S-UTILS" :EXTERNALP NIL)
