
(:TYPE :SYSTEM :NAME "quickproject" :AUTHOR NIL :MAINTAINER NIL :VERSION "1.2"
 :LICENCE NIL :DESCRIPTION "Creates the skeleton of a new Common Lisp project"
 :LONG-DESCRIPTION NIL :DEPENDS-ON ("CL-FAD" "HTML-TEMPLATE") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "QUICKPROJECT" :FULL-NAME "QUICKPROJECT" :DOCUMENTATION
   "The Quickproject package." :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*NAME*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
     :DOCUMENTATION "The name of the project currently being created.")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*TEMPLATE-DIRECTORY*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP T)
     :DOCUMENTATION "A directory to use as a source of template files."
     :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*AUTHOR*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP T)
     :DOCUMENTATION "Set this variable to your contact information."
     :INITIAL-VALUE "\"Your Name <your.name@example.com>\"")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*LICENSE*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP T)
     :DOCUMENTATION NIL :INITIAL-VALUE "\"Specify license here\"")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "UNINTERNED-SYMBOLIZE" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "NAME" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return an uninterned symbol named after NAME, which is treated as a
string designator and upcased.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WRITE-SYSTEM-FORM" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "DEPENDS-ON" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
      ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*STANDARD-OUTPUT*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Write an asdf defsystem form for NAME to STREAM.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PATHNAME-PROJECT-NAME" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP
      NIL)
     :LAMBDA-LIST ((:NAME "PATHNAME" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION
     "Return a project name based on PATHNAME by taking the last element
in the pathname-directory list. E.g. returns \"awesome-project\" for
#p\"src/awesome-project/\".")
    (:TYPE :MACRO :SYMBOL
     (:NAME "WITH-NEW-FILE" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
     :LAMBDA-LIST
     (((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "FILE" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL))
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL))
     :DOCUMENTATION
     "Like WITH-OPEN-FILE, but specialized for output to a file that must
not already exist.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FILE-COMMENT-HEADER" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WRITE-SYSTEM-FILE" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
      (:NAME "FILE" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "DEPENDS-ON" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WRITE-README-FILE" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
      (:NAME "FILE" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WRITE-PACKAGE-FILE" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
      (:NAME "FILE" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WRITE-APPLICATION-FILE" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
      (:NAME "FILE" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*AFTER-MAKE-PROJECT-HOOKS*" :PACKAGE-NAME "QUICKPROJECT"
      :EXTERNALP T)
     :DOCUMENTATION
     "A list of functions to call after MAKE-PROJECT is finished making a
project. Each function is called with the same arguments passed to
MAKE-PROJECT, except that NAME is canonicalized if
necessary. *DEFAULT-PATHNAME-DEFAULTS* bound to the newly created
project directory."
     :INITIAL-VALUE "NIL")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "REWRITE-TEMPLATES" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "TEMPLATE-DIRECTORY" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
      (:NAME "TARGET-DIRECTORY" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
      (:NAME "PARAMETERS" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL))
     :DOCUMENTATION
     "Treat every file in TEMPLATE-DIRECTORY as a template file; fill it
out using PARAMETERS into a corresponding file in
TARGET-DIRECTORY. The rewriting uses HTML-TEMPLATE. The template start
marker is the string \"(#|\" and the template end marker is the string
\"|#)\". Template vars are not modified or escaped when written.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "DEFAULT-TEMPLATE-PARAMETERS" :PACKAGE-NAME "QUICKPROJECT"
      :EXTERNALP T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION
     "Return a plist of :NAME, :LICENSE, and :AUTHOR parameters.")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*TEMPLATE-PARAMETER-FUNCTIONS*" :PACKAGE-NAME "QUICKPROJECT"
      :EXTERNALP T)
     :DOCUMENTATION
     "A list of functions that return plists for use when rewriting
  template files. The results of calling each function are appended
  together to pass to FILL-AND-PRINT-TEMPLATE."
     :INITIAL-VALUE "(LIST 'QUICKPROJECT:DEFAULT-TEMPLATE-PARAMETERS)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TEMPLATE-PARAMETERS" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "INITIAL-PARAMETERS" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return all template parameters returned by calling each element in
*TEMPLATE-PARAMETER-FUNCTIONS*, appended together as a single plist.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-PROJECT" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PATHNAME" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "DEPENDS-ON" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
      (:NAME "TEMPLATE-PARAMETERS" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
      (((:NAME "TEMPLATE-DIRECTORY" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "*TEMPLATE-DIRECTORY*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP
         T))
       (:NAME "*TEMPLATE-DIRECTORY*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP
        T))
      (((:NAME "AUTHOR" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "*AUTHOR*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP T))
       (:NAME "*AUTHOR*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP T))
      (((:NAME "LICENSE" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "*LICENSE*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP T))
       (:NAME "*LICENSE*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP T))
      ((:NAME "NAME" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)
       ((:NAME "PATHNAME-PROJECT-NAME" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP
         NIL)
        (:NAME "PATHNAME" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
       (:NAME "NAME-PROVIDED-P" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP NIL)))
     :DOCUMENTATION
     "Create a project skeleton for NAME in PATHNAME. If DEPENDS-ON is provided,
it is used as the asdf defsystem depends-on list."))
   :EXTERNAL-SYMBOLS
   ((:NAME "*AUTHOR*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP T)
    (:NAME "*LICENSE*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP T)
    (:NAME "*TEMPLATE-PARAMETER-FUNCTIONS*" :PACKAGE-NAME "QUICKPROJECT"
     :EXTERNALP T)
    (:NAME "DEFAULT-TEMPLATE-PARAMETERS" :PACKAGE-NAME "QUICKPROJECT"
     :EXTERNALP T)
    (:NAME "*AFTER-MAKE-PROJECT-HOOKS*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP
     T)
    (:NAME "*TEMPLATE-DIRECTORY*" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP T)
    (:NAME "MAKE-PROJECT" :PACKAGE-NAME "QUICKPROJECT" :EXTERNALP T)))))
