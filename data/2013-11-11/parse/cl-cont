
(:TYPE :SYSTEM :NAME "cl-cont" :AUTHOR
 "Slava Akhmechet, Stephen Compall <s11@member.fsf.org>, John Fremlin, Leslie P. Polzer <polzer@gnu.org>"
 :MAINTAINER "Leslie P. Polzer <polzer@gnu.org>" :VERSION "0.3.8" :LICENCE
 "LLGPL" :DESCRIPTION "A library that implements continuations by
  transforming Common Lisp code to continuation passing style."
 :LONG-DESCRIPTION NIL :DEPENDS-ON ("CLOSER-MOP" "ALEXANDRIA") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-CONT" :FULL-NAME "CL-CONT" :DOCUMENTATION
   "A library that implements continuations by transforming Common
  Lisp code to continuation passing style."
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*SPECIAL-FORM-TRANSFORMERS*" :PACKAGE-NAME "CL-CONT" :EXTERNALP
      NIL)
     :DOCUMENTATION
     "A hashtable that maps symbols to functions. Each function acts as a
  transformer of a special form and must accept three arguments: an
  s-expression to transform to CPS-style, an an s-expression to
  continue to, and an environment object."
     :INITIAL-VALUE "(MAKE-HASH-TABLE :SIZE 25)")
    (:TYPE :MACRO :SYMBOL
     (:NAME "DEFCPSTRANSFORMER" :PACKAGE-NAME "CL-CONT" :EXTERNALP T)
     :LAMBDA-LIST
