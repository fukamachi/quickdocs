
(:TYPE :SYSTEM :NAME "message-oo" :AUTHOR "Roman Klochkov <kalimehtar@mail.ru>"
 :MAINTAINER NIL :VERSION "0.9.2" :LICENCE "BSD" :DESCRIPTION
 "Message-passing Smalltalk-style object system" :LONG-DESCRIPTION NIL
 :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "MESSAGE-OO" :FULL-NAME "MESSAGE-OO" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "+MANGLE+" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(FIND-PACKAGE \"MESSAGE-OO.MANGLE\")")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "METHOD-NAME" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "MESSAGE-LIST" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION "(:add item) -> message-oo.mangle:|ADD:|
   (:add item :at pos) -> message-oo.mangle:|ADD:AT:|
   :add -> message-oo.mangle:add
   (:add item &rest rest) -> message-oo.mangle:|ADD::|
   (:add item :at pos &rest rest) -> message-oo.mangle:|ADD:AT::|")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "METHOD-PARAMS" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "MESSAGE-LIST" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FOR-GENERIC" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "METHOD-PARAMS" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "DEFMESSAGE" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "CLASS-LIST" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL)
      (:NAME "MESSAGE-LIST" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION "It is smalltalk-like:
 (defmessage container (:add item :after old-item)
   (container-add-after container item old-item))")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FIND-MESSAGE" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "MESSAGE" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL (:NAME "@" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "OBJECT" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL)
      (:NAME "MESSAGE" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "MORE-MESSAGES" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION "Enables message pipelines
 (@ l (:map (lambda (x) (process x)))) == (mapcar ...)
 (@ l (:map func) (:reduce #'+)) == (reduce (mapcar ..))")
    (:TYPE :GENERIC :SYMBOL
     (:NAME "GENERIC" :PACKAGE-NAME "MESSAGE-OO.MANGLE" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "OBJECT" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :GENERIC :SYMBOL
     (:NAME "GENERIC" :PACKAGE-NAME "MESSAGE-OO.MANGLE" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "OBJECT" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "GENERIC" :PACKAGE-NAME "MESSAGE-OO.MANGLE" :EXTERNALP NIL)
     :LAMBDA-LIST
     (((:NAME "LIST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "LIST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION NIL)
    (:TYPE :GENERIC :SYMBOL
     (:NAME "FN" :PACKAGE-NAME "MESSAGE-OO.MANGLE" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "OBJECT" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :GENERIC :SYMBOL
     (:NAME "FN" :PACKAGE-NAME "MESSAGE-OO.MANGLE" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "OBJECT" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "FN" :PACKAGE-NAME "MESSAGE-OO.MANGLE" :EXTERNALP NIL) :LAMBDA-LIST
     (((:NAME "LIST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "LIST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Returns function that can be applied to object
Example (mapcar (@ '(:+ 3) :fn) (list 1 2 3 4)) => '(4 5 6 7)")
    (:TYPE :GENERIC :SYMBOL
     (:NAME "TAP:" :PACKAGE-NAME "MESSAGE-OO.MANGLE" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "OBJECT" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL)
      (:NAME "FUNC" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :GENERIC :SYMBOL
     (:NAME "TAP:" :PACKAGE-NAME "MESSAGE-OO.MANGLE" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "OBJECT" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL)
      (:NAME "FUNC" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "TAP:" :PACKAGE-NAME "MESSAGE-OO.MANGLE" :EXTERNALP NIL)
     :LAMBDA-LIST
     (((:NAME "ANY" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL)
       (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      (:NAME "FUNC" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP NIL))
     :DOCUMENTATION "For debugging like in 
 (@ obj (:message1 ...) (:message2 ...) (:tap (lambda (x) (print x))) 
        (:message3 ...))"))
   :EXTERNAL-SYMBOLS
   ((:NAME "DEFMESSAGE" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP T)
    (:NAME "@" :PACKAGE-NAME "MESSAGE-OO" :EXTERNALP T)))
  (:TYPE :PACKAGE :NAME ".MANGLE" :FULL-NAME "MESSAGE-OO.MANGLE" :DOCUMENTATION
   NIL :SYMBOL-LIST NIL :EXTERNAL-SYMBOLS NIL)))
