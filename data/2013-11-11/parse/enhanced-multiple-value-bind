
(:TYPE :SYSTEM :NAME "enhanced-multiple-value-bind" :AUTHOR
 "Jean-Philippe Paradis <hexstream@gmail.com>" :MAINTAINER NIL :VERSION "1.0.1"
 :LICENCE "Public Domain" :DESCRIPTION
 "Provides an enhanced MULTIPLE-VALUE-BIND macro that adds support for lambda keywords by expanding to a MULTIPLE-VALUE-CALL when necessary. This makes catching multiple-value &rest and &key much more lightweight and convenient. A MULTIPLE-VALUE-&BIND alias is supported."
 :LONG-DESCRIPTION NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "ENHANCED-MULTIPLE-VALUE-BIND" :FULL-NAME
   "ENHANCED-MULTIPLE-VALUE-BIND" :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "%EXPAND" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     ((:NAME "LAMBDA-LIST" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND"
       :EXTERNALP NIL)
      (:NAME "VALUES-FORM" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND"
       :EXTERNALP NIL)
      (:NAME "BODY" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND" :EXTERNALP
       NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "MULTIPLE-VALUE-BIND" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND"
      :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "LAMBDA-LIST" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND"
       :EXTERNALP NIL)
      (:NAME "VALUES-FORM" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND"
       :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND" :EXTERNALP
       NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "MULTIPLE-VALUE-&BIND" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND"
      :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "LAMBDA-LIST" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND"
       :EXTERNALP NIL)
      (:NAME "VALUES-FORM" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND"
       :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND" :EXTERNALP
       NIL))
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "MULTIPLE-VALUE-&BIND" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND"
     :EXTERNALP T)
    (:NAME "MULTIPLE-VALUE-BIND" :PACKAGE-NAME "ENHANCED-MULTIPLE-VALUE-BIND"
     :EXTERNALP T)))))
