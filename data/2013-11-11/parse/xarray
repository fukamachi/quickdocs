
(:TYPE :SYSTEM :NAME "xarray" :AUTHOR "Tamas K Papp" :MAINTAINER NIL :VERSION
 NIL :LICENCE "MIT" :DESCRIPTION "" :LONG-DESCRIPTION NIL :DEPENDS-ON
 ("CL-UTILITIES" "ITERATE" "METABANG-BIND" "ANAPHORA") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "XARRAY" :FULL-NAME "XARRAY" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :TYPE :SYMBOL
     (:NAME "FIXNUM-VECTOR" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL) :LAMBDA-LIST
     (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WITHIN-DIMENSION-P" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "SUBSCRIPT" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL)
      (:NAME "DIMENSION" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL))
     :DOCUMENTATION "Check if 0 <= subscript < dimension.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "VECTOR-WITHIN-DIMENSION-P" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "VECTOR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "DIMENSION" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return non-nil iff all elements in vector are in [0,dimension).")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ELEMENT-CONVERSION-FUNCTION" :PACKAGE-NAME "XARRAY" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     ((:NAME "SOURCE" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL)
      (:NAME "DESTINATION" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return a conversion function that is identity if (subtypep source
destination), or constructed with coerce otherwise.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "RM-INDEX" :PACKAGE-NAME "XARRAY" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "DIMENSIONS" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL)
      (:NAME "SUBSCRIPTS" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return a row-major flat index for given subscripts (coerced to a
vector, list also accepted), using dimensions (also coerced to a
vector, list also accepted).  Checks for boundaries and rank.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "RM-SUBSCRIPTS" :PACKAGE-NAME "XARRAY" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "DIMENSIONS" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL)
      (:NAME "I" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL))
     :DOCUMENTATION "Return i decomposed to a list of subscripts, taking
  dimensions (which is coerced to a vector) as a row-major indexing
  scheme.  No error checking is performed, meant for internal use.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CM-INDEX" :PACKAGE-NAME "XARRAY" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "DIMENSIONS" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL)
      (:NAME "SUBSCRIPTS" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL))
     :DOCUMENTATION
     "Calculate the column-major flat index from subscripts (list of
fixnums) and dimensions (list of fixnums).  Works in the corner case
when dimensions and subscripts are both nil.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CM-SUBSCRIPTS" :PACKAGE-NAME "XARRAY" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "DIMENSIONS" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL)
      (:NAME "I" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL))
     :DOCUMENTATION "Return the column-major subscripts (list) for flat index
i (fixnum), using dimensions (list of fixnums).  No error checking,
for internal use only.  Works in the corner case when dimension is
nil.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "VALID-PERMUTATION-P" :PACKAGE-NAME "XARRAY" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "VECTOR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
