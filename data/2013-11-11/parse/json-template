     ((:NAME "TOKENS" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PARSE-TOKENS" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
