     (:NAME "PARSE-TOKEN" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "TOKEN" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PARSE-RAW-TOKENS" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
