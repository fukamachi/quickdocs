     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "GETCONTEXT" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "CONTEXT" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
      (:NAME "KEY" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
      (:NAME "&AUX" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "RESULT" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
       (:NAME "CONTEXT" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "LISTIFY-KEY" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "KEY" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "START" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL) 0))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "LOOKUP-CONTEXT" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "CONTEXTS" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
