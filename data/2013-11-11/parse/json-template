    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ESCAPE-FOR-URI" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*TEMPLATE-FORMATTERS*" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP T)
     :DOCUMENTATION NIL :INITIAL-VALUE "`((,\"html\"
   ,@(JSON-TEMPLATE::MAKE-ESCAPER
      '((#\\< . \"&#60;\") (#\\> . \"&#62;\") (#\\& . \"&#38;\"))))
  (,\"html-attr-value\"
   ,@(JSON-TEMPLATE::MAKE-ESCAPER
      '((#\\< . \"&#60;\") (#\\> . \"&#62;\") (#\\& . \"&#38;\") (#\\' . \"&#39;\")
        (#\\\" . \"&#34;\"))))
  (\"url-param-value\" . JSON-TEMPLATE::ESCAPE-FOR-URI) (\"raw\" . IDENTITY))"))
   :EXTERNAL-SYMBOLS
   ((:NAME "*TEMPLATE-FORMATTERS*" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP T)
    (:NAME "PARSE-TEMPLATE-STRING" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP T)
    (:NAME "EXPAND-TEMPLATE" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP T)))))
