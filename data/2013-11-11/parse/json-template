
(:TYPE :SYSTEM :NAME "json-template" :AUTHOR "Matthias Andreas Benkard"
 :MAINTAINER NIL :VERSION "0.1.0" :LICENCE "Apache License, version 2.0"
 :DESCRIPTION "An implementation of a subset of the JSON Template language."
 :LONG-DESCRIPTION NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "JSON-TEMPLATE" :FULL-NAME "JSON-TEMPLATE"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :CLASS :SYMBOL
     (:NAME "TEMPLATE" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL)
     :SUPER-CLASS-LIST NIL :DOCUMENTATION NIL :SLOT-LIST
     ((:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "THINGS" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL) :ACCESSORS
       ((:NAME "TEMPLATE-THINGS" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP NIL))
       :READERS NIL :WRITERS NIL :DOCUMENTATION NIL)))
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TOKENIZE-TEMPLATE-STRING" :PACKAGE-NAME "JSON-TEMPLATE" :EXTERNALP
