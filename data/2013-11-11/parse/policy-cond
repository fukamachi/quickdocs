
(:TYPE :SYSTEM :NAME "policy-cond" :AUTHOR "Robert Smith <quad@symbo1ics.com>"
 :MAINTAINER "Robert Smith <quad@symbo1ics.com>" :VERSION NIL :LICENCE
 "Public Domain" :DESCRIPTION
 "A macro to insert code based on compiler policy." :LONG-DESCRIPTION NIL
 :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "POLICY-COND" :FULL-NAME "POLICY-COND" :DOCUMENTATION
   NIL :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "GET-POLICY" :PACKAGE-NAME "POLICY-COND" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "ENV" :PACKAGE-NAME "POLICY-COND" :EXTERNALP NIL))
     :DOCUMENTATION
     "Get the optimize policy information for the environment ENV.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "POLICY" :PACKAGE-NAME "POLICY-COND" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "EXPR" :PACKAGE-NAME "POLICY-COND" :EXTERNALP NIL)
      (:NAME "ENV" :PACKAGE-NAME "POLICY-COND" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "POLICY-IF" :PACKAGE-NAME "POLICY-COND" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "EXPR" :PACKAGE-NAME "POLICY-COND" :EXTERNALP NIL)
      (:NAME "THEN" :PACKAGE-NAME "POLICY-COND" :EXTERNALP NIL)
      (:NAME "ELSE" :PACKAGE-NAME "POLICY-COND" :EXTERNALP NIL)
      (:NAME "&ENVIRONMENT" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "ENV" :PACKAGE-NAME "POLICY-COND" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "POLICY-COND" :PACKAGE-NAME "POLICY-COND" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CASES" :PACKAGE-NAME "POLICY-COND" :EXTERNALP NIL))
     :DOCUMENTATION
     "Like COND, except each clause predicate is a policy expression. A
policy expression is a boolean expression using optimize declaration
qualities such as SPEED, SAFETY, DEBUG, COMPILATION-SPEED, etc. as if
they're lexically bound to their actual value.

The result of POLICY-COND will be the first clause whose policy
expression is satisfied. This is computed at compile time based off
the current compiler policy."))
   :EXTERNAL-SYMBOLS
   ((:NAME "POLICY-COND" :PACKAGE-NAME "POLICY-COND" :EXTERNALP T)))))
