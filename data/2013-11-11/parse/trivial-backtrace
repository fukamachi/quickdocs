     ((:NAME "PART-NAME" :PACKAGE-NAME "TRIVIAL-BACKTRACE" :EXTERNALP NIL)
      (:NAME "POSITION" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
