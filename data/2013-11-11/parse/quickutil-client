     (:NAME "SAVE-UTILS-AS" :PACKAGE-NAME "QUICKUTIL-CLIENT" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "FILENAME" :PACKAGE-NAME "QUICKUTIL-CLIENT" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "UTILITIES" :PACKAGE-NAME "QUICKUTIL-CLIENT" :EXTERNALP NIL)
