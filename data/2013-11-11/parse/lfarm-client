     ((:NAME "RESULT-TYPE" :PACKAGE-NAME "LFARM-CLIENT.COGNATE" :EXTERNALP NIL)
      (:NAME "TASK" :PACKAGE-NAME "LFARM-CLIENT.COGNATE" :EXTERNALP NIL)
