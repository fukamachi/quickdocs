
(:TYPE :SYSTEM :NAME "hunchentoot-vhost" :AUTHOR
 "Cyrus Harmon <ch-lisp@bobobeach.com>" :MAINTAINER NIL :VERSION "0.2" :LICENCE
 "BSD" :DESCRIPTION NIL :LONG-DESCRIPTION NIL :DEPENDS-ON ("HUNCHENTOOT")
 :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "HUNCHENTOOT-VHOST" :FULL-NAME "HUNCHENTOOT-VHOST"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :CLASS :SYMBOL
     (:NAME "VIRTUAL-HOST" :PACKAGE-NAME "HUNCHENTOOT-VHOST" :EXTERNALP T)
     :SUPER-CLASS-LIST NIL :DOCUMENTATION
     "An object of this class contains information about
  a virtual host to be handled by the hunchentoot-vhost machinery."
     :SLOT-LIST
     ((:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "NAME" :PACKAGE-NAME "HUNCHENTOOT-VHOST" :EXTERNALP NIL)
