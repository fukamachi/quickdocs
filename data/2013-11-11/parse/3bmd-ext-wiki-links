
(:TYPE :SYSTEM :NAME "3bmd-ext-wiki-links" :AUTHOR NIL :MAINTAINER NIL :VERSION
 NIL :LICENCE NIL :DESCRIPTION
 "example extension to 3bmd implementing simple wiki-style [[links]]"
 :LONG-DESCRIPTION NIL :DEPENDS-ON ("3BMD") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "3BMD-WIKI" :FULL-NAME "3BMD-WIKI" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*WIKI-LINKS*" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP T)
     :DOCUMENTATION NIL :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*WIKI-PROCESSOR*" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP T)
     :DOCUMENTATION
     "set to something PROCESS-WIKI-LINK etc will recognize to enable wiki link support in printer (see also *wiki-links* to enable wiki link parsing)"
     :INITIAL-VALUE "NIL")
    (:TYPE :GENERIC :SYMBOL
     (:NAME "PROCESS-WIKI-LINK" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "WIKI" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
      (:NAME "NORMALIZED-TARGET" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
      (:NAME "FORMATTED-TARGET" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
      (:NAME "ARGS" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "PROCESS-WIKI-LINK" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "W" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
       (:NAME "NULL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      (:NAME "NT" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
      (:NAME "FORMATTED" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
      (:NAME "A" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "PROCESS-WIKI-LINK" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "W" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
       (:NAME "NULL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      (:NAME "NT" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
      (:NAME "FORMATTED" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
      (:NAME "A" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "PRINT-TAGGED-ELEMENT" :PACKAGE-NAME "3BMD-EXT" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "TAG" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL)
       ((:NAME "EQL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        ((:NAME "QUOTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
         (:NAME "WIKI-LINK" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP NIL))))
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "*WIKI-LINKS*" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP T)
    (:NAME "*WIKI-PROCESSOR*" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP T)
    (:NAME "PROCESS-WIKI-LINK" :PACKAGE-NAME "3BMD-WIKI" :EXTERNALP T)))))
