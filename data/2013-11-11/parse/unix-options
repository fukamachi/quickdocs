    (:NAME "*DEFAULT-USAGE-FORMAT-STRING*" :PACKAGE-NAME "UNIX-OPTIONS"
     :EXTERNALP T)
    (:NAME "PRINT-USAGE-SUMMARY" :PACKAGE-NAME "UNIX-OPTIONS" :EXTERNALP T)
    (:NAME "&FREE" :PACKAGE-NAME "UNIX-OPTIONS" :EXTERNALP T)
    (:NAME "OPTION-SPEC" :PACKAGE-NAME "UNIX-OPTIONS" :EXTERNALP T)
    (:NAME "LONG-TOKENS" :PACKAGE-NAME "UNIX-OPTIONS" :EXTERNALP T)
    (:NAME "&PARAMETERS" :PACKAGE-NAME "UNIX-OPTIONS" :EXTERNALP T)
    (:NAME "CLI-OPTIONS" :PACKAGE-NAME "UNIX-OPTIONS" :EXTERNALP T)
    (:NAME "WITH-CLI-OPTIONS" :PACKAGE-NAME "UNIX-OPTIONS" :EXTERNALP T)
    (:NAME "EXE-NAME" :PACKAGE-NAME "UNIX-OPTIONS" :EXTERNALP T)))))
