
(:TYPE :SYSTEM :NAME "cl-autorepo" :AUTHOR
 "Bill St. Clair <bill@billstclair.com>" :MAINTAINER NIL :VERSION "1.0.0"
 :LICENCE "Apache" :DESCRIPTION
 "A simple mechanism to auto-load ASDF systems from repositories."
 :LONG-DESCRIPTION NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-AUTOREPO" :FULL-NAME "CL-AUTOREPO" :DOCUMENTATION
   NIL :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*REPO-DIR*" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "(MERGE-PATHNAMES \".local/share/common-lisp/source/\" (USER-HOMEDIR-PATHNAME))")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ADD-SYSTEM" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
      (:NAME "URL" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "REPOSITORY-TYPE" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
       (:NAME "GIT" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)))
     :DOCUMENTATION "Download the system named NAME of REPOSITORY-TYPE
from the URL, unless it's already defined.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "INITIALIZE-AUTOREPO-SOURCE-REGISTRY" :PACKAGE-NAME "CL-AUTOREPO"
      :EXTERNALP NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :GENERIC :SYMBOL
     (:NAME "DOWNLOAD-REPO" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "REPOSITORY-TYPE" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
      (:NAME "URL" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
      (:NAME "DIRECTORY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Download a repository of REPOSITORY-TYPE from URL, as a
subdirectory of DIRECTORY. REPOSITORY-TYPE can be :GIT, :SVN, :DARCS, or :HG")
    (:TYPE :METHOD :SYMBOL
     (:NAME "DOWNLOAD-REPO" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "REPOSITORY-TYPE" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
       ((:NAME "EQL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        (:NAME "GIT" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)))
      (:NAME "URL" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
      (:NAME "DIRECTORY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "DOWNLOAD-REPO" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "REPOSITORY-TYPE" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
       ((:NAME "EQL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        (:NAME "SVN" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)))
      (:NAME "URL" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
      (:NAME "DIRECTORY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "DOWNLOAD-REPO" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "REPOSITORY-TYPE" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
       ((:NAME "EQL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        (:NAME "DARCS" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)))
      (:NAME "URL" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
      (:NAME "DIRECTORY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "DOWNLOAD-REPO" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "REPOSITORY-TYPE" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
       ((:NAME "EQL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        (:NAME "HG" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)))
      (:NAME "URL" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP NIL)
      (:NAME "DIRECTORY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "ADD-SYSTEM" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP T)
    (:NAME "DOWNLOAD-REPO" :PACKAGE-NAME "CL-AUTOREPO" :EXTERNALP T)))))
