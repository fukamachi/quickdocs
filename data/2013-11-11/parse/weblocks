    (:TYPE :FUNCTION :SYMBOL
     (:NAME "RENDER-WT" :PACKAGE-NAME "WEBLOCKS-UTIL" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "TEMPLATE" :PACKAGE-NAME "WEBLOCKS" :EXTERNALP NIL)
