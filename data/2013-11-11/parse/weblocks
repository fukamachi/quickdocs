       ((:NAME "GEN-ID" :PACKAGE-NAME "WEBLOCKS-UTIL" :EXTERNALP T)))
      ((:NAME "BASE" :PACKAGE-NAME "WEBLOCKS" :EXTERNALP NIL) "")
      (:NAME "ORDERED-LIST-P" :PACKAGE-NAME "WEBLOCKS" :EXTERNALP NIL)
      ((:NAME "EMPTY-MESSAGE" :PACKAGE-NAME "WEBLOCKS" :EXTERNALP NIL)
       (:NAME "*MENU-EMPTY-MESSAGE*" :PACKAGE-NAME "WEBLOCKS" :EXTERNALP T))
      (:NAME "DISABLED-PANE-NAMES" :PACKAGE-NAME "WEBLOCKS" :EXTERNALP NIL)
      ((:NAME "MENU-TEMPLATE" :PACKAGE-NAME "WEBLOCKS" :EXTERNALP NIL)
