    (:TYPE :METHOD :SYMBOL
     (:NAME "SCAFFOLD-CLASS-NAME" :PACKAGE-NAME "WEBLOCKS" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "SCAFFOLD" :PACKAGE-NAME "WEBLOCKS" :EXTERNALP T))
