
(:TYPE :SYSTEM :NAME "weblocks" :AUTHOR "Slava Akhmechet" :MAINTAINER
 "Olexiy Zamkoviy, Scott L. Burson" :VERSION "0.10.7" :LICENCE "LLGPL"
 :DESCRIPTION "A Common Lisp web framework." :LONG-DESCRIPTION NIL :DEPENDS-ON
 ("WEBLOCKS-UTIL" "WEBLOCKS-STORES" "CLOSER-MOP" "HUNCHENTOOT" "PURI" "CL-JSON"
  "CL-WHO" "PARENSCRIPT" "CL-FAD" "OPTIMA" "CL-CONT" "METATILITIES" "CL-PPCRE"
  "ANAPHORA" "F-UNDERSCORE" "BORDEAUX-THREADS" "SALZA2" "HTML-TEMPLATE"
