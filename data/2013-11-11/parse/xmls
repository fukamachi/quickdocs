     NIL :INITIAL-VALUE
     "#((\"lt;\" #\\<) (\"gt;\" #\\>) (\"amp;\" #\\&) (\"apos;\" #\\') (\"quot;\" #\\\"))")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*WHITESPACE*" :PACKAGE-NAME "XMLS" :EXTERNALP NIL) :DOCUMENTATION
     NIL :INITIAL-VALUE
     "(REMOVE-DUPLICATES '(#\\Newline #\\  #\\Tab #\\Return #\\Newline))")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*CHAR-ESCAPES*" :PACKAGE-NAME "XMLS" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "(LET ((XMLS::TABLE (MAKE-ARRAY 256 :ELEMENT-TYPE 'STRING :INITIAL-ELEMENT \"\")))
  (DECLARE (TYPE VECTOR XMLS::*ENTITIES*))
  (LOOP XMLS::FOR XMLS::CODE XMLS::FROM 0 XMLS::TO 255
        XMLS::FOR CHAR = (CODE-CHAR XMLS::CODE)
        XMLS::FOR XMLS::ENTITY = (FIRST
                                  (FIND CHAR XMLS::*ENTITIES* :TEST #'CHAR=
                                        :KEY #'SECOND))
        DO (SETF (SVREF XMLS::TABLE XMLS::CODE)
                   (COND (XMLS::ENTITY (CONCATENATE 'STRING \"&\" XMLS::ENTITY))
                         ((AND (OR (< XMLS::CODE 32) (> XMLS::CODE 126))
                               (NOT (= XMLS::CODE 10)) (NOT (= XMLS::CODE 9)))
                          (FORMAT NIL \"&#x~x;\" XMLS::CODE))
                         (T (FORMAT NIL \"~x\" CHAR))))
        XMLS::FINALLY (RETURN XMLS::TABLE))
  XMLS::TABLE)")
