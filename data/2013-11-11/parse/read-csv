
(:TYPE :SYSTEM :NAME "read-csv" :AUTHOR
 "Warren Wilkinson <warrenwilkinson@gmail.com>" :MAINTAINER NIL :VERSION
 "1.0.0" :LICENCE "lgpl2" :DESCRIPTION
 "A library for reading CSV data from streams." :LONG-DESCRIPTION NIL
 :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "READ-CSV" :FULL-NAME "READ-CSV" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*RECORDS*" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :DOCUMENTATION
     NIL)
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*RECORD*" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :DOCUMENTATION
     NIL)
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*WHITE-CHAR-COUNT*" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL)
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "NOOP" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "C" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL)) :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ADDC" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "C" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL)) :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ADDL" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "C" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL)) :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "NEXT" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "C" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL)) :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "NEXT" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "C" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL)) :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SHIP" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "C" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL)) :DOCUMENTATION NIL)
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "DONE!" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :DOCUMENTATION NIL
     :INITIAL-VALUE "-1")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "START" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :DOCUMENTATION NIL
     :INITIAL-VALUE "0")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "RETUR" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :DOCUMENTATION NIL
     :INITIAL-VALUE "1")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "UNQUO" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :DOCUMENTATION NIL
     :INITIAL-VALUE "2")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "QUOTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :DOCUMENTATION
     NIL :INITIAL-VALUE "3")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "Q+RET" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :DOCUMENTATION NIL
     :INITIAL-VALUE "4")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "Q+QUO" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :DOCUMENTATION NIL
     :INITIAL-VALUE "5")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "Q+Q&W" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :DOCUMENTATION NIL
     :INITIAL-VALUE "6")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+CSV-TABLE+" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(IF (BOUNDP 'READ-CSV::+CSV-TABLE+)
    READ-CSV::+CSV-TABLE+
    (MAKE-ARRAY '(7 6 2) :INITIAL-CONTENTS
                `(((READ-CSV::NOOP ,READ-CSV::START)
                   (READ-CSV::SHIP ,READ-CSV::RETUR)
                   (READ-CSV::SHIP ,READ-CSV::DONE!) (READ-CSV::NOOP ,QUOTE)
                   (READ-CSV::NEXT ,READ-CSV::START)
                   (READ-CSV::ADDC ,READ-CSV::UNQUO))
                  ((READ-CSV::NOOP ,READ-CSV::START)
                   (READ-CSV::SHIP ,READ-CSV::RETUR)
                   (READ-CSV::NOOP ,READ-CSV::DONE!)
                   (READ-CSV::NOOP ,READ-CSV::START)
                   (READ-CSV::NEXT ,READ-CSV::START)
                   (READ-CSV::ADDC ,READ-CSV::UNQUO))
                  ((READ-CSV::ADDC ,READ-CSV::UNQUO)
                   (READ-CSV::SHIP ,READ-CSV::RETUR)
                   (READ-CSV::SHIP ,READ-CSV::DONE!)
                   (READ-CSV::ADDC ,READ-CSV::UNQUO)
                   (READ-CSV::NEXT ,READ-CSV::START)
                   (READ-CSV::ADDC ,READ-CSV::UNQUO))
                  ((READ-CSV::ADDC ,QUOTE) (READ-CSV::NOOP ,READ-CSV::Q+RET)
                   (READ-CSV::ADDL ,QUOTE) (READ-CSV::NOOP ,READ-CSV::Q+QUO)
                   (READ-CSV::ADDC ,QUOTE) (READ-CSV::ADDC ,QUOTE))
                  ((READ-CSV::ADDC ,QUOTE) (READ-CSV::NOOP ,READ-CSV::Q+RET)
                   (READ-CSV::ADDL ,QUOTE) (READ-CSV::NOOP ,READ-CSV::Q+QUO)
                   (READ-CSV::ADDC ,QUOTE) (READ-CSV::ADDC ,QUOTE))
                  ((READ-CSV::NOOP ,READ-CSV::Q+Q&W)
                   (READ-CSV::SHIP ,READ-CSV::RETUR)
                   (READ-CSV::SHIP ,READ-CSV::DONE!) (READ-CSV::ADDC ,QUOTE)
                   (READ-CSV::NEXT ,READ-CSV::START)
                   (READ-CSV::ADDC ,READ-CSV::UNQUO))
                  ((READ-CSV::NOOP ,READ-CSV::Q+Q&W)
                   (READ-CSV::SHIP ,READ-CSV::RETUR)
                   (READ-CSV::SHIP ,READ-CSV::DONE!) (READ-CSV::ADDC ,QUOTE)
                   (READ-CSV::NEXT ,READ-CSV::START)
                   (READ-CSV::ADDC ,READ-CSV::UNQUO)))))")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CHAR-CLASS" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "SEP" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL)
      (:NAME "CHAR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-CSV" :PACKAGE-NAME "READ-CSV" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "SEP" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) #\,)
      ((:NAME "EOF-ERROR-P" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL)
       (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      (:NAME "EOF-VALUE" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return CSV data and a second value that's true if row ended by EOF.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PARSE-CSV" :PACKAGE-NAME "READ-CSV" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "SEP" :PACKAGE-NAME "READ-CSV" :EXTERNALP NIL) #\,))
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "READ-CSV" :PACKAGE-NAME "READ-CSV" :EXTERNALP T)
    (:NAME "PARSE-CSV" :PACKAGE-NAME "READ-CSV" :EXTERNALP T)))))
