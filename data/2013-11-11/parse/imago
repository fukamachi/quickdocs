    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+CRC32-TABLE+" :PACKAGE-NAME "IMAGO" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "(LOOP IMAGO::WITH IMAGO::TABLE = (MAKE-ARRAY 256 :ELEMENT-TYPE
                                             '(UNSIGNED-BYTE 32))
      IMAGO::FOR IMAGO::I IMAGO::BELOW 256
      IMAGO::FOR IMAGO::C = IMAGO::I
      DO (LOOP IMAGO::FOR IMAGO::K IMAGO::BELOW 8
               DO (SETF IMAGO::C
                          (IF (= (LOGAND IMAGO::C 1) 1)
                              (LOGXOR (ASH IMAGO::C -1) 3988292384)
                              (ASH IMAGO::C -1)))) (SETF (AREF IMAGO::TABLE
                                                               IMAGO::I)
                                                           IMAGO::C)
      IMAGO::FINALLY (RETURN IMAGO::TABLE))")
