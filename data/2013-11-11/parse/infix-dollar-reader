
(:TYPE :SYSTEM :NAME "infix-dollar-reader" :AUTHOR "SUZUKI Shingo" :MAINTAINER
 "SUZUKI Shingo" :VERSION "0.3.0" :LICENCE "MIT" :DESCRIPTION
 "Infix $ operator for reducing redundant parenthesis" :LONG-DESCRIPTION NIL
 :DEPENDS-ON ("CL-SYNTAX") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "INFIX-DOLLAR-READER" :FULL-NAME "INFIX-DOLLAR-READER"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*ORIGINAL-READTABLE*" :PACKAGE-NAME "INFIX-DOLLAR-READER"
      :EXTERNALP NIL)
     :DOCUMENTATION "Saving original readtable" :INITIAL-VALUE "NIL")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "INFIX-DOLLAR-READER" :PACKAGE-NAME "INFIX-DOLLAR-READER"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CHAR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "USE-INFIX-DOLLAR" :PACKAGE-NAME "INFIX-DOLLAR-READER" :EXTERNALP
      T)
     :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "PRINT-NOTE" :PACKAGE-NAME "INFIX-DOLLAR-READER" :EXTERNALP NIL))
     :DOCUMENTATION "Enable infix $ operator.
CAUTION: enable-infix-dollar could modify *readtable* globally.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "UNUSE-INFIX-DOLLAR" :PACKAGE-NAME "INFIX-DOLLAR-READER" :EXTERNALP
      T)
     :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "PRINT-NOTE" :PACKAGE-NAME "INFIX-DOLLAR-READER" :EXTERNALP NIL))
     :DOCUMENTATION "Restore whole *readtable*.
CAUTION: unuse-infix-dollar could rollback all of the changes on *readtable*
after invoking enable-infix-dollar."))
   :EXTERNAL-SYMBOLS
   ((:NAME "UNUSE-INFIX-DOLLAR" :PACKAGE-NAME "INFIX-DOLLAR-READER" :EXTERNALP
     T)
    (:NAME "USE-INFIX-DOLLAR" :PACKAGE-NAME "INFIX-DOLLAR-READER" :EXTERNALP
     T)))))
