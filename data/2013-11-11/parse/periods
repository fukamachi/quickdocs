    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-DURATION" :PACKAGE-NAME "PERIODS" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (((:NAME "YEARS" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "DUM745" :PACKAGE-NAME NIL :EXTERNALP NIL))
       0)
      (((:NAME "MONTHS" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "DUM746" :PACKAGE-NAME NIL :EXTERNALP NIL))
       0)
      (((:NAME "DAYS" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "DUM747" :PACKAGE-NAME NIL :EXTERNALP NIL))
       0)
      (((:NAME "HOURS" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "DUM748" :PACKAGE-NAME NIL :EXTERNALP NIL))
       0)
      (((:NAME "MINUTES" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "DUM749" :PACKAGE-NAME NIL :EXTERNALP NIL))
       0)
      (((:NAME "SECONDS" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "DUM750" :PACKAGE-NAME NIL :EXTERNALP NIL))
       0)
      (((:NAME "MILLISECONDS" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "DUM751" :PACKAGE-NAME NIL :EXTERNALP NIL))
       0)
      (((:NAME "MICROSECONDS" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "DUM752" :PACKAGE-NAME NIL :EXTERNALP NIL))
       0)
      (((:NAME "NANOSECONDS" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "DUM753" :PACKAGE-NAME NIL :EXTERNALP NIL))
       0))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "DURATION" :PACKAGE-NAME "PERIODS" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "ARGS" :PACKAGE-NAME "PERIODS" :EXTERNALP NIL))
     :DOCUMENTATION "Create a DURATION object.

  One thing to note about duration: there is no way to determine the total
length of a duration in terms of any specific time quantity, without first
binding that duration to a fixed point in time (after all, how many days are
in a month if you don't know which month it is?)  Therefore, those looking for
a function like \"duration-seconds\" are really wanting to work with ranges,
not just durations.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "WITH-SKIPPERS" :PACKAGE-NAME "PERIODS" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "PERIODS" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ADD-TIME" :PACKAGE-NAME "PERIODS" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "FIXED-TIME" :PACKAGE-NAME "PERIODS" :EXTERNALP T)
      (:NAME "DURATION" :PACKAGE-NAME "PERIODS" :EXTERNALP T)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "REVERSE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Given a FIXED-TIME, add the supplied DURATION.

  Example (reader notation requires calling LOCAL-TIME:ENABLE-READ-MACROS):

  (add-time @2007-05-20T12:10:10.000 (duration :hours 50))
    => @2007-05-22T14:10:10.000

  NOTE: This function always adds the largest increments first, so:

  (add-time @2003-01-09 (duration :years 1 :days 20)) => @2004-02-29

If days has been added before years, the result would have been
\"@2004-03-01\".")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SUBTRACT-TIME" :PACKAGE-NAME "PERIODS" :EXTERNALP T) :LAMBDA-LIST
