      ((:NAME "REVERSE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      ((:NAME "INCLUSIVE-P" :PACKAGE-NAME "PERIODS" :EXTERNALP NIL)
       (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Return a list of all times within the given range.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "DO-RELATIVE-TIMES" :PACKAGE-NAME "PERIODS" :EXTERNALP NIL)
     :LAMBDA-LIST
     (((:NAME "VAR" :PACKAGE-NAME "PERIODS" :EXTERNALP NIL)
       (:NAME "ANCHOR" :PACKAGE-NAME "PERIODS" :EXTERNALP NIL)
       (:NAME "RELATIVE-TIME" :PACKAGE-NAME "PERIODS" :EXTERNALP NIL)
       (:NAME "END" :PACKAGE-NAME "PERIODS" :EXTERNALP NIL)
       (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       ((:NAME "RESULT" :PACKAGE-NAME "PERIODS" :EXTERNALP NIL)
        (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "PERIODS" :EXTERNALP NIL))
     :DOCUMENTATION
     "A 'do' style version of the functional MAP-RELATIVE-TIMES macro.

  The disadvantage to DO-RELATIVE-TIMES is that there is no way to ask for a
reversed time sequence, or specify an inclusive endpoint.")
    (:TYPE :FUNCTION :SYMBOL
