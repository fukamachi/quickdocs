    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*NEWLINE-ELEMENTS*" :PACKAGE-NAME
      "COM.INFORMATIMAGO.COMMON-LISP.HTML-GENERATOR.HTML" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "'(COM.INFORMATIMAGO.COMMON-LISP.HTML-GENERATOR.HTML:P COM.INFORMATIMAGO.COMMON-LISP.HTML-GENERATOR.HTML:H1
   COM.INFORMATIMAGO.COMMON-LISP.HTML-GENERATOR.HTML:H2
   COM.INFORMATIMAGO.COMMON-LISP.HTML-GENERATOR.HTML:H3
   COM.INFORMATIMAGO.COMMON-LISP.HTML-GENERATOR.HTML:H4
   COM.INFORMATIMAGO.COMMON-LISP.HTML-GENERATOR.HTML:H5
   COM.INFORMATIMAGO.COMMON-LISP.HTML-GENERATOR.HTML:H6
   COM.INFORMATIMAGO.COMMON-LISP.HTML-GENERATOR.HTML:UL
   COM.INFORMATIMAGO.COMMON-LISP.HTML-GENERATOR.HTML:OL
