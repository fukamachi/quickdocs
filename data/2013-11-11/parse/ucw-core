     :LAMBDA-LIST
     ((:NAME "SERVER" :PACKAGE-NAME "IT.BESE.UCW.CORE" :EXTERNALP NIL)
      (:NAME "APPLICTAION" :PACKAGE-NAME "IT.BESE.UCW.CORE" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :GENERIC :SYMBOL
     (:NAME "UNREGISTER-APPLICATION" :PACKAGE-NAME "IT.BESE.UCW.CORE"
