     (:NAME "*LDAP-FILTER-PARSER*" :PACKAGE-NAME "TRIVIAL-LDAP" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "'#S(YACC::PARSER
    :STATES 20
    :GOTO #(((TRIVIAL-LDAP::FILTER . 1) (TRIVIAL-LDAP::ITEM . 10)
             (TRIVIAL-LDAP::SIMPLE . 11))
            NIL
            ((TRIVIAL-LDAP::FILTERCOMP . 3) (TRIVIAL-LDAP::ITEM . 5)
             (TRIVIAL-LDAP::SIMPLE . 11))
            NIL NIL NIL
            ((TRIVIAL-LDAP::FILTERLIST . 7) (TRIVIAL-LDAP::FILTER . 8)
             (TRIVIAL-LDAP::ITEM . 10) (TRIVIAL-LDAP::SIMPLE . 11))
            NIL
            ((TRIVIAL-LDAP::FILTERLIST . 9) (TRIVIAL-LDAP::FILTER . 8)
             (TRIVIAL-LDAP::ITEM . 10) (TRIVIAL-LDAP::SIMPLE . 11))
            NIL NIL NIL NIL ((TRIVIAL-LDAP::VALUE . 14)) NIL NIL
            ((TRIVIAL-LDAP::FILTERLIST . 17) (TRIVIAL-LDAP::FILTER . 8)
             (TRIVIAL-LDAP::ITEM . 10) (TRIVIAL-LDAP::SIMPLE . 11))
            NIL
            ((TRIVIAL-LDAP::FILTER . 19) (TRIVIAL-LDAP::ITEM . 10)
             (TRIVIAL-LDAP::SIMPLE . 11))
            NIL)
    :ACTION #(((TRIVIAL-LDAP::LPAR . #<YACC::SHIFT-ACTION 2>)
               (TRIVIAL-LDAP::ATTR . #<YACC::SHIFT-ACTION 12>))
              ((YACC::YACC-EOF-SYMBOL . #<YACC::ACCEPT-ACTION >))
              ((TRIVIAL-LDAP::ATTR . #<YACC::SHIFT-ACTION 12>)
               (AND . #<YACC::SHIFT-ACTION 6>) (OR . #<YACC::SHIFT-ACTION 16>)
               (NOT . #<YACC::SHIFT-ACTION 18>))
              ((TRIVIAL-LDAP::RPAR . #<YACC::SHIFT-ACTION 4>))
              ((YACC::YACC-EOF-SYMBOL
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::FILTER (3)>)
               (TRIVIAL-LDAP::RPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::FILTER (3)>)
               (TRIVIAL-LDAP::ATTR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::FILTER (3)>)
               (TRIVIAL-LDAP::LPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::FILTER (3)>))
              ((TRIVIAL-LDAP::RPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::FILTERCOMP (1)>))
              ((TRIVIAL-LDAP::ATTR . #<YACC::SHIFT-ACTION 12>)
               (TRIVIAL-LDAP::LPAR . #<YACC::SHIFT-ACTION 2>))
              ((TRIVIAL-LDAP::RPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::FILTERCOMP (2)>))
              ((TRIVIAL-LDAP::ATTR . #<YACC::SHIFT-ACTION 12>)
               (TRIVIAL-LDAP::LPAR . #<YACC::SHIFT-ACTION 2>)
               (TRIVIAL-LDAP::RPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::FILTERLIST (1)>))
              ((TRIVIAL-LDAP::RPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::FILTERLIST (2)>))
              ((YACC::YACC-EOF-SYMBOL
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::FILTER (1)>)
               (TRIVIAL-LDAP::RPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::FILTER (1)>)
               (TRIVIAL-LDAP::ATTR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::FILTER (1)>)
               (TRIVIAL-LDAP::LPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::FILTER (1)>))
              ((YACC::YACC-EOF-SYMBOL
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::ITEM (1)>)
               (TRIVIAL-LDAP::RPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::ITEM (1)>)
               (TRIVIAL-LDAP::ATTR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::ITEM (1)>)
               (TRIVIAL-LDAP::LPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::ITEM (1)>))
              ((TRIVIAL-LDAP::FILTERTYPE . #<YACC::SHIFT-ACTION 13>))
              ((STRING . #<YACC::SHIFT-ACTION 15>))
              ((YACC::YACC-EOF-SYMBOL
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::SIMPLE (3)>)
               (TRIVIAL-LDAP::RPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::SIMPLE (3)>)
               (TRIVIAL-LDAP::ATTR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::SIMPLE (3)>)
               (TRIVIAL-LDAP::LPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::SIMPLE (3)>))
              ((YACC::YACC-EOF-SYMBOL
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::VALUE (1)>)
               (TRIVIAL-LDAP::RPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::VALUE (1)>)
               (TRIVIAL-LDAP::ATTR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::VALUE (1)>)
               (TRIVIAL-LDAP::LPAR
                . #<YACC::REDUCE-ACTION TRIVIAL-LDAP::VALUE (1)>))
