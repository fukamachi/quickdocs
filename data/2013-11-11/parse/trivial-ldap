      (10 TRIVIAL-LDAP:REFERRAL \"Referral\")
      (11 TRIVIAL-LDAP:ADMINLIMITEXCEEDED \"Admin Limit Exceeded\")
      (12 TRIVIAL-LDAP:UNAVAILABLECRITICALEXTENSION
       \"Unavailable Critical Extension\")
      (13 TRIVIAL-LDAP:CONFIDENTIALITYREQUIRED \"Confidentiality Required\")
      (14 TRIVIAL-LDAP:SASLBINDINPROGRESS \"SASL Bind In Progress\")
      (16 TRIVIAL-LDAP:NOSUCHATTRIBUTE \"No Such Attribute\")
      (17 TRIVIAL-LDAP:UNDEFINEDATTRIBUTETYPE \"Undefined Attribute Type\")
      (18 TRIVIAL-LDAP:INAPPROPRIATEMATCHING \"Inappropriate Matching\")
      (19 TRIVIAL-LDAP:CONSTRAINTVIOLATION \"Constraint Violation\")
      (20 TRIVIAL-LDAP:ATTRIBUTEORVALUEEXISTS \"Attribute Or Value Exists\")
      (21 TRIVIAL-LDAP:INVALIDATTRIBUTESYNTAX \"Invalid Attribute Syntax\")
      (32 TRIVIAL-LDAP:NOSUCHOBJECT \"No Such Object\")
      (33 TRIVIAL-LDAP:ALIASPROBLEM \"Alias Problem\")
      (34 TRIVIAL-LDAP:INVALIDDNSYNTAX \"Invalid DN Syntax\")
      (36 TRIVIAL-LDAP:ALIASDEREFERENCINGPROBLEM \"Alias Dereferencing Problem\")
      (48 TRIVIAL-LDAP:INAPPROPRIATEAUTHENTICATION
       \"Inappropriate Authentication\")
      (49 TRIVIAL-LDAP:INVALIDCREDENTIALS \"Invalid Credentials\")
      (50 TRIVIAL-LDAP:INSUFFICIENTACCESSRIGHTS \"Insufficient Access Rights\")
      (51 TRIVIAL-LDAP:BUSY \"Busy\") (52 TRIVIAL-LDAP:UNAVAILABLE \"Unavailable\")
      (53 TRIVIAL-LDAP:UNWILLINGTOPERFORM \"Unwilling To Perform\")
      (54 TRIVIAL-LDAP:LOOPDETECT \"Loop Detect\")
      (64 TRIVIAL-LDAP:NAMINGVIOLATION \"Naming Violation\")
      (65 TRIVIAL-LDAP:OBJECTCLASSVIOLATION \"Object Class Violation\")
      (66 TRIVIAL-LDAP:NOTALLOWEDONLEAF \"Not Allowed On Leaf\")
