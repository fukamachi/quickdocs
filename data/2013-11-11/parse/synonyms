
(:TYPE :SYSTEM :NAME "synonyms" :AUTHOR "Robert Smith" :MAINTAINER NIL :VERSION
 NIL :LICENCE "Public Domain" :DESCRIPTION
 "A package containing useful synonyms to Common Lisp functionalities."
 :LONG-DESCRIPTION NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "SYNONYMS" :FULL-NAME "SYNONYMS" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :CONSTANT :SYMBOL
     (:NAME "TRUE" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T) :DOCUMENTATION
     "The canonical truth value." :INITIAL-VALUE "T")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "FALSE" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T) :DOCUMENTATION
     "The false value." :INITIAL-VALUE "NIL")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "POSITIVEP" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "N" :PACKAGE-NAME "SYNONYMS" :EXTERNALP NIL)) :DOCUMENTATION
     "Check if a number N is positive.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "NEGATIVEP" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "N" :PACKAGE-NAME "SYNONYMS" :EXTERNALP NIL)) :DOCUMENTATION
     "Check if a number N is negative.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "NON-POSITIVE-P" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "N" :PACKAGE-NAME "SYNONYMS" :EXTERNALP NIL))
     :DOCUMENTATION
     "Check if a number N is non-positive (less than or equal to zero).")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "NON-NEGATIVE-P" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "N" :PACKAGE-NAME "SYNONYMS" :EXTERNALP NIL))
     :DOCUMENTATION
     "Check if a number N is non-negative (greater or equal to zero).")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "NON-ZERO-P" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "N" :PACKAGE-NAME "SYNONYMS" :EXTERNALP NIL)) :DOCUMENTATION
     "Check if a number is non-zero.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "HEAD" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "LIST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)) :DOCUMENTATION
     "Return the head of a list.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TAIL" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "LIST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)) :DOCUMENTATION
     "Return the tail (rest) of the list.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "STRCAT" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "A" :PACKAGE-NAME "SYNONYMS" :EXTERNALP NIL)
      (:NAME "B" :PACKAGE-NAME "SYNONYMS" :EXTERNALP NIL))
     :DOCUMENTATION "Concatenate two strings A and B.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "YES" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "IGNORED" :PACKAGE-NAME "SYNONYMS" :EXTERNALP NIL))
     :DOCUMENTATION "A function that always returns a truth vale. Equivalent
to (CONSTANTLY T).")
    (:TYPE :FUNCTION :SYMBOL (:NAME "NO" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "IGNORED" :PACKAGE-NAME "SYNONYMS" :EXTERNALP NIL))
     :DOCUMENTATION "A function that akways returns a false value. Equivalent
to (CONSTANTLY NIL)."))
   :EXTERNAL-SYMBOLS
   ((:NAME "NON-ZERO-P" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
    (:NAME "TAIL" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
    (:NAME "NON-NEGATIVE-P" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
    (:NAME "STRCAT" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
    (:NAME "TRUE" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
    (:NAME "POSITIVEP" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
    (:NAME "NON-POSITIVE-P" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
    (:NAME "FALSE" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
    (:NAME "HEAD" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
    (:NAME "YES" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
    (:NAME "NO" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)
    (:NAME "NEGATIVEP" :PACKAGE-NAME "SYNONYMS" :EXTERNALP T)))))
