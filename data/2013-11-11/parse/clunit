     (:NAME "RERUN-FAILED-TESTS" :PACKAGE-NAME "CLUNIT" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
