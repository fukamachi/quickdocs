    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*CLUNIT-REPORT-FORMAT*" :PACKAGE-NAME "CLUNIT" :EXTERNALP T)
     :DOCUMENTATION
     "Controls the output format of the unit test results. Possible values are :default, :tap or NIL."
     :INITIAL-VALUE ":DEFAULT")
