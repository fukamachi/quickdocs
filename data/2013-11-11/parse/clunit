    (:TYPE :METHOD :SYMBOL
     (:NAME "PRINT-FORMAT" :PACKAGE-NAME "CLUNIT" :EXTERNALP NIL) :LAMBDA-LIST
     (((:NAME "CONDITION" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "ASSERTION-FAILED" :PACKAGE-NAME "CLUNIT" :EXTERNALP NIL))
