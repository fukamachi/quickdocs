     ((:NAME "TEST" :PACKAGE-NAME "CLUNIT" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "USE-DEBUGGER" :PACKAGE-NAME "CLUNIT" :EXTERNALP NIL)
      ((:NAME "REPORT-PROGRESS" :PACKAGE-NAME "CLUNIT" :EXTERNALP NIL)
