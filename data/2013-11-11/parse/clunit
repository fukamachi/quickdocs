     (:NAME "*LAST-CLUNIT-REPORT*" :PACKAGE-NAME "CLUNIT" :EXTERNALP NIL)
     :DOCUMENTATION
     "Holds the last returned CLUNIT-REPORT object. Default optional argument to the function RERUN-FAILED-TESTS."
     :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*CLUNIT-EQUALITY-TEST*" :PACKAGE-NAME "CLUNIT" :EXTERNALP T)
     :DOCUMENTATION
     "The equality test used to compare values by the ASSERT-EQUALITY macro."
     :INITIAL-VALUE "#'EQUALP")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*CLUNIT-TEST-REPORT*" :PACKAGE-NAME "CLUNIT" :EXTERNALP NIL)
     :DOCUMENTATION
