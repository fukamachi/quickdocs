       (:NAME "READ-QUEUE" :PACKAGE-NAME "CLWS" :EXTERNALP NIL) :ACCESSORS NIL
       :READERS
       ((:NAME "RESOURCE-READ-QUEUE" :PACKAGE-NAME "CLWS" :EXTERNALP NIL))
