
(:TYPE :SYSTEM :NAME "cl-syslog" :AUTHOR "Erik Enge" :MAINTAINER NIL :VERSION
 "0.1.0" :LICENCE "MIT" :DESCRIPTION "Common Lisp syslog interface"
 :LONG-DESCRIPTION NIL :DEPENDS-ON ("CFFI") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-SYSLOG" :FULL-NAME "CL-SYSLOG" :DOCUMENTATION
   "Common Lisp interface to syslog." :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*PRIORITIES*" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "'((:EMERG . 0) (:ALERT . 1) (:CRIT . 2) (:ERR . 3) (:WARNING . 4) (:NOTICE . 5)
  (:INFO . 6) (:DEBUG . 7))")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*FACILITIES*" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "'((:KERN . 0) (:USER . 1) (:MAIL . 2) (:DAEMON . 3) (:AUTH . 4) (:SYSLOG . 5)
  (:LPR . 6) (:NEWS . 7) (:UUCP . 8) (:CRON . 9) (:AUTHPRIV . 10) (:FTP . 11)
  (:LOCAL0 . 16) (:LOCAL1 . 17) (:LOCAL2 . 18) (:LOCAL3 . 19) (:LOCAL4 . 20)
  (:LOCAL5 . 21) (:LOCAL6 . 22) (:LOCAL7 . 23))")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+LOG-PID+" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T) :DOCUMENTATION
     "Log the pid with each message." :INITIAL-VALUE "1")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+LOG-CONS+" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T) :DOCUMENTATION
     "Log on the console if errors in sending." :INITIAL-VALUE "2")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+LOG-ODELAY+" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)
     :DOCUMENTATION "Delay open until first syslog() (default)." :INITIAL-VALUE
     "4")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+LOG-NDELAY+" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)
     :DOCUMENTATION "Don't delay open." :INITIAL-VALUE "8")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+LOG-NOWAIT+" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)
     :DOCUMENTATION "Don't wait for console forks: deprecated." :INITIAL-VALUE
     "16")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+LOG-PERROR+" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)
     :DOCUMENTATION "Log to stderr as well." :INITIAL-VALUE "32")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "OPENLOG" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "IDENT" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL)
      (:NAME "OPTION" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL)
      (:NAME "FACILITY" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CLOSELOG" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL) :LAMBDA-LIST
     (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SYSLOG" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "PRIORITY" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL)
      (:NAME "FORMAT" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "GET-FACILITY" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "FACILITY-NAME" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return facility number given the facility's name.  If there is no
such facility, signal `invalid-facility' error.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "GET-PRIORITY" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "PRIORITY-NAME" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return priority number given the priority's name.  If there is no
such priority, signal `invalid-priority' error.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "LOG" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL)
      (:NAME "FACILITY" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL)
      (:NAME "PRIORITY" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL)
      (:NAME "TEXT" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "OPTION" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP NIL) 0))
     :DOCUMENTATION "Print message to syslog.
'option' can be any of the +log...+ constants"))
   :EXTERNAL-SYMBOLS
   ((:NAME "+LOG-ODELAY+" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)
    (:NAME "LOG" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)
    (:NAME "+LOG-CONS+" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)
    (:NAME "+LOG-PID+" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)
    (:NAME "GET-PRIORITY" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)
    (:NAME "GET-FACILITY" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)
    (:NAME "+LOG-NDELAY+" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)
    (:NAME "+LOG-NOWAIT+" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)
    (:NAME "+LOG-PERROR+" :PACKAGE-NAME "CL-SYSLOG" :EXTERNALP T)))))
