
(:TYPE :SYSTEM :NAME "stumpwm" :AUTHOR "Shawn Betts <sabetts@vcn.bc.ca>"
 :MAINTAINER "Shawn Betts <sabetts@vcn.bc.ca>" :VERSION "0.9.7" :LICENCE NIL
 :DESCRIPTION "A tiling, keyboard driven window manager" :LONG-DESCRIPTION NIL
 :DEPENDS-ON ("CL-PPCRE" "CLX" "SB-POSIX") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "STUMPWM" :FULL-NAME "STUMPWM" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*SUPPRESS-ABORT-MESSAGES*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "Suppress abort message when non-nil." :INITIAL-VALUE
     "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*TIMEOUT-WAIT*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "Specifies, in seconds, how long a message will appear for. This must
be an integer."
     :INITIAL-VALUE "5")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*TIMEOUT-FRAME-INDICATOR-WAIT*" :PACKAGE-NAME "STUMPWM" :EXTERNALP
      T)
     :DOCUMENTATION "The amount of time a frame indicator timeout takes."
     :INITIAL-VALUE "1")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*FRAME-INDICATOR-TIMER*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "Keep track of the timer that hides the frame indicator."
     :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*FRAME-INDICATOR-TEXT*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "What appears in the frame indicator window?"
     :INITIAL-VALUE "\" Current Frame \"")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*SUPPRESS-FRAME-INDICATOR*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "Set this to T if you never want to see the frame indicator."
     :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*MESSAGE-WINDOW-TIMER*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "Keep track of the timer that hides the message window."
     :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*GRAB-POINTER-COUNT*" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :DOCUMENTATION "The number of times the pointer has been grabbed"
     :INITIAL-VALUE "0")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*COMMAND-MODE-START-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "A hook called whenever command mode is started"
     :INITIAL-VALUE "'(STUMPWM:COMMAND-MODE-START-MESSAGE)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*COMMAND-MODE-END-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "A hook called whenever command mode is ended"
     :INITIAL-VALUE "'(STUMPWM:COMMAND-MODE-END-MESSAGE)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*URGENT-WINDOW-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "A hook called whenever a window sets the property indicating that
  it demands the user's attention"
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*MAP-WINDOW-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :DOCUMENTATION "A hook called whenever a window is mapped." :INITIAL-VALUE
     "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*UNMAP-WINDOW-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :DOCUMENTATION "A hook called whenever a window is withdrawn."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*NEW-WINDOW-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "A hook called whenever a window is added to the window list. This
includes a genuinely new window as well as bringing a withdrawn window
back into the window list."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*DESTROY-WINDOW-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "A hook called whenever a window is destroyed or withdrawn."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*FOCUS-WINDOW-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "A hook called when a window is given focus. It is called with 2
arguments: the current window and the last window (could be nil)."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*PLACE-WINDOW-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "A hook called whenever a window is placed by rule. Arguments are
window group and frame"
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*START-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T) :DOCUMENTATION
     "A hook called when stumpwm starts." :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*INTERNAL-LOOP-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "A hook called inside stumpwm's inner loop." :INITIAL-VALUE
     "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*FOCUS-FRAME-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "A hook called when a frame is given focus. The hook functions are
called with 2 arguments: the current frame and the last frame."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*NEW-FRAME-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "A hook called when a new frame is created. the hook is called with
the frame as an argument."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*SPLIT-FRAME-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "A hook called when a frame is split. the hook is called with
the old frame (window is removed), and two new frames as arguments."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*MESSAGE-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "A hook called whenever stumpwm displays a message. The hook
function is passed any number of arguments. Each argument is a
line of text."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*TOP-LEVEL-ERROR-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "Called when a top level error occurs. Note that this hook is
run before the error is dealt with according to
*top-level-error-action*."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*FOCUS-GROUP-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "A hook called whenever stumpwm switches groups. It is called with 2 arguments: the current group and the last group."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*KEY-PRESS-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "A hook called whenever a key under *top-map* is pressed.
It is called with 3 argument: the key, the (possibly incomplete) key
sequence it is a part of, and command value bound to the key."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*ROOT-CLICK-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "A hook called whenever there is a mouse click on the root
window. Called with 4 arguments, the screen containing the root
window, the button clicked, and the x and y of the pointer."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*MODE-LINE-CLICK-HOOK*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "Called whenever the mode-line is clicked. It is called with 4 arguments,
the mode-line, the button clicked, and the x and y of the pointer."
     :INITIAL-VALUE "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*DISPLAY*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T) :DOCUMENTATION
     "The display for the X server" :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*SHELL-PROGRAM*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "The shell program used by @code{run-shell-command}."
     :INITIAL-VALUE "\"/bin/sh\"")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*MAXSIZE-BORDER-WIDTH*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "The width in pixels given to the borders of windows with maxsize or ratio hints."
     :INITIAL-VALUE "1")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*TRANSIENT-BORDER-WIDTH*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "The width in pixels given to the borders of transient or pop-up windows."
     :INITIAL-VALUE "1")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*NORMAL-BORDER-WIDTH*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "The width in pixels given to the borders of regular windows."
     :INITIAL-VALUE "1")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*TEXT-COLOR*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T) :DOCUMENTATION
     "The color of message text." :INITIAL-VALUE "\"white\"")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*MENU-MAXIMUM-HEIGHT*" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :DOCUMENTATION
     "Defines the maxium number of lines to display in the menu before enabling
   scrolling. If NIL scrolling is disabled."
     :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*MENU-SCROLLING-STEP*" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :DOCUMENTATION
     "Number of lines to scroll when hitting the menu list limit."
     :INITIAL-VALUE "1")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+NETWM-SUPPORTED+" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :DOCUMENTATION "Supported NETWM properties.
Window types are in +WINDOW-TYPES+."
     :INITIAL-VALUE
     "'(:_NET_SUPPORTING_WM_CHECK :_NET_NUMBER_OF_DESKTOPS :_NET_DESKTOP_GEOMETRY
  :_NET_DESKTOP_VIEWPORT :_NET_CURRENT_DESKTOP :_NET_WM_WINDOW_TYPE
  :_NET_WM_STATE :_NET_WM_STATE_MODAL :_NET_WM_ALLOWED_ACTIONS
  :_NET_WM_STATE_FULLSCREEN :_NET_WM_STATE_HIDDEN
  :_NET_WM_STATE_DEMANDS_ATTENTION :_NET_WM_FULL_WINDOW_PLACEMENT
  :_NET_CLOSE_WINDOW :_NET_CLIENT_LIST :_NET_CLIENT_LIST_STACKING
  :_NET_ACTIVE_WINDOW :_NET_WM_DESKTOP :_KDE_NET_SYSTEM_TRAY_WINDOW_FOR)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+NETWM-ALLOWED-ACTIONS+" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :DOCUMENTATION "Allowed NETWM actions for managed windows" :INITIAL-VALUE
     "'(:_NET_WM_ACTION_CHANGE_DESKTOP :_NET_WM_ACTION_FULLSCREEN
  :_NET_WM_ACTION_CLOSE)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+NETWM-WINDOW-TYPES+" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :DOCUMENTATION "Alist mapping NETWM window types to keywords.
Include only those we are ready to support."
     :INITIAL-VALUE
     "'((:_NET_WM_WINDOW_TYPE_DOCK . :DOCK) (:_NET_WM_WINDOW_TYPE_DIALOG . :DIALOG)
  (:_NET_WM_WINDOW_TYPE_NORMAL . :NORMAL))")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+WITHDRAWN-STATE+" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "0")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+NORMAL-STATE+" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "1")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+ICONIC-STATE+" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "3")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*WINDOW-EVENTS*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "The events to listen for on managed windows."
     :INITIAL-VALUE
     "'(:STRUCTURE-NOTIFY :PROPERTY-CHANGE :COLORMAP-CHANGE :FOCUS-CHANGE
  :ENTER-WINDOW)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*WINDOW-PARENT-EVENTS*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "The events to listen for on managed windows' parents."
     :INITIAL-VALUE "'(:SUBSTRUCTURE-NOTIFY :SUBSTRUCTURE-REDIRECT)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*MESSAGE-WINDOW-PADDING*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "The number of pixels that pad the text in the message window."
     :INITIAL-VALUE "5")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*MESSAGE-WINDOW-GRAVITY*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "This variable controls where the message window appears. The follow
are valid values.
@table @asis
@item :top-left
@item :top-right
@item :bottom-left
@item :bottom-right
@item :center
@end table"
     :INITIAL-VALUE ":TOP-RIGHT")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*EDITOR-BINDINGS*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION "A list of key-bindings for line editing." :INITIAL-VALUE
     "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*INPUT-WINDOW-GRAVITY*" :PACKAGE-NAME "STUMPWM" :EXTERNALP T)
     :DOCUMENTATION
     "This variable controls where the input window appears. The follow
are valid values.
@table @asis
@item :top-left
@item :top-right
@item :bottom-left
@item :bottom-right
@item :center
@end table"
     :INITIAL-VALUE ":TOP-RIGHT")
