    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SELECT-FROM-MENU" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "SCREEN" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
      (:NAME "TABLE" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
