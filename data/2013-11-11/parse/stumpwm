     (:NAME "FIND-MATCHING-WINDOWS" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "PROPS" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
      (:NAME "ALL-GROUPS" :PACKAGE-NAME "STUMPWM" :EXTERNALP NIL)
