 ((:TYPE :PACKAGE :NAME "S-BASE64" :FULL-NAME "S-BASE64" :DOCUMENTATION
   "An implementation of standard Base64 encoding and decoding" :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "+BASE64-ALPHABET+" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "\"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/\"")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+INVERSE-BASE64-ALPHABET+" :PACKAGE-NAME "S-BASE64" :EXTERNALP
      NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "(LET ((S-BASE64::INVERSE-BASE64-ALPHABET (MAKE-ARRAY CHAR-CODE-LIMIT)))
  (DOTIMES (S-BASE64::I CHAR-CODE-LIMIT S-BASE64::INVERSE-BASE64-ALPHABET)
    (SETF (AREF S-BASE64::INVERSE-BASE64-ALPHABET S-BASE64::I)
            (POSITION (CODE-CHAR S-BASE64::I) S-BASE64::+BASE64-ALPHABET+))))")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CORE-ENCODE-BASE64" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "BYTE1" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
      (:NAME "BYTE2" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
      (:NAME "BYTE3" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
