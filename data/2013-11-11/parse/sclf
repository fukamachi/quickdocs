
(:TYPE :SYSTEM :NAME "sclf" :AUTHOR "Walter C. Pelissero <walter@pelissero.de>"
 :MAINTAINER "Walter C. Pelissero <walter@pelissero.de>" :VERSION NIL :LICENCE
 "LGPL" :DESCRIPTION "Stray Common Lisp Functions" :LONG-DESCRIPTION
 "A collection of Common Lisp functions for the most disparate
uses, too small to fit anywhere else."
 :DEPENDS-ON ("SB-POSIX") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "SCLF" :FULL-NAME "SCLF" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :MACRO :SYMBOL (:NAME "BE" :PACKAGE-NAME "SCLF" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
