 (HEMLOCK-INTERFACE:MAKE-MODELINE-FIELD :NAME :MORE-PROMPT :FUNCTION
                                        #'(LAMBDA
                                              (HEMLOCK-INTERNALS:BUFFER
                                               HEMLOCK-INTERFACE:WINDOW)
                                            (DECLARE
                                             (IGNORE HEMLOCK-INTERFACE:WINDOW))
                                            (ECASE
                                                HEMLOCK-INTERFACE:*MORE-PROMPT-ACTION*
                                              (:MORE \"--More--\")
