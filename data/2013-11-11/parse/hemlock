  (:TYPE :PACKAGE :NAME "CLIM-HEMLOCK" :FULL-NAME "CLIM-HEMLOCK" :DOCUMENTATION
   NIL :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*GUTTER*" :PACKAGE-NAME "CLIM-HEMLOCK" :EXTERNALP NIL)
     :DOCUMENTATION
     "The gutter to place between between the matter in a hemlock pane and its
   margin to improve legibility (sp?, damn i miss ispell)."
     :INITIAL-VALUE "10")
    (:TYPE :CLASS :SYMBOL
     (:NAME "CLIM-DEVICE" :PACKAGE-NAME "CLIM-HEMLOCK" :EXTERNALP NIL)
     :SUPER-CLASS-LIST
     ((:NAME "DEVICE" :PACKAGE-NAME "HEMLOCK-INTERFACE" :EXTERNALP T))
     :DOCUMENTATION NIL :SLOT-LIST
     ((:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "CURSOR-HUNK" :PACKAGE-NAME "CLIM-HEMLOCK" :EXTERNALP NIL)
       :ACCESSORS NIL :READERS NIL :WRITERS NIL :DOCUMENTATION
       "The hunk that has the cursor.")
