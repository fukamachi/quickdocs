      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "TARGET" :PACKAGE-NAME "HEMLOCK" :EXTERNALP NIL)
       ((:NAME "PROMPT-FOR-STRING" :PACKAGE-NAME "HEMLOCK-INTERFACE" :EXTERNALP
         T)
        (:NAME "PROMPT" :PACKAGE-NAME "KEYWORD" :EXTERNALP T) "Query Replace: "
        (:NAME "HELP" :PACKAGE-NAME "KEYWORD" :EXTERNALP T) "Target string"
        (:NAME "DEFAULT" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)
        (:NAME "*LAST-SEARCH-STRING*" :PACKAGE-NAME "HEMLOCK" :EXTERNALP NIL)))
      ((:NAME "REPLACEMENT" :PACKAGE-NAME "HEMLOCK" :EXTERNALP NIL)
       ((:NAME "PROMPT-FOR-STRING" :PACKAGE-NAME "HEMLOCK-INTERFACE" :EXTERNALP
         T)
