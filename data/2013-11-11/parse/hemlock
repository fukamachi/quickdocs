    (:TYPE :CLASS :SYMBOL
     (:NAME "LINE" :PACKAGE-NAME "HEMLOCK-INTERNALS" :EXTERNALP T)
     :SUPER-CLASS-LIST NIL :DOCUMENTATION
     "A Hemlock line object.  See Hemlock design document for details."
     :SLOT-LIST
     ((:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "%CHARS" :PACKAGE-NAME "HEMLOCK-INTERNALS" :EXTERNALP NIL)
       :ACCESSORS
       ((:NAME "LINE-%CHARS" :PACKAGE-NAME "HEMLOCK-INTERNALS" :EXTERNALP NIL))
       :READERS NIL :WRITERS NIL :DOCUMENTATION
       "Something that represents the contents of the line.  This is
                    guaranteed to change (as compared by EQL) whenver the contents of the
                    line changes, but might at arbitarary other times.  There are
                    currently about three different cases:
  
                    Normal:
                       A simple string holding the contents of the line.
  
                    A cached line:
                       The line is eq to Open-Line, and the actual contents are in the
                       line cache.  The %Chars may be either the original contents or a
                       negative fixnum.
  
                    A buffered line:
                       The line hasn't been touched since it was read from a file, and the
                       actual contents are in some system I/O area.  This is indicated by
                       the Line-Buffered-P slot being true.  In buffered lines on the RT,
                       the %Chars slot contains the system-area-pointer to the beginning
                       of the characters.")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "PREVIOUS" :PACKAGE-NAME "HEMLOCK-INTERNALS" :EXTERNALP NIL)
       :ACCESSORS
       ((:NAME "LINE-PREVIOUS" :PACKAGE-NAME "HEMLOCK-INTERFACE" :EXTERNALP T))
       :READERS NIL :WRITERS NIL :DOCUMENTATION
       "Pointer to the previous line in the doubly linked list of line instances.")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "NEXT" :PACKAGE-NAME "HEMLOCK-INTERNALS" :EXTERNALP NIL)
       :ACCESSORS
       ((:NAME "LINE-NEXT" :PACKAGE-NAME "HEMLOCK-INTERFACE" :EXTERNALP T))
       :READERS NIL :WRITERS NIL :DOCUMENTATION
       "Pointer to the next line in the doubly linked list of line instances.")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "MARKS" :PACKAGE-NAME "HEMLOCK-INTERNALS" :EXTERNALP NIL)
       :ACCESSORS
       ((:NAME "LINE-MARKS" :PACKAGE-NAME "HEMLOCK-INTERNALS" :EXTERNALP NIL))
       :READERS NIL :WRITERS NIL :DOCUMENTATION
       "A list of all the permanent marks pointing into this line.")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "%BUFFER" :PACKAGE-NAME "HEMLOCK-INTERNALS" :EXTERNALP NIL)
       :ACCESSORS
       ((:NAME "LINE-%BUFFER" :PACKAGE-NAME "HEMLOCK-INTERNALS" :EXTERNALP
         NIL))
       :READERS NIL :WRITERS NIL :DOCUMENTATION
       "The buffer to which this line belongs, or a *disembodied-buffer-count* if the line is not in any buffer.")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "NUMBER" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :ACCESSORS
       ((:NAME "LINE-NUMBER" :PACKAGE-NAME "HEMLOCK-INTERNALS" :EXTERNALP NIL))
       :READERS NIL :WRITERS NIL :DOCUMENTATION
       "A non-negative integer (fixnum) that represents the ordering of lines
                    within continguous range of lines (a buffer or disembuffered region).
                    The number of the Line-Next is guaranteed to be strictly greater than
                    our number, and the Line-Previous is guaranteed to be strictly less.")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "PLIST" :PACKAGE-NAME "HEMLOCK-INTERNALS" :EXTERNALP NIL)
       :ACCESSORS
       ((:NAME "LINE-PLIST" :PACKAGE-NAME "HEMLOCK-INTERFACE" :EXTERNALP T))
       :READERS NIL :WRITERS NIL :DOCUMENTATION
       "The line property list, used by user code to annotate the text.")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "BUFFERED-P" :PACKAGE-NAME "HEMLOCK-INTERNALS" :EXTERNALP NIL)
       :ACCESSORS
       ((:NAME "LINE-BUFFERED-P" :PACKAGE-NAME "HEMLOCK-INTERFACE" :EXTERNALP
         T))
       :READERS NIL :WRITERS NIL :DOCUMENTATION
       "A slot that indicates whether this line is a buffered line, and if so
                    contains information about how the text is stored. On the RT, this is
                    the length of the text pointed to by the Line-%Chars")))
