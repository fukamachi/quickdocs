
(:TYPE :SYSTEM :NAME "cl-syntax" :AUTHOR "Tomohiro Matsuyama" :MAINTAINER NIL
 :VERSION "0.3" :LICENCE "LLGPL" :DESCRIPTION
 "Reader Syntax Coventions for Common Lisp and SLIME" :LONG-DESCRIPTION NIL
 :DEPENDS-ON ("TRIVIAL-TYPES" "NAMED-READTABLES") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-SYNTAX" :FULL-NAME "CL-SYNTAX" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*STANDARD-PACKAGE-SYNTAX-NAME*" :PACKAGE-NAME "CL-SYNTAX"
      :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(SYMBOL-NAME :SYNTAX)")
    (:TYPE :TYPE :SYMBOL
     (:NAME "SYNTAX" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP T) :LAMBDA-LIST
     (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :DOCUMENTATION NIL)
    (:TYPE :TYPE :SYMBOL
     (:NAME "SYNTAX-DESIGNATOR" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PACKAGE-SYNTAX-NAME" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "DEFSYNTAX" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "OPTIONS" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "DEFINE-PACKAGE-SYNTAX" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) :NAME
       "OPTIONS" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FIND-SYNTAX" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP NIL)) :DOCUMENTATION
     NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "%USE-SYNTAX" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "NAME" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "USE-SYNTAX" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP NIL)) :DOCUMENTATION
     NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "USE-SYNTAX" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP T)
    (:NAME "DEFSYNTAX" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP T)
    (:NAME "SYNTAX" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP T)
    (:NAME "DEFINE-PACKAGE-SYNTAX" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP T)
    (:NAME "SYNTAX-DESIGNATOR" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP T)
    (:NAME "FIND-SYNTAX" :PACKAGE-NAME "CL-SYNTAX" :EXTERNALP T)))))
