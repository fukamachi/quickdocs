
(:TYPE :SYSTEM :NAME "yaclanapht" :AUTHOR NIL :MAINTAINER NIL :VERSION "0.1"
 :LICENCE "GPLv3" :DESCRIPTION NIL :LONG-DESCRIPTION NIL :DEPENDS-ON
 ("DEFMACRO-ENHANCE" "ITERATE") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "YACLANAPHT" :FULL-NAME "YACLANAPHT" :DOCUMENTATION
   "Collection of anaphoric macros from 'On Lisp' and 'Let Over Lambda'.
Distinct feature is that it interns IT (and other anaphoric symbols) directly into callee-package's
namespace."
   :SYMBOL-LIST
   ((:TYPE :MACRO :SYMBOL (:NAME "AIF" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "TEST" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL)
      (:NAME "THEN" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "ELSE" :PACKAGE-NAME "ITERATE" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "AWHEN" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "TEST" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "AVIF" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "VAR" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL)
      (:NAME "TEST" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL)
      (:NAME "THEN" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "ELSE" :PACKAGE-NAME "ITERATE" :EXTERNALP T))
     :DOCUMENTATION
     "Anaphoric-variants IF. Binds VAR to result of TEST evaluation.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "APIF" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "OP" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL)
      (:NAME "TEST" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL)
      (:NAME "THEN" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "ELSE" :PACKAGE-NAME "ITERATE" :EXTERNALP T))
     :DOCUMENTATION "Anaphoric-predicative if (i.e. it binds IT to
test, then uses OP to determine whether to go to THEN or
ELSE.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "AAND" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CLAUSES" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL))
     :DOCUMENTATION "Binds IT to the subsequent return-values of clauses.
Such that for each clause, return-value of previous one
is accessible through IT.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "AANDL" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CLAUSES" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL))
     :DOCUMENTATION "Like AAND, but IT is gradually consed list of
return-values of clauses.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ACOND-GOT" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CLAUSES" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL))
     :DOCUMENTATION
     "Like COND, but assumes that all clauses predicates return 2 values: IT and GOT (like GETHASH function).
Clause succeeds, if GOT is T and IT is bound to the first value in the body of the clause.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "AVCOND-GOT" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "VAR" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CLAUSES" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP NIL))
     :DOCUMENTATION "Anaphoric-variants ACOND-GOT."))
   :EXTERNAL-SYMBOLS
   ((:NAME "AAND" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T)
    (:NAME "AVIF" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T)
    (:NAME "ACOND-GOT" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T)
    (:NAME "AANDL" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T)
    (:NAME "APIF" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T)
    (:NAME "AVCOND-GOT" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T)
    (:NAME "AIF" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T)
    (:NAME "AWHEN" :PACKAGE-NAME "YACLANAPHT" :EXTERNALP T)))))
