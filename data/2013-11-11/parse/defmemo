
(:TYPE :SYSTEM :NAME "defmemo" :AUTHOR "Orivej Desh <orivej@gmx.fr>"
 :MAINTAINER NIL :VERSION "1.5" :LICENCE
 "Unlicense <http://unlicense.org/UNLICENSE>" :DESCRIPTION "Memoizing defun"
 :LONG-DESCRIPTION NIL :DEPENDS-ON ("ALEXANDRIA" "TRIVIAL-GARBAGE")
 :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "DEFMEMO" :FULL-NAME "DEFMEMO" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "GET-MEMO" :PACKAGE-NAME "DEFMEMO" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "SYMBOL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)) :DOCUMENTATION
     "Get memoizing hash table.")
    (:TYPE :FUNCTION :SYMBOL
     ((:NAME "SETF" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "GET-MEMO" :PACKAGE-NAME "DEFMEMO" :EXTERNALP T))
     :LAMBDA-LIST
     ((:NAME "VALUE" :PACKAGE-NAME "DEFMEMO" :EXTERNALP NIL)
      (:NAME "SYMBOL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Replace memoizing hash of symbol with value.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CLEAR-MEMO" :PACKAGE-NAME "DEFMEMO" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "SYMBOL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)) :DOCUMENTATION
     "Reset memoizing hash table.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FLAT-ARGLIST" :PACKAGE-NAME "DEFMEMO" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "ARGS" :PACKAGE-NAME "DEFMEMO" :EXTERNALP NIL)) :DOCUMENTATION
     "Convert ordinary lambda list into funcallable list.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "DEFMEMO" :PACKAGE-NAME "DEFMEMO" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "DEFMEMO" :EXTERNALP NIL)
      (:NAME "ARGS" :PACKAGE-NAME "DEFMEMO" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "DEFMEMO" :EXTERNALP NIL))
     :DOCUMENTATION
     "Construct defun with body wrapped into memoizing hash table.  Put the latter under :memo property of name."))
   :EXTERNAL-SYMBOLS
   ((:NAME "CLEAR-MEMO" :PACKAGE-NAME "DEFMEMO" :EXTERNALP T)
    (:NAME "DEFMEMO" :PACKAGE-NAME "DEFMEMO" :EXTERNALP T)
    (:NAME "GET-MEMO" :PACKAGE-NAME "DEFMEMO" :EXTERNALP T)))))
