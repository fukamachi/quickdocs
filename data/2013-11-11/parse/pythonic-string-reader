
(:TYPE :SYSTEM :NAME "pythonic-string-reader" :AUTHOR
 "Yury Sulsky and Zach Kost-Smith" :MAINTAINER
 "Zach Kost-Smith <zachkostsmith@gmail.com>" :VERSION NIL :LICENCE NIL
 :DESCRIPTION "A simple and unintrusive read table modification that allows for
simple string literal definition that doesn't require escaping characters."
 :LONG-DESCRIPTION NIL :DEPENDS-ON ("NAMED-READTABLES") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "PYTHONIC-STRING-READER" :FULL-NAME
   "PYTHONIC-STRING-READER" :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-MULTILINE-STRING" :PACKAGE-NAME "PYTHONIC-STRING-READER"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "C" :PACKAGE-NAME "PYTHONIC-STRING-READER" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ENABLE-PYTHONIC-STRING-SYNTAX" :PACKAGE-NAME
      "PYTHONIC-STRING-READER" :EXTERNALP T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "DISABLE-PYTHONIC-STRING-SYNTAX" :PACKAGE-NAME
      "PYTHONIC-STRING-READER" :EXTERNALP T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "PYTHONIC-STRING-SYNTAX" :PACKAGE-NAME "PYTHONIC-STRING-READER"
     :EXTERNALP T)
    (:NAME "DISABLE-PYTHONIC-STRING-SYNTAX" :PACKAGE-NAME
     "PYTHONIC-STRING-READER" :EXTERNALP T)
    (:NAME "ENABLE-PYTHONIC-STRING-SYNTAX" :PACKAGE-NAME
     "PYTHONIC-STRING-READER" :EXTERNALP T)))))
