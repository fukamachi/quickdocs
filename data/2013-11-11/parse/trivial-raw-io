
(:TYPE :SYSTEM :NAME "trivial-raw-io" :AUTHOR
 "Brit Butler <redline6561@gmail.com>" :MAINTAINER
 "Brit Butler <redline6561@gmail.com>" :VERSION "0.0.1" :LICENCE "BSD"
 :DESCRIPTION "Helpers for doing raw POSIX I/O" :LONG-DESCRIPTION NIL
 :DEPENDS-ON ("ALEXANDRIA") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "TRIVIAL-RAW-IO" :FULL-NAME "TRIVIAL-RAW-IO"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :MACRO :SYMBOL
     (:NAME "WITH-RAW-IO" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       ((:NAME "VMIN" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP NIL) 1)
       ((:NAME "VTIME" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP NIL) 0))
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP NIL))
     :DOCUMENTATION "Execute BODY without echoing input IO actions.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-CHAR" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*QUERY-IO*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      ((:NAME "EOF-ERROR-P" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP NIL)
       (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      (:NAME "EOF-VALUE" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP NIL)
      (:NAME "RECURSIVE-P" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP NIL))
     :DOCUMENTATION
     "Read a single character without echoing it from stream STREAM.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-LINE" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*QUERY-IO*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      ((:NAME "EOF-ERROR-P" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP NIL)
       (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      (:NAME "EOF-VALUE" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP NIL)
      (:NAME "RECURSIVE-P" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP NIL))
     :DOCUMENTATION "Read a line without echoing it from stream STREAM."))
   :EXTERNAL-SYMBOLS
   ((:NAME "WITH-RAW-IO" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP T)
    (:NAME "READ-LINE" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP T)
    (:NAME "READ-CHAR" :PACKAGE-NAME "TRIVIAL-RAW-IO" :EXTERNALP T)))))
