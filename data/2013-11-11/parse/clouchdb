     (:NAME "COPY-DOCUMENT" :PACKAGE-NAME "CLOUCHDB" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "SOURCE" :PACKAGE-NAME "CLOUCHDB" :EXTERNALP NIL)
      (:NAME "DESTINATION" :PACKAGE-NAME "CLOUCHDB" :EXTERNALP NIL)
