                 (ASDF/COMPONENT:COMPONENT-PATHNAME
                  (ASDF/SYSTEM:FIND-SYSTEM :QUICKUTIL-SERVER)))")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "RECOMPILE-TEMPLATES" :PACKAGE-NAME "QUICKUTIL-SERVER" :EXTERNALP
      T)
     :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "PATH" :PACKAGE-NAME "QUICKUTIL-SERVER" :EXTERNALP NIL)
       (:NAME "*TEMPLATE-PATH*" :PACKAGE-NAME "QUICKUTIL-SERVER" :EXTERNALP
        T)))
     :DOCUMENTATION NIL)
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*CATEGORIES*" :PACKAGE-NAME "QUICKUTIL-SERVER" :EXTERNALP T)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "(LOOP QUICKUTIL-SERVER::WITH QUICKUTIL-SERVER::CATEGORIES = (MAKE-HASH-TABLE
                                                             :TEST 'EQUAL)
      QUICKUTIL-SERVER::FOR QUICKUTIL-SERVER::UTILITY QUICKUTIL-SERVER::BEING THE QUICKUTIL-SERVER::HASH-VALUES QUICKUTIL-SERVER::IN QUICKUTIL-UTILITIES::*UTILITY-REGISTRY*
      DO (LOOP QUICKUTIL-SERVER::FOR QUICKUTIL-SERVER::CAT QUICKUTIL-SERVER::IN (QUICKUTIL-UTILITIES::UTIL.CATEGORIES
                                                                                 QUICKUTIL-SERVER::UTILITY)
               DO (INCF
                   (GETHASH (STRING-DOWNCASE QUICKUTIL-SERVER::CAT)
                            QUICKUTIL-SERVER::CATEGORIES 0)))
      QUICKUTIL-SERVER::FINALLY (RETURN
                                 (LOOP QUICKUTIL-SERVER::FOR QUICKUTIL-SERVER::CAT QUICKUTIL-SERVER::IN (SORT
                                                                                                         (COPY-LIST
                                                                                                          (ALEXANDRIA.0.DEV:HASH-TABLE-KEYS
                                                                                                           QUICKUTIL-SERVER::CATEGORIES))
                                                                                                         #'STRING<)
                                       QUICKUTIL-SERVER::COLLECT `(:NAME
                                                                   ,QUICKUTIL-SERVER::CAT
                                                                   :COUNT
                                                                   ,(GETHASH
                                                                     QUICKUTIL-SERVER::CAT
                                                                     QUICKUTIL-SERVER::CATEGORIES)))))")
