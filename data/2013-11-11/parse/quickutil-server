 ((:TYPE :PACKAGE :NAME ".APP" :FULL-NAME "QUICKUTIL-SERVER.APP" :DOCUMENTATION
   NIL :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*WEB*" :PACKAGE-NAME "QUICKUTIL-SERVER.APP" :EXTERNALP T)
     :DOCUMENTATION NIL :INITIAL-VALUE "(MAKE-INSTANCE 'NINGLE.APP:<APP>)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*API*" :PACKAGE-NAME "QUICKUTIL-SERVER.APP" :EXTERNALP T)
     :DOCUMENTATION NIL :INITIAL-VALUE "(MAKE-INSTANCE 'NINGLE.APP:<APP>)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*APP*" :PACKAGE-NAME "QUICKUTIL-SERVER.APP" :EXTERNALP T)
     :DOCUMENTATION NIL :INITIAL-VALUE "#'(LAMBDA (QUICKUTIL-SERVER.APP::ENV)
    (SYMBOL-MACROLET ((QUICKUTIL-SERVER.APP::PATH-INFO
                       (GETF QUICKUTIL-SERVER.APP::ENV :PATH-INFO)))
      (IF (CL-PPCRE:SCAN \"^/api/\" QUICKUTIL-SERVER.APP::PATH-INFO)
          (PROGN
           (SETF QUICKUTIL-SERVER.APP::PATH-INFO
                   (SUBSEQ QUICKUTIL-SERVER.APP::PATH-INFO 4))
           (CLACK.COMPONENT:CALL QUICKUTIL-SERVER.APP:*API*
                                 QUICKUTIL-SERVER.APP::ENV))
          (CLACK.COMPONENT:CALL QUICKUTIL-SERVER.APP:*WEB*
                                QUICKUTIL-SERVER.APP::ENV))))"))
   :EXTERNAL-SYMBOLS
   ((:NAME "*WEB*" :PACKAGE-NAME "QUICKUTIL-SERVER.APP" :EXTERNALP T)
    (:NAME "*APP*" :PACKAGE-NAME "QUICKUTIL-SERVER.APP" :EXTERNALP T)
    (:NAME "*API*" :PACKAGE-NAME "QUICKUTIL-SERVER.APP" :EXTERNALP T)))
