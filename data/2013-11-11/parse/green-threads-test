
(:TYPE :SYSTEM :NAME "green-threads-test" :AUTHOR "Stephen A. Goss" :MAINTAINER
 NIL :VERSION NIL :LICENCE "Modified BSD" :DESCRIPTION NIL :LONG-DESCRIPTION
 NIL :DEPENDS-ON ("GREEN-THREADS" "CL-TEST-MORE") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "GREEN-THREADS-TEST" :FULL-NAME "GREEN-THREADS-TEST"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*VAR*" :PACKAGE-NAME "GREEN-THREADS-TEST" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*THREADS-SAVED*" :PACKAGE-NAME "GREEN-THREADS-TEST" :EXTERNALP
      NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*THREAD*" :PACKAGE-NAME "GREEN-THREADS-TEST" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(GREEN-THREADS:MAKE-THREAD
 (LAMBDA ()
   (SETF GREEN-THREADS-TEST::*THREADS-SAVED* (GREEN-THREADS:ALL-THREADS))
   (CL-TEST-MORE:IS (LENGTH GREEN-THREADS-TEST::*THREADS-SAVED*) 1
                    \"One active thread.\")
   (WHEN GREEN-THREADS-TEST::*THREADS-SAVED*
     (CL-TEST-MORE:IS
      (GREEN-THREADS:THREAD-ALIVE-P (CAR GREEN-THREADS-TEST::*THREADS-SAVED*))
      T \"Thread is alive.\"))
   (SETF GREEN-THREADS-TEST::*VAR* :SET)))")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*FUTURE-ONE*" :PACKAGE-NAME "GREEN-THREADS-TEST" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(CL-ASYNC-FUTURE:MAKE-FUTURE)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*VAL1*" :PACKAGE-NAME "GREEN-THREADS-TEST" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*VAL2*" :PACKAGE-NAME "GREEN-THREADS-TEST" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*THREAD1*" :PACKAGE-NAME "GREEN-THREADS-TEST" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*FUTURE*" :PACKAGE-NAME "GREEN-THREADS-TEST" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(CL-ASYNC-FUTURE:MAKE-FUTURE)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*THREAD-JOINED*" :PACKAGE-NAME "GREEN-THREADS-TEST" :EXTERNALP
      NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*X*" :PACKAGE-NAME "GREEN-THREADS-TEST" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "0"))
   :EXTERNAL-SYMBOLS NIL)))
