
(:TYPE :SYSTEM :NAME "conium" :AUTHOR NIL :MAINTAINER NIL :VERSION NIL :LICENCE
 NIL :DESCRIPTION NIL :LONG-DESCRIPTION NIL :DEPENDS-ON ("CLOSER-MOP")
 :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CONIUM" :FULL-NAME "CONIUM" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*INTERFACE-FUNCTIONS*" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL)
     :DOCUMENTATION "The names of all interface functions." :INITIAL-VALUE
     "'NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*UNIMPLEMENTED-INTERFACES*" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL)
     :DOCUMENTATION "List of interface functions that are not implemented.
DEFINTERFACE adds to this list and DEFIMPLEMENTATION removes."
     :INITIAL-VALUE "'NIL")
    (:TYPE :MACRO :SYMBOL
     (:NAME "DEFINTERFACE" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL)
      (:NAME "ARGS" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL)
      (:NAME "DOCUMENTATION" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "DEFAULT-BODY" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL))
     :DOCUMENTATION "Define an interface function for the backend to implement.
A function is defined with NAME, ARGS, and DOCUMENTATION.  This
function first looks for a function to call in NAME's property list
that is indicated by 'IMPLEMENTATION; failing that, it looks for a
function indicated by 'DEFAULT. If neither is present, an error is
signaled.

If a DEFAULT-BODY is supplied, then a function with the same body and
ARGS will be added to NAME's property list as the property indicated
by 'DEFAULT.

Backends implement these functions using DEFIMPLEMENTATION.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "DEFIMPLEMENTATION" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL)
      (:NAME "ARGS" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WARN-UNIMPLEMENTED-INTERFACES" :PACKAGE-NAME "CONIUM" :EXTERNALP
      NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION "Warn the user about unimplemented backend features.
The portable code calls this function at startup.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "IMPORT-FROM" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "SYMBOL-NAMES" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "TO-PACKAGE" :PACKAGE-NAME "CONIUM" :EXTERNALP NIL)
       (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION
     "Import the list of SYMBOL-NAMES found in the package PACKAGE.")
    (:TYPE :MACRO :SYMBOL
