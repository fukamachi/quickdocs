    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*EXPRESSION-KINDS*" :PACKAGE-NAME "ESRAP" :EXTERNALP NIL)
     :DOCUMENTATION "Names and corresponding types of acceptable expression
constructors."
     :INITIAL-VALUE "`((CHARACTER EQL CHARACTER)
  (ESRAP:CHARACTER-RANGES CONS (EQL ESRAP:CHARACTER-RANGES))
  (STRING CONS (EQL STRING) (CONS ALEXANDRIA.0.DEV:ARRAY-LENGTH NULL))
  (AND CONS (EQL AND)) (OR CONS (EQL OR))
  ,@(MAPCAR (LAMBDA (SYMBOL) `(,SYMBOL CONS (EQL ,SYMBOL) (CONS T NULL)))
            '(NOT * + ESRAP:? ESRAP:& ESRAP:!))
  (ESRAP::TERMINAL . ESRAP::TERMINAL) (ESRAP::NONTERMINAL . ESRAP::NONTERMINAL)
  (ESRAP::PREDICATE CONS ESRAP::PREDICATE-NAME (CONS (NOT NULL) NULL))
  (FUNCTION CONS (EQL FUNCTION) (CONS SYMBOL NULL)) (T . T))")
