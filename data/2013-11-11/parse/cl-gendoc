
(:TYPE :SYSTEM :NAME "cl-gendoc" :AUTHOR "Ryan Pavlik" :MAINTAINER NIL :VERSION
 "1.0" :LICENCE "LLGPL, BSD" :DESCRIPTION
 "cl-gendoc: Simple component-based documentation generator" :LONG-DESCRIPTION
 NIL :DEPENDS-ON ("CL-WHO" "3BMD" "3BMD-EXT-CODE-BLOCKS") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "GENDOC" :FULL-NAME "GENDOC" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :MACRO :SYMBOL
     (:NAME "DEFINE-GENDOC-LOAD-OP" :PACKAGE-NAME "GENDOC" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "SYSTEM-NAME" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
      (:NAME "PACKAGE-NAME" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "FUNCTION-SYMBOL" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL))
     :DOCUMENTATION
     "Define `PERFORM` and `OPERATION-DONE-P` for `ASDF:LOAD-OP` for the system
`SYSTEM-NAME`, causing `LOAD-OP` to always call `FUNCTION-SYMBOL` in the
package `PACKAGE-NAME`.

For this to be useful, the specified function should call `GENDOC` to actually
generate documentation.")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*PART-PROCESSOR*" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(MAKE-HASH-TABLE)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ADD-PROCESSOR" :PACKAGE-NAME "GENDOC" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
      (:NAME "FUNCTION" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION
     "Call `FUNCTION` when the component named `NAME` is encountered in the
gendoc spec.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-ALL" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "FILE-STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ARGLIST" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "FUNCTION" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL (:NAME "GENDOC" :PACKAGE-NAME "GENDOC" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        ((:NAME "QUOTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
         (:NAME "*STANDARD-OUTPUT*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
       ((:NAME "TITLE" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
        "GenDoc Documentation")
       (:NAME "CSS" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
       (:NAME "PATH" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
       (:NAME "OUTPUT-SYSTEM" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
       (:NAME "OUTPUT-COMPONENT" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
       (:NAME "OUTPUT-FILENAME" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL))
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "PARTS" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL))
     :DOCUMENTATION "=> string or no values

Generate documentation based on `PARTS`.  Each part is processed in order
and the output is written to `STREAM`, or, if specified, `OUTPUT-FILE` is
overwritten (`:if-exists :supersede`).

`TITLE` and `CSS` specify their HTML counterparts.

`OUTPUT-SYSTEM` and `OUTPUT-COMPONENT` may be specified; if so, they are
consulted to set `*DEFAULT-PATHNAME-DEFAULTS*`, and files are read from
and written to this location.

`PATH` may be specified instead or along with `OUTPUT-SYSTEM` and
`OUTPUT-COMPONENT`; if both are specified, `PATH` overrides.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FILENAME-ID" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "FILENAME" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)) :DOCUMENTATION
     NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PROCESS-TEXT-FILE" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "NAME" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
      (:NAME "PART" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PROCESS-MARKDOWN-FILE" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "NAME" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
      (:NAME "PART" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SPECIAL-P" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "SYMBOL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)) :DOCUMENTATION
     NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FUNCTION-P" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "SYMBOL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)) :DOCUMENTATION
     NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MACRO-P" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "SYMBOL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)) :DOCUMENTATION
     NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "APIREF-SYMBOLS" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "TYPE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "APIREF-SPEC" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "TYPE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "SYM" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "APIREF-LAMBDA" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "TYPE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "SYM" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "APIREF-RESULT" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "TYPE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "SYM" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "APIREF-DOC" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "TYPE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "SYM" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "APIREF-SECTION-ID" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "SYMBOL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MARKDOWN-PLAIN" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "INPUT" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL))
     :DOCUMENTATION
     "Runs `input` through markdown an removes the enclosing <p> tags.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "APIREF-SECTION-SYMBOL" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "TYPE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "SYMBOL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PACKAGE-SECTION-ID" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "SUFFIX" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "GEN-APIREF" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PROCESS-APIREF" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "NAME" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL)
      (:NAME "PACKAGE-LIST" :PACKAGE-NAME "GENDOC" :EXTERNALP NIL))
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "GENDOC" :PACKAGE-NAME "GENDOC" :EXTERNALP T)
    (:NAME "DEFINE-GENDOC-LOAD-OP" :PACKAGE-NAME "GENDOC" :EXTERNALP T)
    (:NAME "ADD-PROCESSOR" :PACKAGE-NAME "GENDOC" :EXTERNALP T)))))
