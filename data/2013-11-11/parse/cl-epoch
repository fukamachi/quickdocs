
(:TYPE :SYSTEM :NAME "cl-epoch" :AUTHOR
 "Andrew Pennebaker <andrew.pennebaker@gmail.com>" :MAINTAINER NIL :VERSION
 "0.1" :LICENCE "FREEBSD" :DESCRIPTION
 "Converts between CL Universal Time and UNIX Epoch Time" :LONG-DESCRIPTION NIL
 :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-EPOCH" :FULL-NAME "CL-EPOCH" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*UNIX-EPOCH-DIFFERENCE*" :PACKAGE-NAME "CL-EPOCH" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "(ENCODE-UNIVERSAL-TIME 0 0 0 1 1 1970 0)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "UNIVERSAL->UNIX-TIME" :PACKAGE-NAME "CL-EPOCH" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "UNIVERSAL-TIME" :PACKAGE-NAME "CL-EPOCH" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "UNIX->UNIVERSAL-TIME" :PACKAGE-NAME "CL-EPOCH" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "UNIX-TIME" :PACKAGE-NAME "CL-EPOCH" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "GET-EPOCH-TIME" :PACKAGE-NAME "CL-EPOCH" :EXTERNALP T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "UNIX->UNIVERSAL-TIME" :PACKAGE-NAME "CL-EPOCH" :EXTERNALP T)
    (:NAME "UNIVERSAL->UNIX-TIME" :PACKAGE-NAME "CL-EPOCH" :EXTERNALP T)
    (:NAME "GET-EPOCH-TIME" :PACKAGE-NAME "CL-EPOCH" :EXTERNALP T)))))
