
(:TYPE :SYSTEM :NAME "cl-rfc2047" :AUTHOR "Christian Haselbach" :MAINTAINER NIL
 :VERSION "0.2" :LICENCE "MIT" :DESCRIPTION "Support for rfc-2047 de-/encoding"
 :LONG-DESCRIPTION NIL :DEPENDS-ON ("CL-BASE64" "FLEXI-STREAMS" "CL-PPCRE")
 :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-RFC2047" :FULL-NAME "CL-RFC2047" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :CONSTANT :SYMBOL
     (:NAME "+CRLFSP+" :PACKAGE-NAME "CL-RFC2047" :EXTERNALP NIL)
     :DOCUMENTATION
     "The character sequence <CRLFSP> which seperates the lines of multi
line fields."
     :INITIAL-VALUE
     "(CONCATENATE 'STRING (STRING #\\Return) (STRING #\\Newline) (STRING #\\ ))"))
   :EXTERNAL-SYMBOLS
   ((:NAME "ENCODE" :PACKAGE-NAME "CL-RFC2047" :EXTERNALP T)
    (:NAME "DECODE" :PACKAGE-NAME "CL-RFC2047" :EXTERNALP T)
    (:NAME "DECODE*" :PACKAGE-NAME "CL-RFC2047" :EXTERNALP T)))))
