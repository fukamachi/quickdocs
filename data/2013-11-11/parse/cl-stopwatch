
(:TYPE :SYSTEM :NAME "cl-stopwatch" :AUTHOR "Robert Smith" :MAINTAINER NIL
 :VERSION "0.1" :LICENCE "BSD 3-clause. See LICENSE." :DESCRIPTION
 "Timing and clocking in Common Lisp." :LONG-DESCRIPTION NIL :DEPENDS-ON NIL
 :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-STOPWATCH" :FULL-NAME "CL-STOPWATCH" :DOCUMENTATION
   NIL :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*STOPWATCH-TIMING*" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(MAKE-HASH-TABLE)")
    (:TYPE :MACRO :SYMBOL
     (:NAME "GET-ACCURATE-TIME" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION "Get the time in milliseconds, accurately.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "STOPWATCH-START" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "KW" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL))
     :DOCUMENTATION "Start a stopwatch designated by KW.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "STOPWATCH-READ" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "KW" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL))
     :DOCUMENTATION "Read a stopwatch designated by KW.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "STOPWATCH-STOP" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "KW" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL))
     :DOCUMENTATION
     "Stop the stopwatch designated by KW and return the reading in milliseconds.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "STOPWATCH-RESET" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "KW" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL))
     :DOCUMENTATION
     "Resets the stopwatch designated by KW. Equivalent to starting it.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "WITH-STOPWATCH" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL))
     :DOCUMENTATION
     "Execute BODY, returning its value as well as the value of the watch.")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*PROGRESS-INDENT*" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "0")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PROGRESS-TASK-START" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "KW" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL)
      (:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*STANDARD-OUTPUT*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION
     "Start a task designated by the keyword KW, with a comment STRING,
and print an indication that the task has started and start a
stopwatch. Print everything to STREAM.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PROGRESS-TASK-DONE" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "KW" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*STANDARD-OUTPUT*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION
     "End the task designated by the keyword KW and report its completion
time to STREAM.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "PROGRESS-TASK" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "KW" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL)
      (:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL))
     :DOCUMENTATION
     "Execute BODY, reporting the task, labeled by the keyword KW and
commented with STRING. Report its end time and return the value
returned by BODY.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "TIME*" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "FORM" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP NIL))
     :DOCUMENTATION "Freshly compile FORM and time its execution."))
   :EXTERNAL-SYMBOLS
   ((:NAME "STOPWATCH-RESET" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
    (:NAME "TIME*" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
    (:NAME "PROGRESS-TASK" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
    (:NAME "STOPWATCH-START" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
    (:NAME "STOPWATCH-STOP" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
    (:NAME "STOPWATCH-READ" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
    (:NAME "PROGRESS-TASK-DONE" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
    (:NAME "WITH-STOPWATCH" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)
    (:NAME "PROGRESS-TASK-START" :PACKAGE-NAME "CL-STOPWATCH" :EXTERNALP T)))))
