     (:NAME "MAKE-UTIL" :PACKAGE-NAME "QUICKUTIL-UTILITIES" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
