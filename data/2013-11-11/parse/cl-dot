
(:TYPE :SYSTEM :NAME "cl-dot" :AUTHOR "Juho Snellman <jsnell@iki.fi>"
 :MAINTAINER "Michael Weber <michaelw@foldr.org>" :VERSION "0.8.0" :LICENCE NIL
 :DESCRIPTION "Generate Dot Output from Arbitrary Lisp Data" :LONG-DESCRIPTION
 NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-DOT" :FULL-NAME "CL-DOT" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*GRAPH-ATTRIBUTES*" :PACKAGE-NAME "CL-DOT" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "'((:SIZE CL-DOT::TEXT) (:PAGE CL-DOT::TEXT) (:RATIO (:FILL :COMPRESS :AUTO))
  (:MARGIN FLOAT) (:NODESEP FLOAT) (:NODE CL-DOT::ATTRIBUTE)
  (:EDGE CL-DOT::ATTRIBUTE) (:RANKSEP FLOAT) (:ORDERING (:OUT))
  (:RANKDIR (\"LR\" \"RL\" \"BT\")) (:PAGEDIR CL-DOT::TEXT) (:RANK (:SAME :MIN :MAX))
  (:ROTATE INTEGER) (:CENTER INTEGER) (:NSLIMIT FLOAT) (:MCLIMIT FLOAT)
  (:LAYERS CL-DOT::TEXT) (:FONTSIZE INTEGER) (:FONTNAME CL-DOT::TEXT)
  (:COLOR CL-DOT::TEXT) (:BGCOLOR CL-DOT::TEXT))")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*NODE-ATTRIBUTES*" :PACKAGE-NAME "CL-DOT" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "'((:HEIGHT INTEGER) (:WIDTH INTEGER) (:FIXED-SIZE BOOLEAN)
  (:LABEL CL-DOT::TEXT)
  (:SHAPE
   (:RECORD :PLAINTEXT :ELLIPSE :CIRCLE :EGG :TRIANGLE :BOX :DIAMOND :TRAPEZIUM
    :PARALLELOGRAM :HOUSE :HEXAGON :OCTAGON :DOUBLEOCTAGON))
  (:FONTSIZE INTEGER) (:FONTNAME CL-DOT::TEXT) (:COLOR CL-DOT::TEXT)
  (:FILLCOLOR CL-DOT::TEXT)
  (:STYLE (:FILLED :SOLID :DASHED :DOTTED :BOLD :INVIS)) (:LAYER CL-DOT::TEXT))")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*EDGE-ATTRIBUTES*" :PACKAGE-NAME "CL-DOT" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "'((:MINLEN INTEGER) (:WEIGHT INTEGER) (:LABEL CL-DOT::TEXT) (:FONTSIZE INTEGER)
  (:FONTNAME CL-DOT::TEXT) (:FONTCOLOR CL-DOT::TEXT)
  (:STYLE (:SOLID :DASHED :DOTTED :BOLD :INVIS)) (:COLOR CL-DOT::TEXT)
  (:DIR (:FORWARD :BACK :BOTH :NONE)) (:TAILCLIP BOOLEAN) (:HEADCLIP BOOLEAN)
  (:ARROWHEAD
   (:NONE :NORMAL :INV :DOT :ODOT :INVDOT :INVODOT :TEE :EMPTY :INVEMPTY :OPEN
    :HALFOPEN :DIAMOND :ODIAMOND :BOX :OBOX :CROW))
  (:ARROWTAIL
   (:NONE :NORMAL :INV :DOT :ODOT :INVDOT :INVODOT :TEE :EMPTY :INVEMPTY :OPEN
    :HALFOPEN :DIAMOND :ODIAMOND :BOX :OBOX :CROW))
  (:HEADLABEL CL-DOT::TEXT) (:TAILLABEL CL-DOT::TEXT) (:LABELFONTSIZE INTEGER)
  (:LABELFONTNAME CL-DOT::TEXT) (:LABELFONTCOLOR CL-DOT::TEXT)
  (:LABELDISTANCE INTEGER) (:PORT-LABEL-DISTANCE INTEGER) (:DECORATE BOOLEAN)
  (:SAMEHEAD BOOLEAN) (:SAMETAIL BOOLEAN) (:CONSTRAINT BOOLEAN)
  (:LAYER CL-DOT::TEXT))")
