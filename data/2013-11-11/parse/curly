
(:TYPE :SYSTEM :NAME "curly" :AUTHOR
 "Maciej Pasternacki <maciekp@japhy.fnord.org>" :MAINTAINER NIL :VERSION "0.1"
 :LICENCE "BSD sans advertising clause (see file COPYING for details)"
 :DESCRIPTION "Reader macros for easy function currying and composition."
 :LONG-DESCRIPTION NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CURLY" :FULL-NAME "CURLY" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*BLANK-ARGUMENT*" :PACKAGE-NAME "CURLY" :EXTERNALP T)
     :DOCUMENTATION "Symbol used to indicate argument place in curly syntax."
     :INITIAL-VALUE "'*")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CURLYLIST" :PACKAGE-NAME "CURLY" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "ITEM" :PACKAGE-NAME "CURLY" :EXTERNALP NIL)
      (:NAME "REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CURLY-READER" :PACKAGE-NAME "CURLY" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CHAR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&AUX" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FUNS" :PACKAGE-NAME "CURLY" :EXTERNALP NIL)
       ((:NAME "READ-DELIMITED-LIST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        #\} (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))))
     :DOCUMENTATION "Reader macro for #{ character.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SQUARE-READER" :PACKAGE-NAME "CURLY" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CHAR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&AUX" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FUNS" :PACKAGE-NAME "CURLY" :EXTERNALP NIL)
       ((:NAME "READ-DELIMITED-LIST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        #\] (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))))
     :DOCUMENTATION "Reader macro for #[ character.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-CURLY-READTABLE" :PACKAGE-NAME "CURLY" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "ORIGINAL-READTABLE" :PACKAGE-NAME "CURLY" :EXTERNALP NIL)
       (:NAME "*READTABLE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      (:NAME "&AUX" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "RV" :PACKAGE-NAME "CURLY" :EXTERNALP NIL)
       ((:NAME "COPY-READTABLE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        (:NAME "ORIGINAL-READTABLE" :PACKAGE-NAME "CURLY" :EXTERNALP NIL))))
     :DOCUMENTATION "Return new readtable with curly syntax enabled.")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*ORIGINAL-READTABLE*" :PACKAGE-NAME "CURLY" :EXTERNALP NIL)
     :DOCUMENTATION
     "Original readtable to restore, used by ENABLE/DISABLE-CURLY-SYNTAX."
     :INITIAL-VALUE "NIL")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ENABLE-CURLY-SYNTAX" :PACKAGE-NAME "CURLY" :EXTERNALP T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION "Enable curly syntax for current file.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "DISABLE-CURLY-SYNTAX" :PACKAGE-NAME "CURLY" :EXTERNALP T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION "Disable curly syntax for current file.

Warning: Calling DISABLE-CURLY-SYNTAX when curly syntax is not
enabled can give funny results.  Also, reading multiple files
using ENABLE-CURLY-SYNTAX and DISABLE-CURLY-SYNTAX in different
threads can invoke a disaster.  ENABLE-CURLY-SYNTAX itself is
safe."))
   :EXTERNAL-SYMBOLS
   ((:NAME "SQUARE-READER" :PACKAGE-NAME "CURLY" :EXTERNALP T)
    (:NAME "MAKE-CURLY-READTABLE" :PACKAGE-NAME "CURLY" :EXTERNALP T)
    (:NAME "DISABLE-CURLY-SYNTAX" :PACKAGE-NAME "CURLY" :EXTERNALP T)
    (:NAME "*BLANK-ARGUMENT*" :PACKAGE-NAME "CURLY" :EXTERNALP T)
    (:NAME "CURLY-READER" :PACKAGE-NAME "CURLY" :EXTERNALP T)
    (:NAME "ENABLE-CURLY-SYNTAX" :PACKAGE-NAME "CURLY" :EXTERNALP T)))))
