
(:TYPE :SYSTEM :NAME "cl-quickcheck" :AUTHOR
 "Andrew Pennebaker <andrew.pennebaker@gmail.com>" :MAINTAINER NIL :VERSION
 "0.4" :LICENCE "MIT" :DESCRIPTION
 "Common Lisp port of the QuickCheck unit test framework" :LONG-DESCRIPTION NIL
 :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-QUICKCHECK" :FULL-NAME "CL-QUICKCHECK"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :MACRO :SYMBOL
     (:NAME "QUICKCHECK" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL))
     :DOCUMENTATION "Run BODY and report the results of any tests.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "TEST" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "FLAG" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL))
     :DOCUMENTATION "Test that FLAG is true.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "IS" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "FN" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "OPERANDS" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL))
     :DOCUMENTATION
     "Test that FN applied to OPERANDS is true, with the failure message
detailing the failing arguments.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ISNT" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "FN" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "OPERANDS" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL))
     :DOCUMENTATION "Test that FN applied to OPERANDS is false.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "IS=" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "X" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL)
      (:NAME "Y" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL))
     :DOCUMENTATION "Test that X is EQUAL to Y.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ISNT=" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "X" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL)
      (:NAME "Y" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL))
     :DOCUMENTATION "Test that X is not EQUAL to Y.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "IS-MACRO" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "FORM" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL)
