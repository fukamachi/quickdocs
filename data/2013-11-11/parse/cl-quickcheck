    (:TYPE :MACRO :SYMBOL
     (:NAME "SHOULD-SIGNAL" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "CONDITION" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
