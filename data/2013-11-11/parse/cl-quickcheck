    (:TYPE :FUNCTION :SYMBOL
     (:NAME "DEFAULT-GENERATOR" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "NAME" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL))
     :DOCUMENTATION
     "Give a generator expression for a name that's missing an explicit one.
You'll have to define the meaning of this shorthand elsewhere.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CONCAT-SYMBOL" :PACKAGE-NAME "CL-QUICKCHECK" :EXTERNALP NIL)
     :LAMBDA-LIST
