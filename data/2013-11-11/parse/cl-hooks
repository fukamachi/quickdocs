    (:TYPE :MACRO :SYMBOL
     (:NAME "WITH-HANDLERS" :PACKAGE-NAME "HOOKS" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "HOOKS-AND-HANDLERS" :PACKAGE-NAME "HOOKS" :EXTERNALP NIL)
