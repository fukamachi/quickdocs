   NIL :SYMBOL-LIST
   ((:TYPE :MACRO :SYMBOL
     (:NAME "MACRO-LEVEL" :PACKAGE-NAME "MACRO-LEVEL" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "MACRO-LEVEL" :EXTERNALP NIL))
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "MACRO-LEVEL" :PACKAGE-NAME "MACRO-LEVEL" :EXTERNALP T)))))
