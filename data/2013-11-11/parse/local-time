      "LOCAL-TIME" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(COERCE
 (CONS 0
       (LOOP LOCAL-TIME::WITH
             LOCAL-TIME::SUM = 0
             LOCAL-TIME::FOR LOCAL-TIME::DAYS :ACROSS LOCAL-TIME::+ROTATED-MONTH-DAYS-WITHOUT-LEAP-DAY+
             LOCAL-TIME::COLLECT (INCF LOCAL-TIME::SUM LOCAL-TIME::DAYS)))
 '(SIMPLE-ARRAY FIXNUM (*)))")
