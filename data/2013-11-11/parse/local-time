    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*DEFAULT-TIMEZONE-REPOSITORY-PATH*" :PACKAGE-NAME "LOCAL-TIME"
      :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "(FLET ((LOCAL-TIME::TRY (LOCAL-TIME::PROJECT-HOME-DIRECTORY)
         (WHEN LOCAL-TIME::PROJECT-HOME-DIRECTORY
           (IGNORE-ERRORS
            (TRUENAME
             (MERGE-PATHNAMES \"zoneinfo/\"
                              (MAKE-PATHNAME :DIRECTORY
                                             (PATHNAME-DIRECTORY
                                              LOCAL-TIME::PROJECT-HOME-DIRECTORY))))))))
  (OR
   (WHEN (FIND-PACKAGE \"ASDF\")
     (LET ((LOCAL-TIME::PATH
            (EVAL
             (READ-FROM-STRING
              \"(let ((system (asdf:find-system :local-time nil)))
                                (when system
                                  (asdf:component-pathname system)))\"))))
       (LOCAL-TIME::TRY LOCAL-TIME::PATH)))
   (LET ((LOCAL-TIME::PATH
          (OR
           #P\"/home/quickdocs_dev/quicklisp/dists/quicklisp/software/local-time-20131111-git/src/local-time.lisp\"
           *LOAD-TRUENAME*)))
     (WHEN LOCAL-TIME::PATH
       (LOCAL-TIME::TRY (MERGE-PATHNAMES \"../\" LOCAL-TIME::PATH))))))")
