      (:NAME "OMIT-DATE-PART" :PACKAGE-NAME "LOCAL-TIME" :EXTERNALP NIL)
      (:NAME "OMIT-TIME-PART" :PACKAGE-NAME "LOCAL-TIME" :EXTERNALP NIL)
      ((:NAME "OMIT-TIMEZONE-PART" :PACKAGE-NAME "LOCAL-TIME" :EXTERNALP NIL)
       (:NAME "OMIT-TIME-PART" :PACKAGE-NAME "LOCAL-TIME" :EXTERNALP NIL))
      ((:NAME "USE-ZULU" :PACKAGE-NAME "LOCAL-TIME" :EXTERNALP NIL)
       (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      ((:NAME "TIMEZONE" :PACKAGE-NAME "LOCAL-TIME" :EXTERNALP NIL)
