
(:TYPE :SYSTEM :NAME "cl-base32" :AUTHOR NIL :MAINTAINER NIL :VERSION "0.1"
 :LICENCE NIL :DESCRIPTION NIL :LONG-DESCRIPTION NIL :DEPENDS-ON NIL
 :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "-ASD" :FULL-NAME "CL-BASE32-ASD" :DOCUMENTATION NIL
   :SYMBOL-LIST NIL :EXTERNAL-SYMBOLS NIL)
  (:TYPE :PACKAGE :NAME "CL-BASE32" :FULL-NAME "CL-BASE32" :DOCUMENTATION
   "This package implements the base32 encoding/decoding defined in RF 4648."
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*BASE32-ALPHABET*" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "\"abcdefghijklmnopqrstuvwxyz234567\"")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ENCODE-WORD" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "A-WORD" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return the digit in the base32 alphabet corresponding to a word")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "DECODE-WORD" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "A-DIGIT" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return the word encoded as a digit in the base32 alphabet")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-WORD" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "SOME-BYTES" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL)
      (:NAME "WORD-INDEX" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return the word (a 5-bit integer) found in some-bytes located at word-index")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WRITE-WORD" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "SOME-BYTES" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL)
      (:NAME "WORD-INDEX" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL)
      (:NAME "WORD" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL))
     :DOCUMENTATION
     "Write the word into the bits located at word-index in some-bytes")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "UNPADDED-BASE32-LENGTH" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "BASE32-STRING" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL))
     :DOCUMENTATION
     "Given a base32 string, compute the size of the raw base32 string,
   without any = padding
  ")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "BYTE-LENGTH-FROM-BASE32" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "BASE32-STRING" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL))
     :DOCUMENTATION "Given a base32 string, compute the number of bytes in the 
   decoded data
  ")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "BASE32-LENGTH-FROM-BYTES" :PACKAGE-NAME "CL-BASE32" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     ((:NAME "SOME-BYTES" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL))
     :DOCUMENTATION "Given bytes of unencoded data, determine the length of the
   corresponding base32-encoded string
  ")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "BYTES-TO-BASE32" :PACKAGE-NAME "CL-BASE32" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "SOME-BYTES" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return a base32 string encoding of the provided vector of bytes")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "BASE32-TO-BYTES" :PACKAGE-NAME "CL-BASE32" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "BASE32-STRING" :PACKAGE-NAME "CL-BASE32" :EXTERNALP NIL))
     :DOCUMENTATION
     "Return the bytes decoded from the supplied base32 string"))
   :EXTERNAL-SYMBOLS
   ((:NAME "BASE32-TO-BYTES" :PACKAGE-NAME "CL-BASE32" :EXTERNALP T)
    (:NAME "BYTES-TO-BASE32" :PACKAGE-NAME "CL-BASE32" :EXTERNALP T)))))
