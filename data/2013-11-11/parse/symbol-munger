
(:TYPE :SYSTEM :NAME "symbol-munger" :AUTHOR NIL :MAINTAINER NIL :VERSION "0.1"
 :LICENCE "BSD" :DESCRIPTION "Functions to convert between the spacing and
  capitalization conventions of various environments"
 :LONG-DESCRIPTION NIL :DEPENDS-ON ("ITERATE" "ALEXANDRIA") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "SYMBOL-MUNGER" :FULL-NAME "SYMBOL-MUNGER"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :METHOD :SYMBOL
     (:NAME "%COERCE-TO-STRING" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "S" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL))
     :DOCUMENTATION "This method can be specialized to help turn objects into
   strings so they can be combined and normalized correctly")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ENSURE-LIST!" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "PLACE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "NORMALIZE-CAPITALIZATION-AND-SPACING" :PACKAGE-NAME
      "SYMBOL-MUNGER" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "S" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "CAPITALIZE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
       (:NAME "EACH-WORD" :PACKAGE-NAME "KEYWORD" :EXTERNALP T))
      ((:NAME "WORD-SEPARATORS" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
       #\ )
      (:NAME "WORD-SEPARATORS-TO-REPLACE" :PACKAGE-NAME "SYMBOL-MUNGER"
       :EXTERNALP NIL)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "IN-PLACE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL))
     :DOCUMENTATION
     "Will recapitalize a string and replace word-separators with a standard one
   (in-place if desired and possible)

   If s is a lisp tree, then each part will be %coerce-to-string'ed and treated
   as a separate part of the phrase being normalized

   Will write to a stream if given otherwise it.
   Defaults to capitalizing each word but can be any of
     {:each-word :first-word T (:all is an alias for T) nil :but-first-word (likeJavaScript) }

   word-separators are used to distinguish new words for the purposes of capitalization
     The first of these will be used to replace word-separators-to-replace
   word-separators-to-replace helps normalize word separators so that spaces or underscores
     become the appropriate word-separator.
     If this eql :capitals it assumes capital letters indicate a new word separation

   returns a string (new or the one passed in if in-place) unless :stream is provided")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ENGLISH->LISP-NAME" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PHRASE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CAPITALIZE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL))
     :DOCUMENTATION
     "Turns an english phrase into a string containing a common lisp style symbol-name")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ENGLISH->LISP-SYMBOL" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PHRASE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION
     "Turns an english phrase into a common lisp symbol in the specified package")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ENGLISH->KEYWORD" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PHRASE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL))
     :DOCUMENTATION "Turns an english phrase into a common lisp keyword")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ENGLISH->CAMEL-CASE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PHRASE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "CAPITALIZE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
       (:NAME "BUT-FIRST-WORD" :PACKAGE-NAME "KEYWORD" :EXTERNALP T)))
     :DOCUMENTATION "Turns an english phrase into a camelCasePhraseLikeThis ")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ENGLISH->STUDLY-CASE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PHRASE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Turns an english phrase into a CamelCasePhraseLikeThis")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ENGLISH->UNDERSCORES" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PHRASE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CAPITALIZE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL))
     :DOCUMENTATION "Turns an english phrase into a a_phrase_like_this")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "LISP->ENGLISH" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PHRASE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "CAPITALIZE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
       (:NAME "EACH-WORD" :PACKAGE-NAME "KEYWORD" :EXTERNALP T))
      ((:NAME "WORD-SEPARATOR" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
       #\ ))
     :DOCUMENTATION
     "Converts a common lisp symbol (or symbol-name) into an english phrase")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "LISP->KEYWORD" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PHRASE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "COMBINE-SYMBOLS" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "PHRASE" :PACKAGE-NAME "SYMBOL-MUNGER" :EXTERNALP NIL)
