       (*COMPILE-PRINT* (WHEN NAIVE-PARSER-GENERATOR::*DEBUG* T)))
   (NAIVE-PARSER-GENERATOR:RESET-GRAMMAR)
   (FORMAT T \"~&creating e-mail address grammar...~%\")
   (MIME4CL::POPULATE-GRAMMAR)
   (LET ((NAIVE-PARSER-GENERATOR:GRAMMAR
          (NAIVE-PARSER-GENERATOR:GENERATE-GRAMMAR #'STRING=)))
     (NAIVE-PARSER-GENERATOR:RESET-GRAMMAR)
     (NAIVE-PARSER-GENERATOR:PRINT-GRAMMAR-FIGURES
      NAIVE-PARSER-GENERATOR:GRAMMAR)
     NAIVE-PARSER-GENERATOR:GRAMMAR)))")
