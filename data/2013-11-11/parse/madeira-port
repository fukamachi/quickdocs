    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FIND-FUNCTION-FEATUREP" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     ((:NAME "SYMBOL-NAME" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "PACKAGE-NAME" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "ALLOW-INTERNAL" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FIND-MACRO-FEATUREP" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP NIL)
