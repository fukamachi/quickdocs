     ((:NAME "VALUE-EXPR" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP NIL)
      (:NAME "TYPE-EXPR" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FIND-PACKAGE-FEATUREP" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP
      NIL)
     :LAMBDA-LIST ((:NAME "NAME" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "QUOTE-FEATUREP" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP NIL)
