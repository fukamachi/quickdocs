    (:TYPE :FUNCTION :SYMBOL
     (:NAME "EQUALP-FEATUREP" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "FEATURE1" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP NIL)
      (:NAME "FEATURE2" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TYPEP-FEATUREP" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP NIL)
