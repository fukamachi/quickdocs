     ((:NAME "FEATURE" :PACKAGE-NAME "ASDF/OPERATION" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "OR-FEATUREP" :PACKAGE-NAME "MADEIRA-PORT" :EXTERNALP NIL)
