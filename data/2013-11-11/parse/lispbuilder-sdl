    (:TYPE :FUNCTION :SYMBOL
     (:NAME "GFX-DRAW-CHARACTER-SOLID-*" :PACKAGE-NAME "LISPBUILDER-SDL"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "C" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP NIL)
