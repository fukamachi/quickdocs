    (:TYPE :FUNCTION :SYMBOL
     (:NAME "GFX-DRAW-CHARACTER-SHADED-*" :PACKAGE-NAME "LISPBUILDER-SDL"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "C" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP NIL)
