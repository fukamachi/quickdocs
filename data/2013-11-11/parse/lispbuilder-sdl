       (:NAME "COLOR" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP T))
      ((:NAME "BG-COLOR" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP NIL)
       (:NAME "COLOR" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP T))
      ((:NAME "FONT" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP NIL)
       (:NAME "FONT" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP NIL))
      (:NAME "FREE" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP T)
      (:NAME "CACHE" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "_RENDER-STRING-SHADED_" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     (((:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      ((:NAME "FG-COLOR" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP NIL)
