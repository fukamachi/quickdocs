    (:TYPE :CLASS :SYMBOL
     (:NAME "FOREIGN-OBJECT" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP NIL)
     :SUPER-CLASS-LIST NIL :DOCUMENTATION NIL :SLOT-LIST
