     (:NAME "*EVENTS*" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(LISPBUILDER-SDL::CREATE-EVENTS
 (LIST
  (LIST :ACTIVE-EVENT
        (LISPBUILDER-SDL::CREATE-EVENT
         (LIST (LIST :GAIN 'LISPBUILDER-SDL::ACTIVE-GAIN)
               (LIST :STATE 'LISPBUILDER-SDL::ACTIVE-STATE))))
  (LIST :KEY-DOWN-EVENT
        (LISPBUILDER-SDL::CREATE-EVENT
         (LIST (LIST :STATE 'LISPBUILDER-SDL::KEY-STATE)
               (LIST :SCANCODE 'LISPBUILDER-SDL::KEY-SCANCODE)
               (LIST :KEY 'LISPBUILDER-SDL::KEY-KEY)
               (LIST :MOD 'LISPBUILDER-SDL::KEY-MOD)
               (LIST :MOD-KEY 'LISPBUILDER-SDL::KEY-MOD-KEY)
               (LIST :UNICODE 'LISPBUILDER-SDL::KEY-UNICODE))))
  (LIST :KEY-UP-EVENT
        (LISPBUILDER-SDL::CREATE-EVENT
         (LIST (LIST :STATE 'LISPBUILDER-SDL::KEY-STATE)
               (LIST :SCANCODE 'LISPBUILDER-SDL::KEY-SCANCODE)
               (LIST :KEY 'LISPBUILDER-SDL::KEY-KEY)
               (LIST :MOD 'LISPBUILDER-SDL::KEY-MOD)
               (LIST :MOD-KEY 'LISPBUILDER-SDL::KEY-MOD-KEY)
               (LIST :UNICODE 'LISPBUILDER-SDL::KEY-UNICODE))))
  (LIST :MOUSE-MOTION-EVENT
        (LISPBUILDER-SDL::CREATE-EVENT
         (LIST (LIST :STATE 'LISPBUILDER-SDL::MOUSE-MOTION-BUTTON)
               (LIST :X 'LISPBUILDER-SDL::MOUSE-MOTION-X)
               (LIST :Y 'LISPBUILDER-SDL::MOUSE-MOTION-Y)
               (LIST :X-REL 'LISPBUILDER-SDL::MOUSE-MOTION-X-REL)
               (LIST :Y-REL 'LISPBUILDER-SDL::MOUSE-MOTION-Y-REL))))
  (LIST :MOUSE-BUTTON-DOWN-EVENT
        (LISPBUILDER-SDL::CREATE-EVENT
         (LIST (LIST :BUTTON 'LISPBUILDER-SDL::MOUSE-BUTTON-BUTTON)
               (LIST :STATE 'LISPBUILDER-SDL::MOUSE-BUTTON-STATE)
               (LIST :X 'LISPBUILDER-SDL::MOUSE-BUTTON-X)
               (LIST :Y 'LISPBUILDER-SDL::MOUSE-BUTTON-Y))))
  (LIST :MOUSE-BUTTON-UP-EVENT
        (LISPBUILDER-SDL::CREATE-EVENT
         (LIST (LIST :BUTTON 'LISPBUILDER-SDL::MOUSE-BUTTON-BUTTON)
               (LIST :STATE 'LISPBUILDER-SDL::MOUSE-BUTTON-STATE)
               (LIST :X 'LISPBUILDER-SDL::MOUSE-BUTTON-X)
               (LIST :Y 'LISPBUILDER-SDL::MOUSE-BUTTON-Y))))
  (LIST :JOY-AXIS-MOTION-EVENT
        (LISPBUILDER-SDL::CREATE-EVENT
         (LIST (LIST :WHICH 'LISPBUILDER-SDL::JOY-AXIS-MOTION-WHICH)
               (LIST :AXIS 'LISPBUILDER-SDL::JOY-AXIS-MOTION-AXIS)
               (LIST :VALUE 'LISPBUILDER-SDL::JOY-AXIS-MOTION-VALUE))))
  (LIST :JOY-BUTTON-DOWN-EVENT
        (LISPBUILDER-SDL::CREATE-EVENT
         (LIST (LIST :WHICH 'LISPBUILDER-SDL::JOY-BUTTON-WHICH)
               (LIST :BUTTON 'LISPBUILDER-SDL::JOY-BUTTON-BUTTON)
               (LIST :STATE 'LISPBUILDER-SDL::JOY-BUTTON-STATE))))
  (LIST :JOY-BUTTON-UP-EVENT
        (LISPBUILDER-SDL::CREATE-EVENT
         (LIST (LIST :WHICH 'LISPBUILDER-SDL::JOY-BUTTON-WHICH)
               (LIST :BUTTON 'LISPBUILDER-SDL::JOY-BUTTON-BUTTON)
               (LIST :STATE 'LISPBUILDER-SDL::JOY-BUTTON-STATE))))
  (LIST :JOY-HAT-MOTION-EVENT
