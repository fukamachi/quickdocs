    (:TYPE :FUNCTION :SYMBOL
     (:NAME "GET-FONT-FACES" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
