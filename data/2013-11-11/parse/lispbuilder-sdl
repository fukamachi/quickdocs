    (:TYPE :FUNCTION :SYMBOL
     (:NAME "INIT-SUBSYSTEMS" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "SUBSYSTEMS" :PACKAGE-NAME "LISPBUILDER-SDL" :EXTERNALP NIL)
