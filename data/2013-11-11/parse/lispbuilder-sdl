         (LIST (LIST :WHICH 'LISPBUILDER-SDL::JOY-HAT-MOTION-WHICH)
               (LIST :AXIS 'LISPBUILDER-SDL::JOY-HAT-MOTION-AXIS)
               (LIST :VALUE 'LISPBUILDER-SDL::JOY-HAT-MOTION-VALUE))))
  (LIST :JOY-BALL-MOTION-EVENT
        (LISPBUILDER-SDL::CREATE-EVENT
         (LIST (LIST :WHICH 'LISPBUILDER-SDL::JOY-BALL-MOTION-WHICH)
               (LIST :BALL 'LISPBUILDER-SDL::JOY-BALL-MOTION-BALL)
               (LIST :X-REL 'LISPBUILDER-SDL::JOY-BALL-MOTION-X-REL)
               (LIST :Y-REL 'LISPBUILDER-SDL::JOY-BALL-MOTION-Y-REL))))
  (LIST :VIDEO-RESIZE-EVENT
        (LISPBUILDER-SDL::CREATE-EVENT
         (LIST (LIST :W 'LISPBUILDER-SDL::VIDEO-RESIZE-W)
               (LIST :H 'LISPBUILDER-SDL::VIDEO-RESIZE-H))))
  (LIST :VIDEO-EXPOSE-EVENT (LISPBUILDER-SDL::CREATE-EVENT NIL))
  (LIST :SYS-WM-EVENT (LISPBUILDER-SDL::CREATE-EVENT NIL))
  (LIST :USER-EVENT
        (LISPBUILDER-SDL::CREATE-EVENT
         (LIST (LIST :TYPE 'LISPBUILDER-SDL::USER-TYPE)
               (LIST :CODE 'LISPBUILDER-SDL::USER-CODE)
               (LIST :DATA1 'LISPBUILDER-SDL::USER-DATA1)
               (LIST :DATA2 'LISPBUILDER-SDL::USER-DATA2))))))")
