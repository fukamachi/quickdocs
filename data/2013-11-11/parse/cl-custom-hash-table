 :DESCRIPTION NIL :LONG-DESCRIPTION
 "This library allows creation of hash tables with arbitrary TEST/HASH functions,
in addition to the TEST functions allowed by the standard (EQ, EQL, EQUAL and EQUALP),
even in implementations that don't support this functionality directly."
 :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-CUSTOM-HASH-TABLE" :FULL-NAME
   "CL-CUSTOM-HASH-TABLE" :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :MACRO :SYMBOL
     (:NAME "CHECKING-READER-CONDITIONALS" :PACKAGE-NAME "CL-CUSTOM-HASH-TABLE"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "&WHOLE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "WHOLE" :PACKAGE-NAME "CL-CUSTOM-HASH-TABLE" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "CL-CUSTOM-HASH-TABLE" :EXTERNALP NIL))
     :DOCUMENTATION
     "Break unless the body contains exactly one form. Inspired by code from Steve Haflich.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "DEFINE-CUSTOM-HASH-TABLE-CONSTRUCTOR" :PACKAGE-NAME
      "CL-CUSTOM-HASH-TABLE" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "MAKE" :PACKAGE-NAME "CL-CUSTOM-HASH-TABLE" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "TEST" :PACKAGE-NAME "CL-CUSTOM-HASH-TABLE" :EXTERNALP NIL)
      (:NAME "HASH-FUNCTION" :PACKAGE-NAME "CL-CUSTOM-HASH-TABLE" :EXTERNALP
       NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "WITH-CUSTOM-HASH-TABLE" :PACKAGE-NAME "CL-CUSTOM-HASH-TABLE"
      :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "CL-CUSTOM-HASH-TABLE" :EXTERNALP NIL))
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "DEFINE-CUSTOM-HASH-TABLE-CONSTRUCTOR" :PACKAGE-NAME
     "CL-CUSTOM-HASH-TABLE" :EXTERNALP T)
    (:NAME "WITH-CUSTOM-HASH-TABLE" :PACKAGE-NAME "CL-CUSTOM-HASH-TABLE"
     :EXTERNALP T)))))
