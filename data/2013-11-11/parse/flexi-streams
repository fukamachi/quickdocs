     (:NAME "FLEXI-IO-STREAM" :PACKAGE-NAME "FLEXI-STREAMS" :EXTERNALP T)
     :SUPER-CLASS-LIST
     ((:NAME "FLEXI-INPUT-STREAM" :PACKAGE-NAME "FLEXI-STREAMS" :EXTERNALP T)
