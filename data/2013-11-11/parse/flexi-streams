     (:NAME "WITH-OUTPUT-TO-SEQUENCE" :PACKAGE-NAME "FLEXI-STREAMS" :EXTERNALP
      T)
     :LAMBDA-LIST
     (((:NAME "VAR" :PACKAGE-NAME "FLEXI-STREAMS" :EXTERNALP NIL)
