     :INITIAL-VALUE "(IF (BOUNDP 'FLEXI-STREAMS::+ISO-8859-HASHES+)
    (SYMBOL-VALUE 'FLEXI-STREAMS::+ISO-8859-HASHES+)
    (LOOP FLEXI-STREAMS::FOR (FLEXI-STREAMS::NAME
                              . FLEXI-STREAMS::TABLE) FLEXI-STREAMS::IN FLEXI-STREAMS::+ISO-8859-TABLES+
          FLEXI-STREAMS::COLLECT (CONS FLEXI-STREAMS::NAME
                                       (FLEXI-STREAMS::INVERT-TABLE
                                        FLEXI-STREAMS::TABLE))))")
