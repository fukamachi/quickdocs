    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PIPE-APPEND" :PACKAGE-NAME "PIPES" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "X" :PACKAGE-NAME "PIPES" :EXTERNALP NIL)
      (:NAME "Y" :PACKAGE-NAME "PIPES" :EXTERNALP NIL))
     :DOCUMENTATION "Return a pipe that appends the elements of x and y.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PIPE-MAPPEND" :PACKAGE-NAME "PIPES" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "FN" :PACKAGE-NAME "PIPES" :EXTERNALP NIL)
      (:NAME "PIPE" :PACKAGE-NAME "PIPES" :EXTERNALP NIL))
     :DOCUMENTATION "Lazily map fn over pipe, appending results.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PIPE-MAPPEND-FILTERING" :PACKAGE-NAME "PIPES" :EXTERNALP T)
