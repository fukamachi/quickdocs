    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PIPE-FORCE" :PACKAGE-NAME "PIPES" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "PIPE" :PACKAGE-NAME "PIPES" :EXTERNALP NIL)) :DOCUMENTATION
     "Force the enumeration of all of the pipe. Never returns
if the pipe is infinite in length.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PIPE-FILTER" :PACKAGE-NAME "PIPES" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "PREDICATE" :PACKAGE-NAME "PIPES" :EXTERNALP NIL)
      (:NAME "PIPE" :PACKAGE-NAME "PIPES" :EXTERNALP NIL))
     :DOCUMENTATION "Keep only items in pipe satisfying predicate.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PIPE-MAP" :PACKAGE-NAME "PIPES" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "FN" :PACKAGE-NAME "PIPES" :EXTERNALP NIL)
      (:NAME "PIPE" :PACKAGE-NAME "PIPES" :EXTERNALP NIL))
     :DOCUMENTATION "Map fn over pipe, delaying all but the first fn call.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PIPE-MAP-FILTERING" :PACKAGE-NAME "PIPES" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "FN" :PACKAGE-NAME "PIPES" :EXTERNALP NIL)
      (:NAME "PIPE" :PACKAGE-NAME "PIPES" :EXTERNALP NIL)
