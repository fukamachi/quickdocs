
(:TYPE :SYSTEM :NAME "local-package-aliases" :AUTHOR
 "Anton Vodonosov <avodonosov@yandex.ru>" :MAINTAINER NIL :VERSION "0.0.1"
 :LICENCE "MIT" :DESCRIPTION
 "Allows to define inside of a package aliases to refer other packages.
Provides a reader macro $ to use the aliases (active only in packages having
alias mapping, and does not affect other code).
Portable."
 :LONG-DESCRIPTION NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "LOCAL-PACKAGE-ALIASES" :FULL-NAME
   "LOCAL-PACKAGE-ALIASES" :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "ERR" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "FORMAT-CONTROL" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
       NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "FORMAT-ARGUMENTS" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
       :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*PACKAGE-TO-ALIASES-MAP*" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
      :EXTERNALP NIL)
     :DOCUMENTATION
     "Mapping from package object to a hash-table of local aliases active in this package.
The hash-table of local aliases maps string alias to a package designator."
     :INITIAL-VALUE "(MAKE-HASH-TABLE :TEST #'EQ)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ALIAS-TABLE-FOR" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
      NIL)
     :LAMBDA-LIST ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "HAS-LOCAL-ALIASES-P" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
      :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SET-ALIAS-TABLE-FOR" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "FOR-PACKAGE" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
       NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "PACKAGE-ALIAS-PAIRS" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
       :EXTERNALP NIL))
     :DOCUMENTATION
     "PACKAGE-ALIAS-PAIRS is a list in the form (package-designator alias-string package-designator alias-string ...)")
    (:TYPE :MACRO :SYMBOL
     (:NAME "SET" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "PACKAGE-ALIAS-PAIRS" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
       :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FIND-ALIASED-PACKAGE" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "ALIAS" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ALIASES-OF" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "IN-PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FIND-ALIASED-SYMBOL" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "TOKEN" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP NIL))
     :DOCUMENTATION
     "TOEKN is a string in the form alias:symbol or alias::symbol.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WHITESPACE-P" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
      NIL)
     :LAMBDA-LIST ((:NAME "CHAR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TERMINATING-MACRO-CHAR-P" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
      :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "CHAR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TERMINATOR-P" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
      NIL)
     :LAMBDA-LIST ((:NAME "CHAR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "APPLY-CASE-MODE" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     ((:NAME "READTABLE-CASE-MODE" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
       :EXTERNALP NIL)
      (:NAME "STR" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-TOKEN" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-PACKAGE-ALIASED-SYMBOL" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CHAR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "ORIGINAL-READTABLE" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
       :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SET-ALIASING-READER" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
      :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "TO-READTABLE" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
       NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "MACRO-CHAR" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
        NIL)
       #\$)
      (:NAME "DEFAULT-READTABLE" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
       :EXTERNALP NIL))
     :DOCUMENTATION
     "Modifies TO-READTABLE so that MACRO-CHAR at the beginning a token in
form $ALIAS:SYMBOL or $ALIAS::SYMBOL is used to refere other packages,
according to the aliases set in the current packges by LOCAL-PACKAGE-ALIASES:SET.

The DEFAULT-READTABLE is used when the current package has no aliases.
In this case the hangler for MACRO-CHAR is retrieved from DEFAULT-READTABLE
and temporary applied to the current readtable using CL:SET-SYNTAX-FROM-CHAR.

The default value for DEFAULT-READTABLE is copy of TO-READTABLE before
it's syntax is modified.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ALIASING-READTABLE" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "PROTOTYPE-READTABLE" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
        :EXTERNALP NIL)
       (:NAME "*READTABLE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      ((:NAME "MACRO-CHAR" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
        NIL)
       #\$))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CALL-WITH-ALIASING-READTABLE" :PACKAGE-NAME
      "LOCAL-PACKAGE-ALIASES" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "THUNK" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP NIL))
     :DOCUMENTATION
     "Convenience function to use in ASDF's :around-compile argument.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CALL-WITH-NICKNAMES" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
      :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "ALIAS-TABLE" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
       NIL)
      (:NAME "FN" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP NIL))
     :DOCUMENTATION "Helper function useful to hook into SLIME
in order to provide completion, go-to-definition,
function parameters help and other SLIME support.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SWANK-BUFFER-PACKAGE" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
      :EXTERNALP NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WITH-ALIASES-AS-NICKNAMES-HOOK" :PACKAGE-NAME
      "LOCAL-PACKAGE-ALIASES" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "NEXT-FN" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "HOOK-INTO-SWANK" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
      T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "UNHOOK-FROM-SWANK" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
      :EXTERNALP T)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION
     "Returns T if the hook handler was uninstalled, and NIL otherwize."))
   :EXTERNAL-SYMBOLS
   ((:NAME "UNHOOK-FROM-SWANK" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
     T)
    (:NAME "CALL-WITH-ALIASING-READTABLE" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
     :EXTERNALP T)
    (:NAME "SET" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP T)
    (:NAME "HOOK-INTO-SWANK" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES" :EXTERNALP
     T)
    (:NAME "SET-ALIASING-READER" :PACKAGE-NAME "LOCAL-PACKAGE-ALIASES"
     :EXTERNALP T)))))
