    (:TYPE :FUNCTION :SYMBOL
     (:NAME "T-TEST-PAIRED" :PACKAGE-NAME "STATISTICS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "D-BAR" :PACKAGE-NAME "STATISTICS" :EXTERNALP NIL)
      (:NAME "SD" :PACKAGE-NAME "STATISTICS" :EXTERNALP T)
      (:NAME "N" :PACKAGE-NAME "STATISTICS" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
