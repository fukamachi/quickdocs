
(:TYPE :SYSTEM :NAME "enhanced-eval-when" :AUTHOR
 "Jean-Philippe Paradis <hexstream@gmail.com>" :MAINTAINER NIL :VERSION "1.0"
 :LICENCE "Public Domain" :DESCRIPTION
 "enhanced-eval-when provides an enhanced EVAL-WHEN macro that supports (eval-when t ...) as a shorthand for (eval-when (:compile-toplevel :load-toplevel :execute) ...), addressing concerns about verbosity. An ENHANCED-EVAL-WHEN alias is also supported, as well as an EVAL-ALWAYS macro and package nickname, for good measure."
 :LONG-DESCRIPTION NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "ENHANCED-EVAL-WHEN" :FULL-NAME "ENHANCED-EVAL-WHEN"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "%EXPAND" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "SITUATIONS" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP NIL)
      (:NAME "BODY" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "EVAL-WHEN" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "SITUATIONS" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "ENHANCED-EVAL-WHEN" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP
      T)
     :LAMBDA-LIST
     ((:NAME "SITUATIONS" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "EVAL-ALWAYS" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP NIL))
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "EVAL-ALWAYS" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP T)
    (:NAME "ENHANCED-EVAL-WHEN" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP
     T)
    (:NAME "EVAL-WHEN" :PACKAGE-NAME "ENHANCED-EVAL-WHEN" :EXTERNALP T)))))
