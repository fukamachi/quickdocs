  (:TYPE :PACKAGE :NAME "LEDGER-TEXTUAL" :FULL-NAME "LEDGER-TEXTUAL"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*DATE-REGEXP*" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "\"[0-9-./]+\"")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*SPACING-REGEXP*" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "(FORMAT NIL \"(?:  |~C| ~C)\\\\s*\" #\\Tab #\\Tab)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*COMMENT-REGEXP*" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "(FORMAT NIL \"(?:[ ~C]+;(.+))\" #\\Tab)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*ENTRY-HEADING-SCANNER*" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP
      NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(CL-PPCRE:CREATE-SCANNER
 (FORMAT NIL
         (CONCATENATE 'STRING \"^(?:(~A)(?:=(~A))?)\\\\s+(?:(\\\\*|!)\\\\s*)?\"
                      \"(?:\\\\((.+?)\\\\)\\\\s+)?(.+?)~A?$\")
         LEDGER-TEXTUAL::*DATE-REGEXP* LEDGER-TEXTUAL::*DATE-REGEXP*
         LEDGER-TEXTUAL::*COMMENT-REGEXP*))")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*TRANSACTION-SCANNER*" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP
      NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(CL-PPCRE:CREATE-SCANNER
 (FORMAT NIL
         (CONCATENATE 'STRING \"^\\\\s+(?:(\\\\*|!)\\\\s*)?([\\\\[(])?(.+?)([\\\\])])?\"
                      \"(?:~A(?:([^; ~C].*?)(?:(@@?)\\\\s*(.+?))?))?~A?$\")
         LEDGER-TEXTUAL::*SPACING-REGEXP* #\\Tab
         LEDGER-TEXTUAL::*COMMENT-REGEXP*))")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*DIRECTIVE-HANDLERS*" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP T)
     :DOCUMENTATION NIL :INITIAL-VALUE "`(((#\\; #\\* #\\% #\\#)
   ,@#'(LAMBDA (LEDGER-TEXTUAL::IN LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL)
         (DECLARE (IGNORE LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL))
         (READ-LINE LEDGER-TEXTUAL::IN NIL)
         1))
  (,#\\Return
   ,@#'(LAMBDA (LEDGER-TEXTUAL::IN LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL)
         (DECLARE (IGNORE LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL))
         (IF (EQ #\\Newline (PEEK-CHAR NIL LEDGER-TEXTUAL::IN NIL))
             (READ-CHAR LEDGER-TEXTUAL::IN NIL))
         1))
  (,#\\Newline
   ,@#'(LAMBDA (LEDGER-TEXTUAL::IN LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL)
         (DECLARE (IGNORE LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL))
         (READ-CHAR LEDGER-TEXTUAL::IN NIL)
         1))
  (,#\\Y
   ,@#'(LAMBDA (LEDGER-TEXTUAL::IN LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL)
         (DECLARE (IGNORE LEDGER-TEXTUAL::LINE))
         (READ-CHAR LEDGER-TEXTUAL::IN)
         (PEEK-CHAR T LEDGER-TEXTUAL::IN)
         (SETF (CL-LEDGER:JOURNAL-DEFAULT-YEAR CL-LEDGER:JOURNAL)
                 (READ-PRESERVING-WHITESPACE LEDGER-TEXTUAL::IN)
               (CL-LEDGER:JOURNAL-DATE-FORMAT CL-LEDGER:JOURNAL) \"%m/%d\")
         0))
  (,#\\A
   ,@#'(LAMBDA (LEDGER-TEXTUAL::IN LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL)
         (DECLARE (IGNORE LEDGER-TEXTUAL::LINE))
         (READ-CHAR LEDGER-TEXTUAL::IN)
         (PEEK-CHAR T LEDGER-TEXTUAL::IN)
         (SETF (CL-LEDGER:JOURNAL-DEFAULT-ACCOUNT CL-LEDGER:JOURNAL)
                 (CL-LEDGER:FIND-ACCOUNT CL-LEDGER:JOURNAL
                                         (READ-LINE LEDGER-TEXTUAL::IN)
                                         :CREATE-IF-NOT-EXISTS-P T))
         1))
  (,#\\D
   ,@#'(LAMBDA (LEDGER-TEXTUAL::IN LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL)
         (DECLARE (IGNORE LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL))
         (READ-CHAR LEDGER-TEXTUAL::IN)
         (PEEK-CHAR T LEDGER-TEXTUAL::IN)
         (CAMBL:READ-AMOUNT LEDGER-TEXTUAL::IN)
         0))
  (,#\\N
   ,@#'(LAMBDA (LEDGER-TEXTUAL::IN LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL)
         (DECLARE (IGNORE LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL))
         (READ-CHAR LEDGER-TEXTUAL::IN)
         (PEEK-CHAR T LEDGER-TEXTUAL::IN)
         (LET ((LEDGER-TEXTUAL::COMMODITY
                (CAMBL:FIND-COMMODITY (READ-LINE LEDGER-TEXTUAL::IN)
                                      :CREATE-IF-NOT-EXISTS-P T)))
           (ASSERT
            (NOT (CAMBL:ANNOTATED-COMMODITY-P LEDGER-TEXTUAL::COMMODITY)))
           (SETF (CAMBL:COMMODITY-NO-MARKET-PRICE-P LEDGER-TEXTUAL::COMMODITY)
                   T)
           1)))
  (,#'DIGIT-CHAR-P
   ,@#'(LAMBDA (LEDGER-TEXTUAL::IN LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL)
         (MULTIPLE-VALUE-BIND (CL-LEDGER:ENTRY LEDGER-TEXTUAL::LINES)
             (LEDGER-TEXTUAL::READ-PLAIN-ENTRY LEDGER-TEXTUAL::IN
                                               LEDGER-TEXTUAL::LINE
                                               CL-LEDGER:JOURNAL)
           (IF CL-LEDGER:ENTRY
               (CL-LEDGER:ADD-TO-CONTENTS CL-LEDGER:JOURNAL CL-LEDGER:ENTRY)
               (ERROR \"Failed to read entry at line ~S~%\"
                      LEDGER-TEXTUAL::LINE))
           LEDGER-TEXTUAL::LINES)))
  ((#\\@ #\\!)
   ,@#'(LAMBDA (LEDGER-TEXTUAL::IN LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL)
         (DECLARE (IGNORE CL-LEDGER:JOURNAL))
         (LET ((SYMBOL (READ-PRESERVING-WHITESPACE LEDGER-TEXTUAL::IN))
               (LEDGER-TEXTUAL::ARGUMENT (READ-LINE LEDGER-TEXTUAL::IN)))
           (IF (SYMBOLP SYMBOL)
               (PROGN
                (SETF SYMBOL
                        (FIND-SYMBOL
                         (FORMAT NIL \"ledger-text-directive/~A\"
                                 (SYMBOL-NAME SYMBOL))))
                (IF (AND SYMBOL (FUNCTIONP SYMBOL))
                    (FUNCALL
                     (LOCALLY
                      (DECLARE (MUFFLE-CONDITIONS CODE-DELETION-NOTE))
                      (FDEFINITION SYMBOL))
                     LEDGER-TEXTUAL::ARGUMENT)
                    (ERROR \"Unrecognized directive \\\"~S~\\\" at line ~D\"
                           LEDGER-TEXTUAL::ARGUMENT LEDGER-TEXTUAL::LINE)))
               (ERROR \"Unrecognized directive \\\"~S~\\\" at line ~D\"
                      LEDGER-TEXTUAL::ARGUMENT LEDGER-TEXTUAL::LINE))
           1)))
  (,#\\(
   ,@#'(LAMBDA (LEDGER-TEXTUAL::IN LEDGER-TEXTUAL::LINE CL-LEDGER:JOURNAL)
         (DECLARE (IGNORE CL-LEDGER:JOURNAL))
         (IF CL-LEDGER:*ALLOW-EMBEDDED-LISP*
             (LET ((LEDGER-TEXTUAL::BEGIN-POS
                    (FILE-POSITION LEDGER-TEXTUAL::IN))
                   LEDGER-TEXTUAL::END-POS
                   LEDGER-TEXTUAL::LINES)
               (EVAL (READ-PRESERVING-WHITESPACE LEDGER-TEXTUAL::IN))
               (SETF LEDGER-TEXTUAL::END-POS
                       (FILE-POSITION LEDGER-TEXTUAL::IN))
               (FILE-POSITION LEDGER-TEXTUAL::IN LEDGER-TEXTUAL::BEGIN-POS)
               (LOOP LEDGER-TEXTUAL::REPEAT (- LEDGER-TEXTUAL::END-POS
                                               LEDGER-TEXTUAL::BEGIN-POS)
                     DO (LET ((LEDGER-TEXTUAL::CH
                               (READ-CHAR LEDGER-TEXTUAL::IN))
                              LEDGER-TEXTUAL::FOUND)
                          (LOOP LEDGER-TEXTUAL::WHILE (MEMBER
                                                       LEDGER-TEXTUAL::CH
                                                       '(#\\Newline #\\Return))
                                DO (SETF LEDGER-TEXTUAL::FOUND T
                                         LEDGER-TEXTUAL::CH
                                           (READ-CHAR LEDGER-TEXTUAL::IN)))
                          (IF LEDGER-TEXTUAL::FOUND
                              (INCF LEDGER-TEXTUAL::LINES))))
               LEDGER-TEXTUAL::LINES)
             (ERROR \"Embedded Lisp not allowed at line ~D, ~
                            set LEDGER:*ALLOW-EMBEDDED-LISP*\"
                    LEDGER-TEXTUAL::LINE)))))")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-TRANSACTION" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "IN" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP NIL)
      (:NAME "LINE" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP NIL)
      (:NAME "ENTRY" :PACKAGE-NAME "CL-LEDGER" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-PLAIN-ENTRY" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "IN" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP NIL)
      (:NAME "LINE" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP NIL)
      (:NAME "JOURNAL" :PACKAGE-NAME "CL-LEDGER" :EXTERNALP T))
     :DOCUMENTATION
     "Read in the header line for the entry, which has the syntax:
  
    (DATE(=DATE)?)( (*|!))?( (((.+?))))? (.+)(:spacer:;(.+))?
  
  :spacer: means: two spaces, a tab, or a space and a tab, followed by any
  amount of whitespace.
  
  The groups identified in this regular expression (found in the scanner
  *entry-heading-scanner*) have these meanings:
  
  1 - The actual date of the entry.
  2 - The (optional) effective date of the entry.
  4 - The (optional) status of the entry: *=cleared, !=pending.
  6 - The (optional) \"code\" for the entry; has no meaning to Ledger.
  7 - The payee or description of the entry.
  9 - A comment giving further details about the entry.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-TEXTUAL-JOURNAL" :PACKAGE-NAME "LEDGER-TEXTUAL" :EXTERNALP
      NIL)
     :LAMBDA-LIST
