    (:TYPE :CONSTANT :SYMBOL
     (:NAME "+KECCAK-1600-LANE-BYTE-WIDTH+" :PACKAGE-NAME "SHA3" :EXTERNALP
      NIL)
     :DOCUMENTATION "Lane width in bytes for Keccak-1600." :INITIAL-VALUE
     "(TRUNCATE SHA3::+KECCAK-1600-LANE-WIDTH+ 8)")
    (:TYPE :TYPE :SYMBOL
     (:NAME "KECCAK-1600-LANE" :PACKAGE-NAME "SHA3" :EXTERNALP NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*KECCAK-F-ROUND-CONSTANTS*" :PACKAGE-NAME "SHA3" :EXTERNALP NIL)
     :DOCUMENTATION "Keccak Round Constants" :INITIAL-VALUE
     "(MAKE-ARRAY '(24) :ELEMENT-TYPE 'SHA3::KECCAK-1600-LANE :INITIAL-CONTENTS
            (LOOP SHA3::WITH SHA3::LFRSTATE = 1
                  SHA3::FOR SHA3::I SHA3::FROM 0 SHA3::BELOW 24
                  SHA3::COLLECT (LOOP SHA3::WITH SHA3::CONST = 0
                                      SHA3::FOR SHA3::J SHA3::FROM 0 SHA3::BELOW 7
                                      SHA3::FOR SHA3::BIT-POSITION = (1-
                                                                      (ASH 1
                                                                           SHA3::J))
                                      WHEN (LOGBITP 0 SHA3::LFRSTATE)
                                      DO (SETQ SHA3::CONST
                                                 (LOGXOR SHA3::CONST
                                                         (ASH 1
                                                              SHA3::BIT-POSITION)))
                                      DO (SETQ SHA3::LFRSTATE
                                                 (IF (LOGBITP 7 SHA3::LFRSTATE)
                                                     (LOGXOR
                                                      (LDB (BYTE 8 0)
                                                           (ASH SHA3::LFRSTATE
                                                                1))
                                                      113)
                                                     (ASH SHA3::LFRSTATE 1)))
                                      SHA3::FINALLY (RETURN SHA3::CONST))))")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*KECCAK-F-ROTATE-OFFSETS*" :PACKAGE-NAME "SHA3" :EXTERNALP NIL)
     :DOCUMENTATION "Keccak Rotate Offsets" :INITIAL-VALUE
     "(MAKE-ARRAY (LIST SHA3::+KECCAK-STATE-COLUMNS+ SHA3::+KECCAK-STATE-ROWS+)
            :ELEMENT-TYPE '(UNSIGNED-BYTE 8) :INITIAL-CONTENTS
            '((0 36 3 41 18) (1 44 10 45 2) (62 6 43 15 61) (28 55 25 21 56)
              (27 20 39 8 14)))")
