
(:TYPE :SYSTEM :NAME "cl-sentiment" :AUTHOR "Rob Blackwell" :MAINTAINER NIL
 :VERSION "0.0.1" :LICENCE NIL :DESCRIPTION
 "A sentiment analysis library for Common Lisp." :LONG-DESCRIPTION NIL
 :DEPENDS-ON ("CL-PPCRE" "RT") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-SENTIMENT" :FULL-NAME "CL-SENTIMENT" :DOCUMENTATION
   NIL :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*WORD-SCORES*" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "NIL")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WORDS" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "TEXT" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP NIL))
     :DOCUMENTATION "Returns a list of words in the given text using simplistic
tokenization.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "LOAD-WORD-SCORES" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "FILENAME" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP NIL))
     :DOCUMENTATION "Loads a word score table from a tab separated file.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "INITIALIZE" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FILENAME" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP NIL)
       ((:NAME "MERGE-PATHNAMES" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        ((:NAME "SYSTEM-SOURCE-DIRECTORY" :PACKAGE-NAME "ASDF/SYSTEM"
          :EXTERNALP T)
         (:NAME "CL-SENTIMENT" :PACKAGE-NAME "KEYWORD" :EXTERNALP T))
        "AFINN-111.txt")))
     :DOCUMENTATION
     "Initializes cl-sentiment with a default word-score table.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SENTIMENT" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "TEXT" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "TABLE" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP NIL)
       (:NAME "*WORD-SCORES*" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP NIL)))
     :DOCUMENTATION "Returns the sentiment measure for the given text."))
   :EXTERNAL-SYMBOLS
   ((:NAME "LOAD-WORD-SCORES" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP T)
    (:NAME "INITIALIZE" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP T)
    (:NAME "SENTIMENT" :PACKAGE-NAME "CL-SENTIMENT" :EXTERNALP T)))))
