    (:TYPE :FUNCTION :SYMBOL
     (:NAME "READ-UTF-8-STRING" :PACKAGE-NAME "TRIVIAL-UTF-8" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "INPUT" :PACKAGE-NAME "TRIVIAL-UTF-8" :EXTERNALP NIL)
