  (:TYPE :PACKAGE :NAME "-TOKENIZE" :FULL-NAME "LANGUTILS-TOKENIZE"
   :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "KNOWN-ABBREVIATIONS" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP
