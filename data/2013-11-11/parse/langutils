    (:TYPE :CONSTANT :SYMBOL
     (:NAME "NOUN-PATTERN" :PACKAGE-NAME "LANGUTILS" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(IF (BOUNDP 'LANGUTILS::NOUN-PATTERN)
    (SYMBOL-VALUE 'LANGUTILS::NOUN-PATTERN)
    '(OR
      (AND (LANGUTILS::? OR LANGUTILS::PDT)
           (OR LANGUTILS::DT LANGUTILS::PRP LANGUTILS::PRP$ LANGUTILS::WDT
               LANGUTILS::WP LANGUTILS::WP$)
           (* OR LANGUTILS::VBG LANGUTILS::VBD LANGUTILS::VBN LANGUTILS::JJ
              LANGUTILS::JJR LANGUTILS::JJS LANGUTILS::|,|)
           (+ OR LANGUTILS::NN LANGUTILS::NNS LANGUTILS::NNP LANGUTILS::NNPS
              LANGUTILS::CD))
      (AND (LANGUTILS::? OR LANGUTILS::PDT)
           (* OR LANGUTILS::JJ LANGUTILS::JJR LANGUTILS::JJS LANGUTILS::|,|)
           (+ OR LANGUTILS::NN LANGUTILS::NNS LANGUTILS::NNP LANGUTILS::NNPS
              LANGUTILS::CD))
      LANGUTILS::EX LANGUTILS::PRP LANGUTILS::WP LANGUTILS::WDT))")
    (:TYPE :CONSTANT :SYMBOL
     (:NAME "VERB-PATTERN" :PACKAGE-NAME "LANGUTILS" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(IF (BOUNDP 'LANGUTILS::VERB-PATTERN)
    (SYMBOL-VALUE 'LANGUTILS::VERB-PATTERN)
    '(AND (* OR LANGUTILS::RB LANGUTILS::RBR LANGUTILS::RBS LANGUTILS::WRB)
          (LANGUTILS::? OR LANGUTILS::MD)
          (* OR LANGUTILS::RB LANGUTILS::RBR LANGUTILS::RBS LANGUTILS::WRB)
          (+ OR LANGUTILS::VB LANGUTILS::VBD LANGUTILS::VBG LANGUTILS::VBP
             LANGUTILS::VBZ)
          (* OR LANGUTILS::VB LANGUTILS::VBD LANGUTILS::VBG LANGUTILS::VBN
             LANGUTILS::VBP LANGUTILS::VBZ LANGUTILS::RB LANGUTILS::RBR
             LANGUTILS::RBS LANGUTILS::WRB)
          (LANGUTILS::? OR LANGUTILS::RP)))")
