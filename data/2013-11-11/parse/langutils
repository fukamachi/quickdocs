     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION "Optimize the compiled closure through
   type and optimization declarations")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*CONTEXTUAL-RULE-ARGS*" :PACKAGE-NAME "LANGUTILS" :EXTERNALP NIL)
     :DOCUMENTATION
     "The templates for parsing contextual rules and constructing 
   matching templates over word/pos arrays"
     :INITIAL-VALUE "(LIST
 (LIST 'LANGUTILS::TAG1 'LANGUTILS::TAGS 'SYMBOL
       '(STDUTILS:MKKEYSYM (FOURTH LANGUTILS::PATTERN)))
 (LIST 'LANGUTILS::TAG2 'LANGUTILS::TAGS 'SYMBOL
       '(STDUTILS:MKKEYSYM (FIFTH LANGUTILS::PATTERN)))
 (LIST 'LANGUTILS::WORD1 'LANGUTILS::TOKENS 'FIXNUM
       '(LANGUTILS:ID-FOR-TOKEN (FOURTH LANGUTILS::PATTERN)))
 (LIST 'LANGUTILS::WORD2 'LANGUTILS::TOKENS 'FIXNUM
       '(LANGUTILS:ID-FOR-TOKEN (FIFTH LANGUTILS::PATTERN)))
 (LIST 'LANGUTILS::OLDTAG 'LANGUTILS::TAGS 'SYMBOL
       '(STDUTILS:MKKEYSYM (FIRST LANGUTILS::PATTERN)))
 (LIST 'LANGUTILS::NEWTAG 'LANGUTILS::TAGS 'SYMBOL
       '(STDUTILS:MKKEYSYM (SECOND LANGUTILS::PATTERN))))")
    (:TYPE :FUNCTION :SYMBOL
