      NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TOKENIZE-STREAM" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "BY-SENTENCE" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL)
       (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
      ((:NAME "FRAGMENT" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL) "")
      (:NAME "&AUX" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "INDEX" :PACKAGE-NAME "MY-META" :EXTERNALP T) 0)
      ((:NAME "START" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL) 0)
      ((:NAME "CH" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL) #\ )
      ((:NAME "WS" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL) #\ )
      ((:NAME "STATUS" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL)
       (:NAME "RUNNING" :PACKAGE-NAME "KEYWORD" :EXTERNALP T))
      ((:NAME "SENTENCE?" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL)
       (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION
     "Converts a stream into a string and tokenizes, optionally, one sentence 
   at a time which is nice for large files.  Pretty hairy code: a token 
   processor inside a stream scanner.  The stream scanner walks the input stream
   and tokenizes all punctuation (except periods).  After a sequences of 
   non-whitespace has been read, the inline tokenizer looks at the end of the 
   string for mis-tokenized words (can ' t -> ca n't)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TOKENIZE-STRING" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Returns a fresh, linguistically tokenized string")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TOKENIZE-FILE2" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "SOURCE-FILE" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL)
      (:NAME "TARGET-FILE" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "IF-EXISTS" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL)
       (:NAME "SUPERSEDE" :PACKAGE-NAME "KEYWORD" :EXTERNALP T))
      (:NAME "&AUX" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "TOTAL" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL) 0)
      ((:NAME "REMAINDER" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP NIL)
       ""))
     :DOCUMENTATION "Tokenizes a pure text file a sentence at a time"))
   :EXTERNAL-SYMBOLS
   ((:NAME "TOKENIZE-STREAM" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP T)
    (:NAME "TOKENIZE-STRING" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP T)
    (:NAME "TOKENIZE-FILE" :PACKAGE-NAME "LANGUTILS-TOKENIZE" :EXTERNALP T)))
