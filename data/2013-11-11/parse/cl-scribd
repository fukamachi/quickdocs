 ((:TYPE :PACKAGE :NAME "CL-SCRIBD" :FULL-NAME "CL-SCRIBD" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*API-ROOT*" :PACKAGE-NAME "CL-SCRIBD" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "\"http://api.scribd.com/api\"")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*API-KEY*" :PACKAGE-NAME "CL-SCRIBD" :EXTERNALP T) :DOCUMENTATION
     NIL :INITIAL-VALUE "\"\"")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*API-SECRET*" :PACKAGE-NAME "CL-SCRIBD" :EXTERNALP T)
     :DOCUMENTATION NIL :INITIAL-VALUE "\"\"")
    (:TYPE :MACRO :SYMBOL
     (:NAME "BUILD-API-CALL" :PACKAGE-NAME "CL-SCRIBD" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "FNAME" :PACKAGE-NAME "CL-SCRIBD" :EXTERNALP NIL)
      ((:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "PARAMS" :PACKAGE-NAME "CL-SCRIBD" :EXTERNALP NIL)))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "BUILD-API-SIG" :PACKAGE-NAME "CL-SCRIBD" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "PARAMS" :PACKAGE-NAME "CL-SCRIBD" :EXTERNALP NIL))
