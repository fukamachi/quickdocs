    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SORT-FORMATS" :PACKAGE-NAME "CL-PHOTO" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "FORMATS" :PACKAGE-NAME "CL-PHOTO" :EXTERNALP NIL))
