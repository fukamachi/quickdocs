    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*DIGITAL-CAMERAS*" :PACKAGE-NAME "CL-PHOTO" :EXTERNALP T)
     :DOCUMENTATION NIL :INITIAL-VALUE "(CL-PHOTO::SORT-FORMATS
 (LOOP KMRCL:FOR FORMAT KMRCL:IN CL-PHOTO::+FORMAT-DB+
       WHEN (GETF FORMAT :PIXELS)
       CL-PHOTO::COLLECT FORMAT))")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*CAMERAS*" :PACKAGE-NAME "CL-PHOTO" :EXTERNALP T) :DOCUMENTATION
     NIL :INITIAL-VALUE "(CL-PHOTO::SORT-FORMATS
 (LOOP KMRCL:FOR FORMAT KMRCL:IN CL-PHOTO::+FORMAT-DB+
       WHEN (GETF FORMAT :IMAGER)
       CL-PHOTO::COLLECT FORMAT))")
