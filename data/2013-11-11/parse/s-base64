
(:TYPE :SYSTEM :NAME "s-base64" :AUTHOR "Sven Van Caekenberghe <svc@mac.com>"
 :MAINTAINER "Sven Van Caekenberghe <svc@mac.com>" :VERSION "2" :LICENCE
 "Lesser Lisp General Public License (LLGPL)" :DESCRIPTION
 "Common Lisp Base64 Package" :LONG-DESCRIPTION
 "S-BASE64 is a Common Lisp implementation of Base64 Encoding/Decoding"
 :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "S-BASE64" :FULL-NAME "S-BASE64" :DOCUMENTATION
   "An implementation of standard Base64 encoding and decoding" :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "+BASE64-ALPHABET+" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "\"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/\"")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+INVERSE-BASE64-ALPHABET+" :PACKAGE-NAME "S-BASE64" :EXTERNALP
      NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE
     "(LET ((S-BASE64::INVERSE-BASE64-ALPHABET (MAKE-ARRAY 127)))
  (DOTIMES (S-BASE64::I 127 S-BASE64::INVERSE-BASE64-ALPHABET)
    (SETF (AREF S-BASE64::INVERSE-BASE64-ALPHABET S-BASE64::I)
            (POSITION (CODE-CHAR S-BASE64::I) S-BASE64::+BASE64-ALPHABET+))))")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CORE-ENCODE-BASE64" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "BYTE1" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
      (:NAME "BYTE2" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
      (:NAME "BYTE3" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CORE-DECODE-BASE64" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "CHAR1" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
      (:NAME "CHAR2" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
      (:NAME "CHAR3" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
      (:NAME "CHAR4" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SKIP-BASE64-WHITESPACE" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "DECODE-BASE64-BYTES" :PACKAGE-NAME "S-BASE64" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION
     "Decode a base64 encoded character stream, returns a byte array")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ENCODE-BASE64-BYTES" :PACKAGE-NAME "S-BASE64" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "ARRAY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "BREAK-LINES" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
       (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION
     "Encode a byte array into a base64 encoded character stream")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "DECODE-BASE64" :PACKAGE-NAME "S-BASE64" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "IN" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
      (:NAME "OUT" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL))
     :DOCUMENTATION
     "Decode a base64 encoded character input stream into a binary output stream")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ENCODE-BASE64" :PACKAGE-NAME "S-BASE64" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "IN" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
      (:NAME "OUT" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "BREAK-LINES" :PACKAGE-NAME "S-BASE64" :EXTERNALP NIL)
       (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION
     "Encode a binary input stream into a base64 encoded character output stream"))
   :EXTERNAL-SYMBOLS
   ((:NAME "DECODE-BASE64-BYTES" :PACKAGE-NAME "S-BASE64" :EXTERNALP T)
    (:NAME "ENCODE-BASE64" :PACKAGE-NAME "S-BASE64" :EXTERNALP T)
    (:NAME "DECODE-BASE64" :PACKAGE-NAME "S-BASE64" :EXTERNALP T)
    (:NAME "ENCODE-BASE64-BYTES" :PACKAGE-NAME "S-BASE64" :EXTERNALP T)))))
