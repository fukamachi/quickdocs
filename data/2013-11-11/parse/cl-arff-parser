     (:NAME "REMOVE-ATTRIBUTE-BY-NAME" :PACKAGE-NAME "CL-ARFF-PARSER"
      :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "ARFF" :PACKAGE-NAME "CL-ARFF-PARSER" :EXTERNALP T)
      (:NAME "NAME" :PACKAGE-NAME "CL-ARFF-PARSER" :EXTERNALP NIL))
     :DOCUMENTATION "Removes the feature with the given name from the
  arff object (not from the actual file). It will remove it both from
  that @attributes and the @data.")
    (:TYPE :METHOD :SYMBOL
     (:NAME "REMOVE-ATTRIBUTE-BY-NAME" :PACKAGE-NAME "CL-ARFF-PARSER"
      :EXTERNALP T)
