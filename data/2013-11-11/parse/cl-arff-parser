
(:TYPE :SYSTEM :NAME "cl-arff-parser" :AUTHOR "Pieter Wellens" :MAINTAINER NIL
 :VERSION "0.8" :LICENCE "see LICENSE.txt" :DESCRIPTION
 "A parser for Weka arff (Attribute-Relation File
  Format) Machine learning datasets."
 :LONG-DESCRIPTION NIL :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CL-ARFF-PARSER" :FULL-NAME "CL-ARFF-PARSER"
   :DOCUMENTATION "A parser and some manipulation for Weka
  Machine (ARFF) Learning datasets."
   :SYMBOL-LIST
   ((:TYPE :CLASS :SYMBOL
     (:NAME "ARFF" :PACKAGE-NAME "CL-ARFF-PARSER" :EXTERNALP T)
     :SUPER-CLASS-LIST NIL :DOCUMENTATION
     "An arff object contains all the data found in a
  parsed arff file."
     :SLOT-LIST
     ((:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "ARFF-PATH" :PACKAGE-NAME "CL-ARFF-PARSER" :EXTERNALP T)
       :ACCESSORS
       ((:NAME "ARFF-PATH" :PACKAGE-NAME "CL-ARFF-PARSER" :EXTERNALP T))
       :READERS NIL :WRITERS NIL :DOCUMENTATION
       "A string to the path of the arff
              file. e.g. /home/user/myData/foo.arff")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "ARFF-RELATION" :PACKAGE-NAME "CL-ARFF-PARSER" :EXTERNALP T)
       :ACCESSORS
       ((:NAME "ARFF-RELATION" :PACKAGE-NAME "CL-ARFF-PARSER" :EXTERNALP T))
       :READERS NIL :WRITERS NIL :DOCUMENTATION
       "The string after @relation. This is
             essentially the name of the arff.")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "ARFF-ATTRIBUTES" :PACKAGE-NAME "CL-ARFF-PARSER" :EXTERNALP T)
       :ACCESSORS
       ((:NAME "ARFF-ATTRIBUTES" :PACKAGE-NAME "CL-ARFF-PARSER" :EXTERNALP T))
       :READERS NIL :WRITERS NIL :DOCUMENTATION
       "The attributes as specified in the
               header. Each attribute is a list that looks as
               follows: (\"attribute-name\" (\"type\")). In case of a
               nominal attribute it looks like
               this: (\"attribute-name\" (\"nominal\" . values)). ")
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "ARFF-DATA" :PACKAGE-NAME "CL-ARFF-PARSER" :EXTERNALP T)
       :ACCESSORS
       ((:NAME "ARFF-DATA" :PACKAGE-NAME "CL-ARFF-PARSER" :EXTERNALP T))
       :READERS NIL :WRITERS NIL :DOCUMENTATION
       "All the data. The bulk of the file.")))
