     (:NAME "*FONT-CACHE*" :PACKAGE-NAME "CLX-TRUETYPE" :EXTERNALP NIL)
     :DOCUMENTATION "Hashmap for caching font families, subfamilies and files."
     :INITIAL-VALUE
     "(IF (CL-FAD:FILE-EXISTS-P CLX-TRUETYPE::+FONT-CACHE-FILENAME+)
    (CL-STORE:RESTORE CLX-TRUETYPE::+FONT-CACHE-FILENAME+)
    (MAKE-HASH-TABLE :TEST 'EQUAL))")
