
(:TYPE :SYSTEM :NAME "clx-truetype" :AUTHOR
 "Michael Filonenko <filonenko.mikhail@gmail.com>" :MAINTAINER NIL :VERSION
 "0.1" :LICENCE "MIT" :DESCRIPTION
 "clx-truetype is pure common lisp solution for antialiased TrueType font rendering using CLX and XRender extension."
 :LONG-DESCRIPTION NIL :DEPENDS-ON
 ("CLX" "ZPB-TTF" "CL-VECTORS" "CL-PATHS-TTF" "CL-AA" "CL-FAD" "CL-STORE"
  "TRIVIAL-FEATURES")
 :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "CLX-TRUETYPE" :FULL-NAME "CLX-TRUETYPE" :DOCUMENTATION
   "Package contains API for TrueType text rendering using CLX, XRender. Glyphs information is obtained by ZPB-TTF. Font rasterization is made by CL-VECTORS."
   :SYMBOL-LIST
