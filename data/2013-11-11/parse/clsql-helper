     :DOCUMENTATION NIL :INITIAL-VALUE "\"(?:/|-|\\\\.|:)\"")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+DATE-TIME-REGEX+" :PACKAGE-NAME "CLSQL-HELPER" :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(CL-PPCRE:CREATE-SCANNER
 (WITH-OUTPUT-TO-STRING (#:G277)
   (WRITE-STRING \"^(?:'|\\\")?(\\\\d{1,2})\" #:G277)
   (PRINC (PROGN CLSQL-HELPER::+DATE-SEP+) #:G277)
   (WRITE-STRING \"(\\\\d{1,2})\" #:G277)
   (PRINC (PROGN CLSQL-HELPER::+DATE-SEP+) #:G277)
   (WRITE-STRING \"(\\\\d{2,4})(?:\\\\s*(\\\\d{1,2})\" #:G277)
   (PRINC (PROGN CLSQL-HELPER::+DATE-SEP+) #:G277)
   (WRITE-STRING \"(\\\\d{1,2})(?:\" #:G277)
   (PRINC (PROGN CLSQL-HELPER::+DATE-SEP+) #:G277)
   (WRITE-STRING \"(\\\\d{1,2}))?\\\\s*((?:a|p)m\\\\.?)?)?(?:'|\\\")?\" #:G277))
 :CASE-INSENSITIVE-MODE T)")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "+ISO-8601-ISH-REGEX-STRING+" :PACKAGE-NAME "CLSQL-HELPER"
      :EXTERNALP NIL)
     :DOCUMENTATION NIL :INITIAL-VALUE "(WITH-OUTPUT-TO-STRING (#:G287)
  (WRITE-STRING \"^(?:'|\\\")?(\\\\d{2,4})\" #:G287)
  (PRINC (PROGN CLSQL-HELPER::+DATE-SEP+) #:G287)
  (WRITE-STRING \"(\\\\d{1,2})\" #:G287)
  (PRINC (PROGN CLSQL-HELPER::+DATE-SEP+) #:G287)
  (WRITE-STRING \"(\\\\d{1,2})(?:(?:\\\\s*|T)(\\\\d{1,2})\" #:G287)
  (PRINC (PROGN CLSQL-HELPER::+DATE-SEP+) #:G287)
  (WRITE-STRING \"(\\\\d{1,2})(?:\" #:G287)
  (PRINC (PROGN CLSQL-HELPER::+DATE-SEP+) #:G287)
  (WRITE-STRING
   \"(\\\\d{1,2}))?\\\\s*((?:a|p)m\\\\.?)?(?:Z|,,0|(?:-|\\\\+)\\\\d{1,2}:?\\\\d{2}?)?)?(?:'|\\\")?\"
   #:G287))")
