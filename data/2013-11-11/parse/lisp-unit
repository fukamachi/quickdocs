
(:TYPE :SYSTEM :NAME "lisp-unit" :AUTHOR
 "Thomas M. Hermann <thomas.m.hermann@odonata-research.com>" :MAINTAINER NIL
 :VERSION "0.9.2" :LICENCE "MIT" :DESCRIPTION
 "Common Lisp library that supports unit testing." :LONG-DESCRIPTION NIL
 :DEPENDS-ON NIL :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "LISP-UNIT" :FULL-NAME "LISP-UNIT" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*PASS*" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL) :DOCUMENTATION
     "The passed assertion results." :INITIAL-VALUE "0")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*FAIL*" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL) :DOCUMENTATION
     "The failed assertion results." :INITIAL-VALUE "NIL")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "RESET-COUNTERS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
     :DOCUMENTATION "Reset the counters to empty lists.")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*PRINT-SUMMARY*" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T)
     :DOCUMENTATION
     "Print a summary of the pass, fail, and error count if non-nil."
     :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*PRINT-FAILURES*" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T)
     :DOCUMENTATION "Print failure messages if non-NIL." :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*PRINT-ERRORS*" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T)
     :DOCUMENTATION "Print error messages if non-NIL." :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*USE-DEBUGGER*" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :DOCUMENTATION
     "If not NIL, enter the debugger when an error is encountered in an
assertion."
     :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*SIGNAL-RESULTS*" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :DOCUMENTATION "Signal the result if non NIL." :INITIAL-VALUE "NIL")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "USE-DEBUGGER-P" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "CONDITION" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Debug or ignore errors.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "USE-DEBUGGER" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FLAG" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
       (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Use the debugger when testing, or not.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "SIGNAL-RESULTS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "FLAG" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
       (:NAME "T" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Signal the results for extensibility.")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*TEST-DB*" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :DOCUMENTATION "The unit test database is simply a hash table."
     :INITIAL-VALUE "(MAKE-HASH-TABLE :TEST #'EQ)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "NULL-TESTS-WARNING-REPORT" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     ((:NAME "NULL-TESTS-WARNING" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Write the null-tests-warning to the stream.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PACKAGE-TABLE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CREATE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :MACRO :SYMBOL
     (:NAME "WITH-PACKAGE-TABLE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST
     (((:NAME "TABLE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
       (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
       (:NAME "CREATE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Execute the body only if the package table exists.")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*TAG-DB*" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL) :DOCUMENTATION
     "The tag database is simply a hash table." :INITIAL-VALUE
     "(MAKE-HASH-TABLE :TEST #'EQ)")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "NULL-TAGS-WARNING-REPORT" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     ((:NAME "NULL-TAGS-WARNING" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Write the null-tags-warning to the stream.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PACKAGE-TAGS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "CREATE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Return the tags DB for the package.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "WITH-PACKAGE-TAGS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST
     (((:NAME "TABLE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
       (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
       (:NAME "CREATE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Execute the body only if the package tags exists.")
    (:TYPE :CLASS :SYMBOL
     (:NAME "UNIT-TEST" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :SUPER-CLASS-LIST NIL :DOCUMENTATION
     "Organize the unit test documentation and code." :SLOT-LIST
     ((:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "DOC" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL) :ACCESSORS NIL
       :READERS ((:NAME "DOC" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
       :WRITERS NIL :DOCUMENTATION NIL)
      (:TYPE :CLASS-SLOT :SYMBOL
       (:NAME "CODE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL) :ACCESSORS NIL
       :READERS ((:NAME "CODE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
       :WRITERS NIL :DOCUMENTATION NIL)))
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "PARSE-BODY" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "BODY" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "DOC" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "TAG" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Separate the components of the body.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TEST-NAME-ERROR-REPORT" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "TEST-NAME-ERROR" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Write the test-name-error to the stream.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "VALID-TEST-NAME" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "NAME" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Signal a type-error if the test name is not a symbol.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "DEFINE-TEST" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Store the test in the test database.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "LIST-TESTS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Return a list of the tests in package.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TEST-DOCUMENTATION" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Return the documentation for the test.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TEST-CODE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "NAME" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Returns the code stored for the test name.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "REMOVE-TESTS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "NAMES" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
       (:NAME "ALL" :PACKAGE-NAME "KEYWORD" :EXTERNALP T))
      ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Remove individual tests or entire sets.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "%TESTS-FROM-ALL-TAGS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Return all of the tests that have been tagged.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "%TESTS-FROM-TAGS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "TAGS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Return the tests associated with the tags.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "LIST-TAGS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Return a list of the tags in package.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "TAGGED-TESTS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "TAGS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
       (:NAME "ALL" :PACKAGE-NAME "KEYWORD" :EXTERNALP T))
      ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Return a list of the tests associated with the tags.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "REMOVE-TAGS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "&OPTIONAL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "TAGS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
       (:NAME "ALL" :PACKAGE-NAME "KEYWORD" :EXTERNALP T))
      ((:NAME "PACKAGE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "*PACKAGE*" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
     :DOCUMENTATION "Remove individual tags or entire sets.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ASSERT-EQ" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "EXPECTED" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Assert whether expected and form are EQ.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ASSERT-EQL" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "EXPECTED" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Assert whether expected and form are EQL.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ASSERT-EQUAL" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "EXPECTED" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Assert whether expected and form are EQUAL.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ASSERT-EQUALP" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "EXPECTED" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Assert whether expected and form are EQUALP.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ASSERT-ERROR" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "CONDITION" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Assert whether form signals condition.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ASSERT-EXPANDS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "EXPANSION" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Assert whether form expands to expansion.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ASSERT-FALSE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Assert whether the form is false.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ASSERT-EQUALITY" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "TEST" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "EXPECTED" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION
     "Assert whether expected and form are equal according to test.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ASSERT-PRINTS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "OUTPUT" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Assert whether printing the form generates the output.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "ASSERT-TRUE" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP T) :LAMBDA-LIST
     ((:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Assert whether the form is true.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "EXPAND-ASSERT" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "TYPE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "BODY" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "EXPECTED" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "TEST" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
       ((:NAME "QUOTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        ((:NAME "FUNCTION" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
         (:NAME "EQL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))))
     :DOCUMENTATION "Expand the assertion to the internal format.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "EXPAND-ERROR-FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Wrap the error assertion in HANDLER-CASE.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "EXPAND-OUTPUT-FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Capture the output of the form in a string.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "EXPAND-MACRO-FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "FORM" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
      (:NAME "ENV" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Expand the macro form once.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "EXPAND-EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "EXTRAS" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL))
     :DOCUMENTATION "Expand extra forms.")
    (:TYPE :GENERIC :SYMBOL
     (:NAME "ASSERT-RESULT" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
     :LAMBDA-LIST
     ((:NAME "TYPE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "TEST" :PACKAGE-NAME "LISP-UNIT" :EXTERNALP NIL)
