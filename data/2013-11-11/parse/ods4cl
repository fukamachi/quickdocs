
(:TYPE :SYSTEM :NAME "ods4cl" :AUTHOR
 "Walter C. Pelissero <walter@pelissero.de>" :MAINTAINER
 "Walter C. Pelissero <walter@pelissero.de>" :VERSION NIL :LICENCE "LGPL"
 :DESCRIPTION "Open Document Spreadsheet library" :LONG-DESCRIPTION
 "This is package provides some primitives to create
spreadsheets in Open Document format, the one digestible by
OpenOffice and other modern spreadsheet programmes."
 :DEPENDS-ON ("SCLF") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "ODS4CL" :FULL-NAME "ODS4CL" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :FUNCTION :SYMBOL
     (:NAME "WRITE-PREAMBLE" :PACKAGE-NAME "ODS4CL" :EXTERNALP NIL)
     :LAMBDA-LIST ((:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "ESCAPE-STRING" :PACKAGE-NAME "ODS4CL" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "STRING" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)) :DOCUMENTATION
     "Escape STRING making it safe to be inserted into XML code.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WRITE-CELL" :PACKAGE-NAME "ODS4CL" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "CELL" :PACKAGE-NAME "ODS4CL" :EXTERNALP NIL)
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "WRITE-ROW" :PACKAGE-NAME "ODS4CL" :EXTERNALP NIL) :LAMBDA-LIST
