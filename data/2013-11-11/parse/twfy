
(:TYPE :SYSTEM :NAME "twfy" :AUTHOR "James Thompson <james@jamtho.com>"
 :MAINTAINER NIL :VERSION NIL :LICENCE "BSD-style" :DESCRIPTION
 "TheyWorkForYou API bindings" :LONG-DESCRIPTION NIL :DEPENDS-ON
 ("DRAKMA" "CL-JSON") :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "TWFY" :FULL-NAME "TWFY" :DOCUMENTATION NIL
   :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*API-KEY*" :PACKAGE-NAME "TWFY" :EXTERNALP T) :DOCUMENTATION
     "API key (register for one at http://www.theyworkforyou.com/api/key)"
     :INITIAL-VALUE "NIL")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*DECODE-RESPONSE*" :PACKAGE-NAME "TWFY" :EXTERNALP T)
     :DOCUMENTATION
     "If set, and *output-format* is \"js\", API responses will be decoded from
json to lisp objects with cl-json."
     :INITIAL-VALUE "T")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*OUTPUT-FORMAT*" :PACKAGE-NAME "TWFY" :EXTERNALP T) :DOCUMENTATION
     "Passed in the querystring to control the API response format; values can be
\"js\", \"xml\", \"php\", or \"rabx\"."
     :INITIAL-VALUE "\"js\"")
    (:TYPE :VARIABLE :SYMBOL
     (:NAME "*BASE-URI*" :PACKAGE-NAME "TWFY" :EXTERNALP NIL) :DOCUMENTATION
     "Root for all twfy api calls" :INITIAL-VALUE
     "\"http://www.theyworkforyou.com/api/\"")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "DECODE-JSON" :PACKAGE-NAME "TWFY" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "STR" :PACKAGE-NAME "TWFY" :EXTERNALP NIL)) :DOCUMENTATION
     "Helper for CALL-API. Decodes json string STR into symbols, using the
SIMPLIFIED-CAMEL-CASE translator.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "CLEAN-ALIST" :PACKAGE-NAME "TWFY" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "ALIST" :PACKAGE-NAME "TWFY" :EXTERNALP NIL)) :DOCUMENTATION
     "Helper for CALL-API. Returns a copy of ALIST with all pairs having null
cdr removed, and all remaining cdr's changed to strings.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "API-COMMAND-URI" :PACKAGE-NAME "TWFY" :EXTERNALP NIL) :LAMBDA-LIST
     ((:NAME "FUN" :PACKAGE-NAME "TWFY" :EXTERNALP NIL)) :DOCUMENTATION
     "Returns the URI to use for (string) command FUN.")
    (:TYPE :FUNCTION :SYMBOL
