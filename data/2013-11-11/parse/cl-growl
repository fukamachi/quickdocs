      ((:NAME "APP-ENC" :PACKAGE-NAME "CL-GROWL" :EXTERNALP NIL)
       ((:NAME "STRING-TO-UTF-8-BYTES" :PACKAGE-NAME "TRIVIAL-UTF-8" :EXTERNALP
