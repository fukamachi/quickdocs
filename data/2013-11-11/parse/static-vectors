     (:NAME "%ALLOCATE-STATIC-VECTOR" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP
      NIL)
     :LAMBDA-LIST
     ((:NAME "LENGTH" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "ELEMENT-TYPE" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP NIL))
     :DOCUMENTATION NIL)
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "STATIC-VECTOR-ADDRESS" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP
      NIL)
     :LAMBDA-LIST ((:NAME "VECTOR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION "Return a foreign pointer to VECTOR(including its header).
VECTOR must be a vector created by MAKE-STATIC-VECTOR.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "STATIC-VECTOR-POINTER" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP
      T)
     :LAMBDA-LIST
     ((:NAME "VECTOR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "OFFSET" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP NIL) 0))
     :DOCUMENTATION
     "Return a foreign pointer to the beginning of VECTOR + OFFSET octets.
VECTOR must be a vector created by MAKE-STATIC-VECTOR.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "FREE-STATIC-VECTOR" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP T)
     :LAMBDA-LIST ((:NAME "VECTOR" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION
     "Free VECTOR, which must be a vector created by MAKE-STATIC-VECTOR.")
    (:TYPE :MACRO :SYMBOL
     (:NAME "WITH-STATIC-VECTOR" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "VAR" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP NIL)
       (:NAME "LENGTH" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "&REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "ARGS" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP NIL)
       (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       ((:NAME "ELEMENT-TYPE" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP NIL)
        ((:NAME "QUOTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
         ((:NAME "QUOTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
          ((:NAME "UNSIGNED-BYTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
           8))))
       ((:NAME "INITIAL-ELEMENT" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP NIL)
        (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)))
      (:NAME "&BODY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "BODY" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP NIL))
     :DOCUMENTATION
     "Bind PTR-VAR to a static vector of length LENGTH and execute BODY
within its dynamic extent. The vector is freed upon exit.")
    (:TYPE :FUNCTION :SYMBOL
     (:NAME "MAKE-STATIC-VECTOR" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP T)
     :LAMBDA-LIST
     ((:NAME "LENGTH" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "&KEY" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      ((:NAME "ELEMENT-TYPE" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP NIL)
       ((:NAME "QUOTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        ((:NAME "UNSIGNED-BYTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T) 8)))
      ((:NAME "INITIAL-ELEMENT" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP NIL)
       (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "INITIAL-ELEMENT-P" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP
        NIL))
      ((:NAME "INITIAL-CONTENTS" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP NIL)
       (:NAME "NIL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
       (:NAME "INITIAL-CONTENTS-P" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP
        NIL)))
     :DOCUMENTATION
     "Create a simple vector of length LENGTH and type ELEMENT-TYPE which will
not be moved by the garbage collector. The vector might be allocated in
foreign memory so you must always call FREE-STATIC-VECTOR to free it."))
   :EXTERNAL-SYMBOLS
   ((:NAME "STATIC-VECTOR-POINTER" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP T)
    (:NAME "MAKE-STATIC-VECTOR" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP T)
    (:NAME "REPLACE-FOREIGN-MEMORY" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP
     T)
    (:NAME "WITH-STATIC-VECTOR" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP T)
    (:NAME "FREE-STATIC-VECTOR" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP T)
    (:NAME "FILL-FOREIGN-MEMORY" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP T)
    (:NAME "STATIC-VECTOR" :PACKAGE-NAME "STATIC-VECTORS" :EXTERNALP T)))))
