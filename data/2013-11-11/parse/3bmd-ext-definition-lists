
(:TYPE :SYSTEM :NAME "3bmd-ext-definition-lists" :AUTHOR NIL :MAINTAINER NIL
 :VERSION NIL :LICENCE NIL :DESCRIPTION
 "extension to 3bmd implementing 'PHP Markdown Extra' style definition lists"
 :LONG-DESCRIPTION NIL :DEPENDS-ON ("3BMD" "COLORIZE" "ALEXANDRIA")
 :PACKAGE-LIST
 ((:TYPE :PACKAGE :NAME "3BMD-DEFINITION-LISTS" :FULL-NAME
   "3BMD-DEFINITION-LISTS" :DOCUMENTATION NIL :SYMBOL-LIST
   ((:TYPE :VARIABLE :SYMBOL
     (:NAME "*DEFINITION-LISTS*" :PACKAGE-NAME "3BMD-DEFINITION-LISTS"
      :EXTERNALP T)
     :DOCUMENTATION NIL :INITIAL-VALUE "NIL")
    (:TYPE :METHOD :SYMBOL
     (:NAME "PRINT-TAGGED-ELEMENT" :PACKAGE-NAME "3BMD-EXT" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "TAG" :PACKAGE-NAME "3BMD-DEFINITION-LISTS" :EXTERNALP NIL)
       ((:NAME "EQL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        ((:NAME "QUOTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
         (:NAME "DEFINITION-LIST" :PACKAGE-NAME "3BMD-DEFINITION-LISTS"
          :EXTERNALP NIL))))
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "PRINT-TAGGED-ELEMENT" :PACKAGE-NAME "3BMD-EXT" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "TAG" :PACKAGE-NAME "3BMD-DEFINITION-LISTS" :EXTERNALP NIL)
       ((:NAME "EQL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        ((:NAME "QUOTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
         (:NAME "DEFINITION-TERM" :PACKAGE-NAME "3BMD-DEFINITION-LISTS"
          :EXTERNALP NIL))))
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL)
    (:TYPE :METHOD :SYMBOL
     (:NAME "PRINT-TAGGED-ELEMENT" :PACKAGE-NAME "3BMD-EXT" :EXTERNALP T)
     :LAMBDA-LIST
     (((:NAME "TAG" :PACKAGE-NAME "3BMD-DEFINITION-LISTS" :EXTERNALP NIL)
       ((:NAME "EQL" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
        ((:NAME "QUOTE" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
         (:NAME "DEFINITION-LIST-ITEM" :PACKAGE-NAME "3BMD-DEFINITION-LISTS"
          :EXTERNALP NIL))))
      (:NAME "STREAM" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T)
      (:NAME "REST" :PACKAGE-NAME "COMMON-LISP" :EXTERNALP T))
     :DOCUMENTATION NIL))
   :EXTERNAL-SYMBOLS
   ((:NAME "*DEFINITION-LISTS*" :PACKAGE-NAME "3BMD-DEFINITION-LISTS"
     :EXTERNALP T)))))
